{
  if (null != node) {
    int len=m_nodes.size();
    boolean isMore;
    int i=0;
    do {
      for (; i < len; i++) {
        if (m_nodes.elementAt(i) == node)         return makeNodeHandle(i);
      }
      isMore=nextNode();
      len=m_nodes.size();
    }
 while (isMore || i < len);
  }
  return DTM.NULL;
}
