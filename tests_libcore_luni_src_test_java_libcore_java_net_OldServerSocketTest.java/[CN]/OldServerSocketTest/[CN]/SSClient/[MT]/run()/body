{
  try {
    Thread.sleep(1000);
    cs=new Socket(InetAddress.getLocalHost().getHostName(),port);
    Thread.sleep(10000);
  }
 catch (  InterruptedException e) {
    return;
  }
catch (  Throwable e) {
    System.out.println("Error establishing client: " + e.toString());
  }
 finally {
    try {
      if (cs != null)       cs.close();
    }
 catch (    Exception e) {
    }
  }
}
