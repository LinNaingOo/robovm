{
  if (name == null) {
    throw new NullPointerException(XSLMessages.createMessage(XSLTErrorResources.ER_GET_FEATURE_NULL_NAME,null));
  }
  if ((DOMResult.FEATURE == name) || (DOMSource.FEATURE == name) || (SAXResult.FEATURE == name)|| (SAXSource.FEATURE == name)|| (StreamResult.FEATURE == name)|| (StreamSource.FEATURE == name)|| (SAXTransformerFactory.FEATURE == name)|| (SAXTransformerFactory.FEATURE_XMLFILTER == name))   return true;
 else   if ((DOMResult.FEATURE.equals(name)) || (DOMSource.FEATURE.equals(name)) || (SAXResult.FEATURE.equals(name))|| (SAXSource.FEATURE.equals(name))|| (StreamResult.FEATURE.equals(name))|| (StreamSource.FEATURE.equals(name))|| (SAXTransformerFactory.FEATURE.equals(name))|| (SAXTransformerFactory.FEATURE_XMLFILTER.equals(name)))   return true;
 else   if (name.equals(XMLConstants.FEATURE_SECURE_PROCESSING))   return m_isSecureProcessing;
 else   return false;
}
