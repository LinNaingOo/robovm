{
  if (rep != this) {
    return ecr().union(var);
  }
  TypeVariable y=var.ecr();
  if (this == y) {
    return this;
  }
  if (rank > y.rank) {
    y.rep=this;
    merge(y);
    y.clear();
    return this;
  }
  rep=y;
  if (rank == y.rank) {
    y.rank++;
  }
  y.merge(this);
  clear();
  return y;
}
