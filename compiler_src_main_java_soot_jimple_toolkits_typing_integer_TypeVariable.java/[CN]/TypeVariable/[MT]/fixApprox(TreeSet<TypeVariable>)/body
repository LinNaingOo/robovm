{
  if (rep != this) {
    ecr().fixApprox(workList);
    return;
  }
  for (  TypeVariable typeVariable : parents) {
    TypeVariable parent=typeVariable.ecr();
    if (parent.approx == null) {
      parent.approx=approx;
      workList.add(parent);
    }
 else {
      TypeNode type=parent.approx.lca_2(approx);
      if (type != parent.approx) {
        parent.approx=type;
        workList.add(parent);
      }
    }
  }
  if (type != null) {
    approx=type;
  }
}
