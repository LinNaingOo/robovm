{
  float f;
  double d;
  int i;
  long l;
  f=1234.5678f;
  i=(int)f;
  System.out.println("f=" + f + " --> i="+ i);
  f=-1234.5678f;
  i=(int)f;
  System.out.println("f=" + f + " --> i="+ i);
  d=1234.5678;
  i=(int)d;
  System.out.println("d=" + d + " --> i="+ i);
  d=-1234.5678;
  i=(int)d;
  System.out.println("d=" + d + " --> i="+ i);
  d=5678956789.0123;
  l=(long)d;
  System.out.println("d=" + d + " --> l="+ l);
  d=-5678956789.0123;
  l=(long)d;
  System.out.println("d=" + d + " --> l="+ l);
  i=7654;
  l=(long)i;
  System.out.println("i=" + i + " --> l="+ l);
  i=-7654;
  l=(long)i;
  System.out.println("i=" + i + " --> l="+ l);
  l=5678956789L;
  i=(int)l;
  System.out.println("l=" + l + " --> i="+ i);
  l=-5678956789L;
  i=(int)l;
  System.out.println("l=" + l + " --> i="+ i);
  i=1234;
  f=(float)i;
  System.out.println("i=" + i + " --> f="+ f);
  i=-1234;
  f=(float)i;
  System.out.println("i=" + i + " --> f="+ f);
}
