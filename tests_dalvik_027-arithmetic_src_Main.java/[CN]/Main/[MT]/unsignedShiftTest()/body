{
  byte b=-4;
  short s=-4;
  char c=0xfffc;
  int i=-4;
  b>>>=4;
  s>>>=4;
  c>>>=4;
  i>>>=4;
  System.out.println("b=" + b + ", s="+ s+ ", c="+ (int)c+ ", i="+ i);
  System.out.println("b=0x" + Integer.toHexString((int)b) + ", s=0x"+ Integer.toHexString((int)s)+ ", c=0x"+ Integer.toHexString((int)c)+ ", i=0x"+ Integer.toHexString(i));
}
