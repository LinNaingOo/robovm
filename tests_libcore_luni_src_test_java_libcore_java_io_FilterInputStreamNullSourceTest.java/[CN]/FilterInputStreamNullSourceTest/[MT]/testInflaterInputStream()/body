{
  try {
    new InflaterInputStream(null);
    fail();
  }
 catch (  NullPointerException expected) {
  }
  try {
    new InflaterInputStream(null,new Inflater());
    fail();
  }
 catch (  NullPointerException expected) {
  }
  try {
    new InflaterInputStream(null,new Inflater(),1024);
    fail();
  }
 catch (  NullPointerException expected) {
  }
}
