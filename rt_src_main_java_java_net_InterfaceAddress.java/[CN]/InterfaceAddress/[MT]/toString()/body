{
  StringBuilder builder=new StringBuilder();
  if (addr != null) {
    builder.append(addr.toString());
  }
  builder.append("/");
  builder.append(prefixLength);
  builder.append(" [");
  InetAddress broadcast=cacheBroadcast.ipaddress == null ? getBroadcast() : cacheBroadcast;
  if (broadcast != null) {
    builder.append(broadcast.toString());
  }
 else {
    builder.append("null");
  }
  builder.append("]");
  return builder.toString();
}
