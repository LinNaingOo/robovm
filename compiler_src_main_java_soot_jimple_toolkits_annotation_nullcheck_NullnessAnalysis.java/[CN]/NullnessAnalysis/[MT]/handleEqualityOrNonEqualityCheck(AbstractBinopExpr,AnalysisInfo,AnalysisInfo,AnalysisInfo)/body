{
  Value left=eqExpr.getOp1();
  Value right=eqExpr.getOp2();
  Value val=null;
  if (left == NullConstant.v()) {
    if (right != NullConstant.v()) {
      val=right;
    }
  }
 else   if (right == NullConstant.v()) {
    if (left != NullConstant.v()) {
      val=left;
    }
  }
  if (val != null && val instanceof Local) {
    if (eqExpr instanceof JEqExpr)     handleEquality(val,out,outBranch);
 else     if (eqExpr instanceof JNeExpr)     handleNonEquality(val,out,outBranch);
 else     throw new IllegalStateException("unexpected condition: " + eqExpr.getClass());
  }
}
