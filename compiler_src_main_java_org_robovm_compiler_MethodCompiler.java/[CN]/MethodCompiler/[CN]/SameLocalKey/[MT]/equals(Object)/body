{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  SameLocalKey other=(SameLocalKey)obj;
  if (index != other.index) {
    return false;
  }
  if (type == null) {
    if (other.type != null) {
      return false;
    }
  }
 else   if (!type.equals(other.type)) {
    return false;
  }
  return true;
}
