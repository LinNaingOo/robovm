{
  assertEquals(false,multi.isDeclared(0));
  assertEquals(true,multi.isDeclared(1));
  try {
    multi.isDeclared(-1);
    fail("ArrayIndexOutOfBoundsException expected");
  }
 catch (  ArrayIndexOutOfBoundsException e) {
  }
  try {
    multi.isDeclared(4);
    fail("ArrayIndexOutOfBoundsException expected");
  }
 catch (  ArrayIndexOutOfBoundsException e) {
  }
}
