{
  assertEquals(false,multi.isSpecified(1));
  assertEquals(true,multi.isSpecified(2));
  try {
    multi.isSpecified(-1);
    fail("ArrayIndexOutOfBoundsException expected");
  }
 catch (  ArrayIndexOutOfBoundsException e) {
  }
  try {
    multi.isSpecified(4);
    fail("ArrayIndexOutOfBoundsException expected");
  }
 catch (  ArrayIndexOutOfBoundsException e) {
  }
}
