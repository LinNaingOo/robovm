{
  int ff=m_firstFree;
  if ((ff + 1) >= m_mapSize) {
    if (null == m_map) {
      m_map=new Node[m_blocksize];
      m_mapSize=m_blocksize;
    }
 else {
      m_mapSize+=m_blocksize;
      Node newMap[]=new Node[m_mapSize];
      System.arraycopy(m_map,0,newMap,0,ff + 1);
      m_map=newMap;
    }
  }
  m_map[ff]=value;
  ff++;
  m_firstFree=ff;
}
