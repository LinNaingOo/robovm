{
  Thread t=new Thread(null,new ThreadTestSub(),"Thready",7168);
  t.setDaemon(false);
  System.out.print("Starting thread '" + t.getName() + "'\n");
  t.start();
  try {
    t.join();
  }
 catch (  InterruptedException ex) {
    ex.printStackTrace();
  }
  System.out.print("Thread starter returning\n");
}
