{
  Collection<ExceptionDest> dests=(map == null ? null : map.get(u));
  if (dests == null) {
    if (t == null) {
      return map;
    }
 else {
      if (map == null) {
        map=new HashMap<Unit,Collection<ExceptionDest>>(unitChain.size() * 2 + 1);
      }
      dests=new ArrayList<ExceptionDest>(3);
      map.put(u,dests);
    }
  }
  dests.add(new ExceptionDest(t,caught));
  return map;
}
