{
  StringBuffer buf=new StringBuffer();
  buf.append(throwables.toString());
  buf.append(" -> ");
  if (trap == null) {
    buf.append("(escapes)");
  }
 else {
    buf.append(trap.toString());
    buf.append("handler: ");
    buf.append(getHandlerNode().toString());
  }
  return buf.toString();
}
