{
  LinkedList<SootClass> q=new LinkedList<SootClass>();
  q.add(rootClazz);
  while (!q.isEmpty()) {
    SootClass clazz=q.removeFirst();
    if (!clazz.isPhantom()) {
      if (clazz.isInterface()) {
        AnnotationTag tag=getMarshalerAnnotation0(clazz,type);
        if (tag != null) {
          return tag;
        }
      }
      q.addAll(clazz.getInterfaces());
    }
  }
  return null;
}
