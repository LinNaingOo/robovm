{
  if (input == null || output == null) {
    throw new NullPointerException();
  }
  int result=input.limit() - input.position();
  try {
    output.put(input);
  }
 catch (  java.nio.BufferOverflowException e) {
    throw new ShortBufferException(Messages.getString("crypto.0F",e));
  }
  return result;
}
