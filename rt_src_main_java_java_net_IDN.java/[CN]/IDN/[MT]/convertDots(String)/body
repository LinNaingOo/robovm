{
  String result=input;
  if (result.indexOf("\u3002") != -1) {
    result=result.replace("\u3002","\u002E");
  }
  if (result.indexOf("\uFF0E") != -1) {
    result=result.replace("\uFF0E","\u002E");
  }
  if (result.indexOf("\uFF61") != -1) {
    result=result.replace("\uFF61","\u002E");
  }
  return result;
}
