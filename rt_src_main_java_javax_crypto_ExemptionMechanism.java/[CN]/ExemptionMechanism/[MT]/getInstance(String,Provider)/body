{
  if (algorithm == null) {
    throw new NullPointerException(Messages.getString("crypto.02"));
  }
  if (provider == null) {
    throw new IllegalArgumentException(Messages.getString("crypto.04"));
  }
synchronized (engine) {
    engine.getInstance(algorithm,provider,null);
    return new ExemptionMechanism((ExemptionMechanismSpi)engine.spi,provider,algorithm);
  }
}
