{
  Class<?> type=getType();
  Class<?> declaringClass=getDeclaringClass();
  String name=getName();
  StringBuilder result=new StringBuilder(Modifier.toString(getModifiers()));
  if (result.length() != 0) {
    result.append(' ');
  }
  appendTypeName(result,type);
  result.append(' ');
  appendTypeName(result,declaringClass);
  result.append('.');
  result.append(name);
  return result.toString();
}
