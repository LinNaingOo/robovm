{
  if (s == null) {
    try {
      ByteArrayInputStream bs=new ByteArrayInputStream(bytes);
      DataInputStream d=new DataInputStream(bs);
      String buf=d.readUTF();
      sHashCode=buf.hashCode();
      return buf;
    }
 catch (    IOException e) {
      return "!!IOException!!";
    }
  }
  return s;
}
