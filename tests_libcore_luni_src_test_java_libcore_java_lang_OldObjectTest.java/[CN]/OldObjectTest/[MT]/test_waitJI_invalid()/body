{
  Object o=new Object();
synchronized (o) {
    try {
      o.wait(-1,0);
      fail();
    }
 catch (    IllegalArgumentException expected) {
    }
    try {
      o.wait(0,-1);
      fail();
    }
 catch (    IllegalArgumentException expected) {
    }
    try {
      o.wait(-1,-1);
      fail();
    }
 catch (    IllegalArgumentException expected) {
    }
    try {
      o.wait(Integer.MAX_VALUE + 1,0);
      fail();
    }
 catch (    IllegalArgumentException expected) {
    }
  }
}
