{
  if (this == object) {
    return true;
  }
  if (!(object instanceof DecimalFormatSymbols)) {
    return false;
  }
  DecimalFormatSymbols obj=(DecimalFormatSymbols)object;
  return Arrays.equals(patternChars,obj.patternChars) && infinity.equals(obj.infinity) && NaN.equals(obj.NaN)&& currencySymbol.equals(obj.currencySymbol)&& intlCurrencySymbol.equals(obj.intlCurrencySymbol)&& exponentialSeparator.equals(obj.exponentialSeparator);
}
