{
  Locale[] icuLocales=com.ibm.icu.text.DecimalFormatSymbols.getAvailableLocales();
  if (buildinLocales == null) {
    initBuildInLocales(icuLocales);
  }
  Locale[] providerLocales=LocaleServiceProviderLoader.getProviderSupportLocales(providersCache,PROVIDER_CONFIGURATION_FILE_NAME);
  if (providerLocales == null) {
    return icuLocales;
  }
  Locale[] locales=new Locale[icuLocales.length + providerLocales.length];
  System.arraycopy(icuLocales,0,locales,0,icuLocales.length);
  System.arraycopy(providerLocales,0,locales,icuLocales.length,providerLocales.length);
  return locales;
}
