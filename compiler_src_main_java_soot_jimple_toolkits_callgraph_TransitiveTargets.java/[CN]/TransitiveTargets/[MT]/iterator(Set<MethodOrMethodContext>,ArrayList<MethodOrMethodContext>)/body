{
  for (int i=0; i < worklist.size(); i++) {
    MethodOrMethodContext method=worklist.get(i);
    Iterator it=cg.edgesOutOf(method);
    if (filter != null)     it=filter.wrap(it);
    while (it.hasNext()) {
      Edge e=(Edge)it.next();
      if (s.add(e.getTgt()))       worklist.add(e.getTgt());
    }
  }
  return worklist.iterator();
}
