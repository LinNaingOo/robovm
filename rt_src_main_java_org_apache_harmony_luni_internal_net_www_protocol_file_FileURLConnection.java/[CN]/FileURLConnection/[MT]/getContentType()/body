{
  try {
    if (!connected) {
      connect();
    }
  }
 catch (  IOException e) {
    return MimeTable.UNKNOWN;
  }
  if (isDir) {
    return "text/plain";
  }
  String result=guessContentTypeFromName(url.getFile());
  if (result != null) {
    return result;
  }
  try {
    result=guessContentTypeFromStream(is);
  }
 catch (  IOException e) {
  }
  if (result != null) {
    return result;
  }
  return MimeTable.UNKNOWN;
}
