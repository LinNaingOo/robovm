{
  String contentType=getContentType();
  if (contentType != null) {
    header.put("content-type",contentType);
  }
  if (length >= 0) {
    header.put("content-length",Integer.toString(length));
  }
  if (lastModified != 0) {
    SimpleDateFormat dateFormat=new SimpleDateFormat("EEE, dd MMM yyyy HH:mm:ss 'GMT'");
    header.put("last-modified",dateFormat.format(new Date(lastModified)));
  }
}
