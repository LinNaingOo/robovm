{
  SSLContext sslContext=SSLContext.getInstance("TLS");
  sslContext.init(null,null,null);
  for (int i=0; i < 2048; ++i) {
    sslContext.getSocketFactory().createSocket().close();
  }
  sslContext.init(null,null,new SecureRandom());
  for (int i=0; i < 2048; ++i) {
    sslContext.getSocketFactory().createSocket().close();
  }
}
