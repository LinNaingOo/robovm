{
  String oldIndent=getIndent();
  if (branchTarget) {
    setIndent(labelIndent);
    handleIndent();
    setIndent(oldIndent);
    String label=(String)labels.get(u);
    if (label == null || label.equals("<unnamed>"))     label="[?= " + u + "]";
    output.append(label);
  }
 else {
    String ref=references.get(u);
    if (startOfLine) {
      String newIndent="(" + ref + ")"+ indent.substring(ref.length() + 2);
      setIndent(newIndent);
      handleIndent();
      setIndent(oldIndent);
    }
 else     output.append(ref);
  }
}
