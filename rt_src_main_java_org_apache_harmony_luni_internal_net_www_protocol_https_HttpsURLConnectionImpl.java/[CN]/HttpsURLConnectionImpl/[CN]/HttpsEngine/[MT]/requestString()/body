{
  if (super.usingProxy()) {
    if (makingSSLTunnel) {
      int port=url.getPort();
      return (port > 0) ? url.getHost() + ":" + port : url.getHost();
    }
    String file=url.getFile();
    if (file == null || file.length() == 0) {
      file="/";
    }
    return file;
  }
  return super.requestString();
}
