{
  if (start > end) {
    throw new IllegalArgumentException(Messages.getString("luni.35",Integer.valueOf(start),Integer.valueOf(end)));
  }
  if (start < 0) {
    throw new ArrayIndexOutOfBoundsException(Messages.getString("luni.36",start));
  }
  if (end > arrLength) {
    throw new ArrayIndexOutOfBoundsException(Messages.getString("luni.36",end));
  }
}
