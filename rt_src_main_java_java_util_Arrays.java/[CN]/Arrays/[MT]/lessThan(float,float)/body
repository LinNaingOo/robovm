{
  if (float1 < float2) {
    return true;
  }
  if (float1 > float2) {
    return false;
  }
  if (float1 == float2 && 0.0f != float1) {
    return false;
  }
  if (Float.isNaN(float1)) {
    return false;
  }
 else   if (Float.isNaN(float2)) {
    return true;
  }
  int f1=Float.floatToRawIntBits(float1);
  int f2=Float.floatToRawIntBits(float2);
  return f1 < f2;
}
