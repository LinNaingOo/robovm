{
  if (src == dst && from + len > to) {
    int new_to=to + len - 1;
    for (; from < to; from++, new_to--, len--) {
      dst[new_to]=src[from];
    }
    for (; len > 1; from++, new_to--, len-=2) {
      swap(from,new_to,dst);
    }
  }
 else {
    to=to + len - 1;
    for (; len > 0; from++, to--, len--) {
      dst[to]=src[from];
    }
  }
}
