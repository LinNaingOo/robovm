{
  int m=l;
  int d=1;
  while (m <= r) {
    if (c.compare(val,arr[m]) > bnd) {
      l=m + 1;
    }
 else {
      r=m - 1;
      break;
    }
    m+=d;
    d<<=1;
  }
  while (l <= r) {
    m=(l + r) >>> 1;
    if (c.compare(val,arr[m]) > bnd) {
      l=m + 1;
    }
 else {
      r=m - 1;
    }
  }
  return l - 1;
}
