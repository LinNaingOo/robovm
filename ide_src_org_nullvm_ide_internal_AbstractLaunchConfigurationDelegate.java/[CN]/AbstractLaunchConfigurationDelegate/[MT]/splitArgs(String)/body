{
  if (args == null || args.trim().length() == 0) {
    return Collections.emptyList();
  }
  String[] parts=CommandLine.parse("foo " + args).toStrings();
  if (parts.length <= 1) {
    return Collections.emptyList();
  }
  List<String> result=new ArrayList<String>(parts.length - 1);
  for (int i=1; i < parts.length; i++) {
    result.add(parts[i]);
  }
  return result;
}
