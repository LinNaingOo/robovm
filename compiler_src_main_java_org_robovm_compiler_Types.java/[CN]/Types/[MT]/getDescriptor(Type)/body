{
  if (t instanceof PointerType) {
    return "J";
  }
  if (t == I8) {
    return "B";
  }
  if (t == I16) {
    return "S";
  }
  if (t == I32) {
    return "I";
  }
  if (t == I64) {
    return "J";
  }
  if (t == FLOAT) {
    return "F";
  }
  if (t == DOUBLE) {
    return "D";
  }
  throw new IllegalArgumentException(t.toString());
}
