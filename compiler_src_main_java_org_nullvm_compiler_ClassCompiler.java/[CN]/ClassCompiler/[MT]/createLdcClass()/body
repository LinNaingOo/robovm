{
  Function fn=new Function(_private,new FunctionAttribute[]{alwaysinline,optsize},mangleClass(sootClass) + "_ldc",new FunctionType(OBJECT_PTR,ENV_PTR));
  Variable result=fn.newVariable(OBJECT_PTR);
  fn.add(new Load(result,new ConstantBitcast(new ConstantGetelementptr(classInfoStruct.ref(),0,0,0),new PointerType(OBJECT_PTR))));
  fn.add(new Ret(result.ref()));
  return fn;
}
