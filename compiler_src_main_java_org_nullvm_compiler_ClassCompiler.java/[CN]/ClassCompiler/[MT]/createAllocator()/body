{
  Function fn=new Function(_private,new FunctionAttribute[]{alwaysinline,optsize},mangleClass(sootClass) + "_allocator",new FunctionType(OBJECT_PTR,ENV_PTR));
  Value result=call(fn,NVM_BC_ALLOCATE,fn.getParameterRef(0),new ConstantGetelementptr(classInfoStruct.ref(),0,0,0));
  fn.add(new Ret(result));
  return fn;
}
