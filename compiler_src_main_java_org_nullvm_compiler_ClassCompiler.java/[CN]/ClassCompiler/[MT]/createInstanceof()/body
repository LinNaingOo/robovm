{
  Function fn=new Function(_private,new FunctionAttribute[]{alwaysinline,optsize},mangleClass(sootClass) + "_instanceof",new FunctionType(I32,ENV_PTR,OBJECT_PTR));
  Value result=call(fn,NVM_BC_INSTANCEOF,fn.getParameterRef(0),new ConstantGetelementptr(classInfoStruct.ref(),0,0,0),fn.getParameterRef(1));
