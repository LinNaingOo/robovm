{
  String[] str={"test","",null};
  IdentityScopeSubclass iss;
  for (int i=0; i < str.length; i++) {
    try {
      iss=new IdentityScopeSubclass(str[i]);
      assertNotNull(iss);
      assertTrue(iss instanceof IdentityScope);
    }
 catch (    Exception e) {
      fail("Unexpected exception for parameter " + str[i]);
    }
  }
}
