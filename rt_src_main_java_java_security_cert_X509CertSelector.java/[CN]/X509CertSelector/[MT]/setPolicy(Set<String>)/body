{
  if (policies == null) {
    this.policies=null;
    return;
  }
  HashSet pols=new HashSet(policies.size());
  Iterator it=policies.iterator();
  while (it.hasNext()) {
    String certPolicyId=(String)it.next();
    checkOID(certPolicyId);
    pols.add(certPolicyId);
  }
  this.policies=Collections.unmodifiableSet(pols);
}
