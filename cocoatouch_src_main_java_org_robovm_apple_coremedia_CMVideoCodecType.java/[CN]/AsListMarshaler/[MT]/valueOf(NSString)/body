{
  byte[] by=n.toString().getBytes();
  long value=0;
  for (int i=0; i < by.length; i++) {
    value=(value << 8) + (by[i] & 0xff);
  }
  for (  CMVideoCodecType v : values()) {
    if (v.n == value) {
      return v;
    }
  }
  throw new IllegalArgumentException("No constant with value " + n + " found in "+ CMVideoCodecType.class.getName());
}
