{
  String baseStr="The quick brown fox jumps over the lazy dog!";
  String subStr;
  subStr=baseStr.substring(5,baseStr.length() - 4);
  System.out.println("subStr is '" + subStr + "'");
  System.out.println("Indexes are: " + baseStr.indexOf('T') + ":"+ subStr.indexOf('T')+ ":"+ subStr.indexOf('u')+ ":"+ baseStr.indexOf('!')+ ":"+ subStr.indexOf('y')+ ":"+ subStr.indexOf('d')+ ":"+ baseStr.indexOf('x')+ ":"+ subStr.indexOf('x',0)+ ":"+ subStr.indexOf('x',-1)+ ":"+ subStr.indexOf('x',200)+ ":"+ baseStr.indexOf('x',17)+ ":"+ baseStr.indexOf('x',18)+ ":"+ baseStr.indexOf('x',19)+ ":"+ subStr.indexOf('x',13)+ ":"+ subStr.indexOf('x',14)+ ":"+ subStr.indexOf('&')+ ":"+ baseStr.indexOf(0x12341234));
}
