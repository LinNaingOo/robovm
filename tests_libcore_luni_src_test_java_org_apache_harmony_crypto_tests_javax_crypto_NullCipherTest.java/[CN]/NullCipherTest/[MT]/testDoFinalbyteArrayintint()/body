{
  byte[] b={1,2,3,4,5};
  byte[] r=null;
  r=c.doFinal(b,0,5);
  assertEquals("different length",b.length,r.length);
  assertTrue("different content",Arrays.equals(b,r));
  r=c.doFinal(b,1,3);
  assertEquals("different length",3,r.length);
  for (int i=0; i < 3; i++) {
    assertEquals("different content",b[i + 1],r[i]);
  }
}
