{
  StringBuffer line=new StringBuffer(this.getName());
  if (this.attributes != null) {
    line.append(" [");
    for (    DotGraphAttribute attr : this.attributes) {
      line.append(attr.toString());
      line.append(",");
    }
    line.append("];");
  }
  DotGraphUtility.renderLine(out,new String(line),indent);
}
