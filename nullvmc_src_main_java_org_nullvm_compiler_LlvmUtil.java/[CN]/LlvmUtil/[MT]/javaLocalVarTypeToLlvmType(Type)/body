{
  if (t.getSort() == Type.OBJECT || t.getSort() == Type.ARRAY) {
    return "%Object*";
  }
switch (t.getSort()) {
case Type.BOOLEAN:
case Type.BYTE:
case Type.CHAR:
case Type.SHORT:
case Type.INT:
    return "i32";
case Type.DOUBLE:
  return "double";
case Type.FLOAT:
return "float";
case Type.LONG:
return "i64";
}
return "void";
}
