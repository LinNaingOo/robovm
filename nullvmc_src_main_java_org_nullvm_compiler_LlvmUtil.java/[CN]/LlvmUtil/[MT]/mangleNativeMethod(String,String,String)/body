{
  Type[] args=desc != null ? Type.getArgumentTypes(desc) : new Type[0];
  StringBuilder sb=new StringBuilder();
  sb.append("Java_");
  sb.append(mangleString(owner));
  sb.append("_");
  sb.append(mangleString(name));
  if (args.length > 0) {
    sb.append("__");
    for (    Type arg : args) {
      sb.append(mangleString(arg.getDescriptor()));
    }
  }
  return sb.toString();
}
