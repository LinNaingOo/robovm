{
  Map myMap=new TreeMap();
  for (int counter=0; counter < hmSize; counter++)   myMap.put(objArray2[counter],objArray[counter]);
  LinkedHashMap hm2=new LinkedHashMap(myMap);
  for (int counter=0; counter < hmSize; counter++)   assertTrue("Failed to construct correct LinkedHashMap",hm.get(objArray2[counter]) == hm2.get(objArray2[counter]));
}
