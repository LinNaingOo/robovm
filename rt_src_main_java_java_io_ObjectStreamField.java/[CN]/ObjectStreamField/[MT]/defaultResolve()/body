{
switch (typeString.charAt(0)) {
case 'I':
    type=Integer.TYPE;
  return true;
case 'B':
type=Byte.TYPE;
return true;
case 'C':
type=Character.TYPE;
return true;
case 'S':
type=Short.TYPE;
return true;
case 'Z':
type=Boolean.TYPE;
return true;
case 'J':
type=Long.TYPE;
return true;
case 'F':
type=Float.TYPE;
return true;
case 'D':
type=Double.TYPE;
return true;
default :
type=Object.class;
return false;
}
}
