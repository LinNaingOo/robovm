{
  ParsePosition pp1=new ParsePosition(0);
  ParsePosition pp2=new ParsePosition(0);
  assertTrue("hashCode returns non equal hash codes for equal objects.",pp1.hashCode() == pp2.hashCode());
  pp1.setIndex(Integer.MAX_VALUE);
  assertTrue("hashCode returns equal hash codes for non equal objects.",pp1.hashCode() != pp2.hashCode());
}
