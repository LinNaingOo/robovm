{
  if (size <= 0) {
    throw new IllegalArgumentException(Messages.getString("logging.11"));
  }
  target.getLevel();
  pushLevel.intValue();
  this.target=target;
  this.size=size;
  this.push=pushLevel;
  initProperties("ALL",null,"java.util.logging.SimpleFormatter",null);
  buffer=new LogRecord[size];
}
