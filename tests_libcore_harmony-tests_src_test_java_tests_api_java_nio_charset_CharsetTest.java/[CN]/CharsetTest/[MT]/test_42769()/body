{
  ArrayList<Thread> threads=new ArrayList<Thread>();
  for (int i=0; i < 10; ++i) {
    Thread t=new Thread(new Runnable(){
      public void run(){
        for (int i=0; i < 50; ++i) {
          Charset.availableCharsets();
        }
      }
    }
);
    threads.add(t);
  }
  for (  Thread t : threads) {
    t.start();
  }
  for (  Thread t : threads) {
    t.join();
  }
}
