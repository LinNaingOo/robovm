{
  if ("MockCharset00".equalsIgnoreCase(charsetName) || "MockCharset01".equalsIgnoreCase(charsetName) || "MockCharset02".equalsIgnoreCase(charsetName)) {
    return charset1;
  }
 else   if ("MockCharset10".equalsIgnoreCase(charsetName) || "MockCharset11".equalsIgnoreCase(charsetName) || "MockCharset12".equalsIgnoreCase(charsetName)) {
    return charset2;
  }
  return null;
}
