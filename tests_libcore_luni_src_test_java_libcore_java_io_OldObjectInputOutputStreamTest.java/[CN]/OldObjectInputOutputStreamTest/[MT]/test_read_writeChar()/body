{
  os.writeChar('b');
  os.close();
  openObjectInputStream();
  assertEquals("Test 1: Incorrect char written or read;",'b',is.readChar());
  try {
    is.readChar();
    fail("Test 2: EOFException expected.");
  }
 catch (  EOFException e) {
  }
  is.close();
  try {
    is.readChar();
    fail("Test 3: IOException expected.");
  }
 catch (  IOException e) {
  }
}
