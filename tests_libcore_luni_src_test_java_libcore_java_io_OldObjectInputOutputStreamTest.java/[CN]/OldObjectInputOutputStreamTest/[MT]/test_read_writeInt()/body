{
  os.writeInt(768347202);
  os.close();
  openObjectInputStream();
  assertEquals("Test 1: Incorrect int written or read;",768347202,is.readInt());
  try {
    is.readInt();
    fail("Test 2: EOFException expected.");
  }
 catch (  EOFException e) {
  }
  is.close();
  try {
    is.readInt();
    fail("Test 3: IOException expected.");
  }
 catch (  IOException e) {
  }
}
