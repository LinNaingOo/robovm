{
  this();
  for (Iterator i=dg.iterator(); i.hasNext(); ) {
    Object s=i.next();
    addNode(s);
  }
  for (Iterator i=dg.iterator(); i.hasNext(); ) {
    Object s=i.next();
    List succs=dg.getSuccsOf(s);
    for (Iterator succsIt=succs.iterator(); succsIt.hasNext(); ) {
      Object t=succsIt.next();
      addEdge(s,t);
    }
  }
  heads.clear();
  heads.addAll(dg.getHeads());
  tails.clear();
  tails.addAll(dg.getTails());
}
