{
  if (count == 0) {
    return null;
  }
 else   if (count == 1) {
    AbstractState s=head;
    head=null;
    tail=null;
    count=0;
    s.next=null;
    if (s == null)     throw new InternalError("hm... 1");
    return s;
  }
 else {
    AbstractState s=head;
    head=s.next;
    s.next=null;
    count-=1;
    if (s == null)     throw new InternalError("hm... 2");
    return s;
  }
}
