{
  if (diffScale < LONG_TEN_POW.length && Math.max(thisValue.bitLength,augend.bitLength + LONG_TEN_POW_BIT_LENGTH[diffScale]) + 1 < 64) {
    return valueOf(thisValue.smallValue + augend.smallValue * LONG_TEN_POW[diffScale],thisValue.scale);
  }
  return new BigDecimal(thisValue.getUnscaledValue().add(Multiplication.multiplyByTenPow(augend.getUnscaledValue(),diffScale)),thisValue.scale);
}
