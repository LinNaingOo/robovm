{
  byte[] buffer=new byte[8];
  if (read(buffer,0,buffer.length) != buffer.length) {
    throw new EOFException();
  }
  return ((long)(((buffer[0] & 0xff) << 24) + ((buffer[1] & 0xff) << 16) + ((buffer[2] & 0xff) << 8)+ (buffer[3] & 0xff)) << 32) + ((long)(buffer[4] & 0xff) << 24) + ((buffer[5] & 0xff) << 16)+ ((buffer[6] & 0xff) << 8)+ (buffer[7] & 0xff);
}
