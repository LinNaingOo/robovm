{
  if (null != m_topLevelVariables) {
    int n=getVariableOrParamCount();
    for (int i=0; i < n; i++) {
      ElemVariable var=getVariableOrParam(i);
      if ((var.getXSLToken() == Constants.ELEMNAME_PARAMVARIABLE) && (var.getName().equals(qname)))       return (ElemParam)var;
    }
  }
  return null;
}
