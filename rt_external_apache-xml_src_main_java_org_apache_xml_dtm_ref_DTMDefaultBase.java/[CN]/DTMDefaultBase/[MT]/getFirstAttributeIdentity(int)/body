{
  int type=_type(identity);
  if (DTM.ELEMENT_NODE == type) {
    while (DTM.NULL != (identity=getNextNodeIdentity(identity))) {
      type=_type(identity);
      if (type == DTM.ATTRIBUTE_NODE) {
        return identity;
      }
 else       if (DTM.NAMESPACE_NODE != type) {
        break;
      }
    }
  }
  return DTM.NULL;
}
