{
  if (mode != ENCRYPT_MODE && mode != DECRYPT_MODE) {
    throw new IllegalStateException(Messages.getString("crypto.1C"));
  }
  if (inputOffset < 0 || inputLen < 0 || inputOffset + inputLen > input.length) {
    throw new IllegalArgumentException(Messages.getString("crypto.1E"));
  }
  return spiImpl.engineDoFinal(input,inputOffset,inputLen);
}
