{
  if (!b) {
    ErrorListener errorHandler=getErrorListener();
    if (errorHandler != null) {
      errorHandler.fatalError(new TransformerException(XSLMessages.createMessage(XPATHErrorResources.ER_INCORRECT_PROGRAMMER_ASSERTION,new Object[]{msg}),(SAXSourceLocator)this.getSAXLocator()));
    }
  }
}
