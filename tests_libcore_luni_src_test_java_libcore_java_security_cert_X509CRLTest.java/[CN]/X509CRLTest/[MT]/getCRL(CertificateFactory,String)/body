{
  final InputStream is=Support_Resources.getStream(name);
  assertNotNull("File does not exist: " + name,is);
  try {
    X509CRL crl=(X509CRL)f.generateCRL(is);
    assertNotNull(crl);
    return crl;
  }
  finally {
    try {
      is.close();
    }
 catch (    IOException ignored) {
    }
  }
}
