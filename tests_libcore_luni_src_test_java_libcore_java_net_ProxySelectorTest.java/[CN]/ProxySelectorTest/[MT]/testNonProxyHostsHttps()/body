{
  System.setProperty("https.nonProxyHosts","*.com");
  System.setProperty("https.proxyHost","a");
  assertEquals(Arrays.asList(new Proxy(Proxy.Type.HTTP,createUnresolved("a",443))),proxySelector.select(new URI("https://foo.net")));
  assertEquals(Arrays.asList(Proxy.NO_PROXY),proxySelector.select(new URI("https://foo.com")));
}
