{
{
    SourceLnPosTag tag=(SourceLnPosTag)host.getTag("SourceLnPosTag");
    if (tag != null) {
      if (tag.startLn() <= lineNumber && tag.endLn() >= lineNumber) {
        return true;
      }
    }
  }
{
    LineNumberTag tag=(LineNumberTag)host.getTag("LineNumberTag");
    if (tag != null) {
      if (tag.getLineNumber() == lineNumber) {
        return true;
      }
    }
  }
  return false;
}
