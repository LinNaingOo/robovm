{
  if (identity == NULL) {
    return NULL;
  }
  int type=_type2(identity);
  if (DTM.ELEMENT_NODE == type) {
    while (true) {
      identity++;
      type=_type2(identity);
      if (type == DTM.ATTRIBUTE_NODE) {
        return identity;
      }
 else       if (DTM.NAMESPACE_NODE != type) {
        break;
      }
    }
  }
  return DTM.NULL;
}
