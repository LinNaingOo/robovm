{
  final int result=_currentNode;
  if (result == END)   return DTM.NULL;
  _currentNode=END;
  if (_nodeType >= DTM.NTYPES) {
    if (_exptype2(makeNodeIdentity(result)) == _nodeType) {
      return returnNode(result);
    }
  }
 else {
    if (_type2(makeNodeIdentity(result)) == _nodeType) {
      return returnNode(result);
    }
  }
  return NULL;
}
