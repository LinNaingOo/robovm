{
  if (node == DTMDefaultBase.ROOTNODE)   node=getDocument();
  if (_isRestartable) {
    _startNode=node;
    node=_startNodeID=makeNodeIdentity(node);
    if (node == NULL) {
      _currentNode=node;
      return resetPosition();
    }
    int type=_type2(node);
    if (ExpandedNameTable.ATTRIBUTE == type || ExpandedNameTable.NAMESPACE == type) {
      _currentNode=node;
    }
 else {
      _currentNode=_parent2(node);
      if (NULL != _currentNode)       _currentNode=_firstch2(_currentNode);
 else       _currentNode=node;
    }
    return resetPosition();
  }
  return this;
}
