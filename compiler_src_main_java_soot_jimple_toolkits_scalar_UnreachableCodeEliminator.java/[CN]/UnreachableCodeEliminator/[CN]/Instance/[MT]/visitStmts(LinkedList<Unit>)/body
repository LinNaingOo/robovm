{
  while (!st.isEmpty()) {
    Unit stmt=st.removeLast();
    if (!visited.contains(stmt)) {
      visited.add(stmt);
      Iterator<Unit> succIt=stmtGraph.getSuccsOf(stmt).iterator();
      while (succIt.hasNext()) {
        Unit o=succIt.next();
        if (!visited.contains(o))         st.addLast(o);
      }
    }
  }
}
