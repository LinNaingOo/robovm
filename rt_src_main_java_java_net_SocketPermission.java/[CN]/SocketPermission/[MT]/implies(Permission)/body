{
  SocketPermission sp;
  try {
    sp=(SocketPermission)p;
  }
 catch (  ClassCastException e) {
    return false;
  }
  if (sp == null || (actionsMask & sp.actionsMask) != sp.actionsMask) {
    return false;
  }
  if (!p.getActions().equals("resolve")) {
    if ((sp.portMin < this.portMin) || (sp.portMax > this.portMax)) {
      return false;
    }
  }
  return checkHost(sp);
}
