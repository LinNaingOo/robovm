{
  if (isPartialWild) {
    if (isWild) {
      return true;
    }
    int length=hostName.length() - 1;
    return sp.hostName.regionMatches(sp.hostName.length() - length,hostName,1,length);
  }
  return (getIPString(false) != null && ipString.equals(sp.getIPString(false))) || hostName.equals(sp.hostName);
}
