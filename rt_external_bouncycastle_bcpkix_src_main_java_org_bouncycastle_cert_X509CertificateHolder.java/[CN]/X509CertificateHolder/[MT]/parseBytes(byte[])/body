{
  try {
    return Certificate.getInstance(ASN1Primitive.fromByteArray(certEncoding));
  }
 catch (  ClassCastException e) {
    throw new CertIOException("malformed data: " + e.getMessage(),e);
  }
catch (  IllegalArgumentException e) {
    throw new CertIOException("malformed data: " + e.getMessage(),e);
  }
}
