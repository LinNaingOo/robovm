{
  try {
    DTDprolog();
    m_writer.write("<!ENTITY ");
    m_writer.write(name);
    if (pubID != null) {
      m_writer.write(" PUBLIC \"");
      m_writer.write(pubID);
    }
 else {
      m_writer.write(" SYSTEM \"");
      m_writer.write(sysID);
    }
    m_writer.write("\" NDATA ");
    m_writer.write(notationName);
    m_writer.write(" >");
    m_writer.write(m_lineSep,0,m_lineSepLen);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
