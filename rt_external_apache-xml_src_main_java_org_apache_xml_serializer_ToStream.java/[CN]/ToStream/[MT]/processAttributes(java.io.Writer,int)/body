{
  String encoding=getEncoding();
  for (int i=0; i < nAttrs; i++) {
    final String name=m_attributes.getQName(i);
    final String value=m_attributes.getValue(i);
    writer.write(' ');
    writer.write(name);
    writer.write("=\"");
    writeAttrString(writer,value,encoding);
    writer.write('\"');
  }
}
