{
  Class testClass=ClassLoader.getSystemClassLoader().loadClass("org.w3c.domts.level2.html.alltests");
  Constructor testConstructor=testClass.getConstructor(new Class[]{DOMTestDocumentBuilderFactory.class});
  DOMTestDocumentBuilderFactory factory=new XercesHTML2DocumentBuilderFactory(XercesHTML2DocumentBuilderFactory.getConfiguration1());
  Object test=testConstructor.newInstance(new Object[]{factory});
  return new JUnitTestSuiteAdapter((DOMTestSuite)test);
}
