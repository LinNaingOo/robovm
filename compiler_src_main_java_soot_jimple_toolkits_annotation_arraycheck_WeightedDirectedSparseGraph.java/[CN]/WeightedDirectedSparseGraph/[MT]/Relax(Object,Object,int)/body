{
  IntContainer dfrom=distance.get(from);
  IntContainer dto=distance.get(to);
  if (dfrom != null) {
    int vfrom=dfrom.value;
    int vnew=vfrom + weight;
    if (dto == null) {
      distance.put(to,new IntContainer(vnew));
      pei.put(to,from);
    }
 else {
      int vto=dto.value;
      if (vto > vnew) {
        distance.put(to,new IntContainer(vnew));
        pei.put(to,from);
      }
    }
  }
}
