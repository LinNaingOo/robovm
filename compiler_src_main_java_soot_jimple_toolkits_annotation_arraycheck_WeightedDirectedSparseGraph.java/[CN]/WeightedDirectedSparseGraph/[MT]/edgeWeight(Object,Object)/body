{
  Hashtable targets=sources.get(from);
  if (targets == null)   throw new RuntimeException("No such edge (" + from + " ,"+ to+ ") exists.");
  IntContainer weight=(IntContainer)targets.get(to);
  if (weight == null)   throw new RuntimeException("No such edge (" + from + ", "+ to+ ") exists.");
  return weight.value;
}
