{
synchronized (this) {
    wasRun++;
  }
  if (incrementCount) {
    timerCounter++;
  }
  if (terminateCount == timerCounter && timer != null) {
    timer.cancel();
  }
  if (sleepInRun) {
    try {
      Thread.sleep(200);
    }
 catch (    InterruptedException e) {
      throw new RuntimeException(e);
    }
  }
synchronized (sync) {
    sync.notify();
  }
}
