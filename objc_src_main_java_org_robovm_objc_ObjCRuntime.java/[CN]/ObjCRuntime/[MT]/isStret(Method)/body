{
  Class<?> returnType=method.getReturnType();
  if (returnType.getSuperclass() == Struct.class && (method.getAnnotation(ByVal.class) != null || returnType.getAnnotation(ByVal.class) != null)) {
    int structSize=getStructSize(returnType);
    if (Bro.IS_X86) {
      if (structSize > 8) {
        return true;
      }
    }
 else {
      if (structSize > 4) {
        return true;
      }
    }
  }
  return false;
}
