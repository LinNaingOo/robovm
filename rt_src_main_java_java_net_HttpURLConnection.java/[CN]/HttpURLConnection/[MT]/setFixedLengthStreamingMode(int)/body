{
  if (super.connected) {
    throw new IllegalStateException(Messages.getString("luni.5F"));
  }
  if (0 < chunkLength) {
    throw new IllegalStateException(Messages.getString("luni.60"));
  }
  if (0 > contentLength) {
    throw new IllegalArgumentException(Messages.getString("luni.61"));
  }
  this.fixedContentLength=contentLength;
}
