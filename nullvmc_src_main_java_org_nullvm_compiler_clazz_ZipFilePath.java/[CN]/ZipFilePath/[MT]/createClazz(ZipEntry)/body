{
  return new Clazz(entry.getName(),clazzes){
    byte[] bytes=null;
    public byte[] getBytes() throws IOException {
      if (bytes == null) {
        InputStream in=null;
        try {
          in=zipFile.getInputStream(entry);
          bytes=IOUtils.toByteArray(in);
        }
  finally {
          IOUtils.closeQuietly(in);
        }
      }
      return bytes;
    }
  }
;
}
