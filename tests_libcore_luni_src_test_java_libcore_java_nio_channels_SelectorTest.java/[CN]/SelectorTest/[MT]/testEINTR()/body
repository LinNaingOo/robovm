{
  Selector selector=Selector.open();
  new Thread(new Runnable(){
    @Override public void run(){
      try {
        Thread.sleep(2000);
        Libcore.os.kill(Libcore.os.getpid(),OsConstants.SIGQUIT);
      }
 catch (      Exception ex) {
        fail();
      }
    }
  }
).start();
  assertEquals(0,selector.select());
}
