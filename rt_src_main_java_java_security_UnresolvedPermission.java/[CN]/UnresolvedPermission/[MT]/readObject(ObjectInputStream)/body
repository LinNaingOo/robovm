{
  in.defaultReadObject();
  checkType(getUnresolvedType());
  int certNumber=in.readInt();
  if (certNumber != 0) {
    targetCerts=new Certificate[certNumber];
    for (int i=0; i < certNumber; i++) {
      try {
        String type=in.readUTF();
        int length=in.readInt();
        byte[] enc=new byte[length];
        in.readFully(enc,0,length);
        targetCerts[i]=CertificateFactory.getInstance(type).generateCertificate(new ByteArrayInputStream(enc));
      }
 catch (      CertificateException cee) {
        throw ((IOException)new IOException(Messages.getString("security.32")).initCause(cee));
      }
    }
  }
}
