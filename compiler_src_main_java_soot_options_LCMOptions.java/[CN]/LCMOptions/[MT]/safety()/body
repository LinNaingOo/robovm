{
  String s=soot.PhaseOptions.getString(options,"safety");
  if (s.equalsIgnoreCase("safe"))   return safety_safe;
  if (s.equalsIgnoreCase("medium"))   return safety_medium;
  if (s.equalsIgnoreCase("unsafe"))   return safety_unsafe;
  throw new RuntimeException("Invalid value " + s + " of phase option safety");
}
