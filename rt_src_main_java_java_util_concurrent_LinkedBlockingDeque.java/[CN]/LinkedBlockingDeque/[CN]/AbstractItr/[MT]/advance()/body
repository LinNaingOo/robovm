{
  final ReentrantLock lock=LinkedBlockingDeque.this.lock;
  lock.lock();
  try {
    Node<E> s=nextNode(next);
    if (s == next) {
      next=firstNode();
    }
 else {
      while (s != null && s.item == null)       s=nextNode(s);
      next=s;
    }
    nextItem=(next == null) ? null : next.item;
  }
  finally {
    lock.unlock();
  }
}
