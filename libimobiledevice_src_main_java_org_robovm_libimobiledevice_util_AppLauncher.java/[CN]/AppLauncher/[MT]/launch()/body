{
  try {
    return launchInternal();
  }
 catch (  IOException e) {
    throw e;
  }
catch (  RuntimeException e) {
    throw e;
  }
catch (  Exception e) {
    throw new RuntimeException();
  }
 finally {
    if (closeOutOnExit) {
      try {
        stdout.close();
      }
 catch (      Throwable t) {
      }
    }
  }
}
