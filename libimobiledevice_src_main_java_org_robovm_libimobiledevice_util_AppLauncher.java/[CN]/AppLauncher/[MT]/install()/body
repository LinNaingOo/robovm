{
  if (localAppPath != null) {
    try (LockdowndClient lockdowndClient=new LockdowndClient(device,getClass().getSimpleName(),true)){
      uploadInternal();
      debug("[ 50%] Upload done. Installing app...");
      installInternal();
      localAppPath=null;
    }
 catch (    IOException e) {
      throw e;
    }
catch (    RuntimeException e) {
      throw e;
    }
catch (    Exception e) {
      throw new RuntimeException();
    }
  }
}
