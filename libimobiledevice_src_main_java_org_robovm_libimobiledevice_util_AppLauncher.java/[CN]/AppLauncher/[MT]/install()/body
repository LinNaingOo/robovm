{
  if (localAppPath != null) {
    try (LockdowndClient lockdowndClient=new LockdowndClient(device,getClass().getSimpleName(),true)){
      uploadAndInstall(lockdowndClient);
      localAppPath=null;
    }
 catch (    IOException e) {
      throw e;
    }
catch (    RuntimeException e) {
      throw e;
    }
catch (    Exception e) {
      throw new RuntimeException();
    }
  }
}
