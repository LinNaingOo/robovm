{
  if (bufferedResponses.length() > 0) {
    char c=bufferedResponses.charAt(0);
    bufferedResponses.delete(0,1);
    return c == '+';
  }
  byte[] buffer=new byte[1];
  conn.receive(buffer,0,buffer.length,RECEIVE_TIMEOUT);
  debugGdb("Received ack: " + (char)buffer[0]);
  return buffer[0] == '+';
}
