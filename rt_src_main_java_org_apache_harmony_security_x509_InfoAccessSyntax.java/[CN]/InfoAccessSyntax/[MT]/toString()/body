{
  StringBuilder res=new StringBuilder();
  res.append("\n---- InfoAccessSyntax:");
  if (accessDescriptions != null) {
    for (Iterator it=accessDescriptions.iterator(); it.hasNext(); ) {
      res.append('\n');
      res.append(it.next());
    }
  }
  res.append("\n---- InfoAccessSyntax END\n");
  return res.toString();
}
