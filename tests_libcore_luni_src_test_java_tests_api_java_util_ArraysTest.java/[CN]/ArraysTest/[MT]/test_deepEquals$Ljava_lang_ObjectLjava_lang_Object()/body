{
  int[] a1={1,2,3};
  short[] a2={0,1};
  Object[] a3={new Integer(1),a2};
  int[] a4={6,5,4};
  int[] b1={1,2,3};
  short[] b2={0,1};
  Object[] b3={new Integer(1),b2};
  Object a[]={a1,a2,a3};
  Object b[]={b1,b2,b3};
  assertFalse(Arrays.equals(a,b));
  assertTrue(Arrays.deepEquals(a,b));
  a[2]=a4;
  assertFalse(Arrays.deepEquals(a,b));
}
