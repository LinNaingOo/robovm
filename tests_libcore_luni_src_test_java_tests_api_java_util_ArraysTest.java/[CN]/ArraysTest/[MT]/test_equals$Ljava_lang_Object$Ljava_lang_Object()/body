{
  Object d[]=new Object[1000];
  Object x[]=new Object[1000];
  Object o=new Object();
  Arrays.fill(d,o);
  Arrays.fill(x,new Object());
  assertTrue("Inequal arrays returned true",!Arrays.equals(d,x));
  Arrays.fill(x,o);
  d[50]=null;
  x[50]=null;
  assertTrue("equal arrays returned false",Arrays.equals(d,x));
}
