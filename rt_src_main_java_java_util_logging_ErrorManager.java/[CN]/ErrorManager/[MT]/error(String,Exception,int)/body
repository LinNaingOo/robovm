{
synchronized (this) {
    if (called) {
      return;
    }
    called=true;
  }
  System.err.println(this.getClass().getName() + ": " + FAILURES[errorCode]);
  if (message != null) {
    System.err.println(Messages.getString("logging.1E",message));
  }
  if (exception != null) {
    System.err.println(Messages.getString("logging.1F",exception));
  }
}
