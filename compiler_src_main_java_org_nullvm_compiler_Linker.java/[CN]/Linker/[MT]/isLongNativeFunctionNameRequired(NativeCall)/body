{
  if (nc.getMethodDesc().startsWith("()")) {
    return true;
  }
  Clazz target=config.getClazzes().load(nc.getTarget());
  List<MethodInfo> methods=target.getClazzInfo().getMethods(nc.getMethodName());
  int nativeCount=0;
  for (  MethodInfo mi : methods) {
    if (mi.isNative()) {
      nativeCount++;
    }
  }
  return nativeCount > 1;
}
