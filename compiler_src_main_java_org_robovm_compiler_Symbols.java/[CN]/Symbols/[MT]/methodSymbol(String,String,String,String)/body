{
  StringBuilder sb=new StringBuilder(type == null ? EXTERNAL_SYMBOL_PREFIX : INTERNAL_SYMBOL_PREFIX);
  sb.append(owner.replace('/','.'));
  sb.append('.');
  sb.append(name);
  sb.append(desc);
  if (type != null) {
    sb.append('[');
    sb.append(type);
    sb.append(']');
  }
  return sb.toString();
}
