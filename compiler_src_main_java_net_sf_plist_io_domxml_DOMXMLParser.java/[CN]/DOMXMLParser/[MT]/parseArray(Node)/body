{
  final NodeList children=node.getChildNodes();
  final ArrayList<NSObject> result=new ArrayList<NSObject>();
  for (int i=0; i < children.getLength(); i++) {
    if (children.item(i).getNodeType() == Node.TEXT_NODE) {
      if (children.item(i).getTextContent().trim().length() > 0)       throw new PropertyListException("Unexpected text content in NSArray node.");
    }
 else {
      result.add(parseNode(children.item(i)));
    }
  }
  return new NSArray(result);
}
