{
  try {
    new DatagramPacket("Hello".getBytes(),6);
    fail("IllegalArgumentException was not thrown.");
  }
 catch (  IllegalArgumentException expected) {
  }
  try {
    new DatagramPacket("Hello".getBytes(),-1);
    fail("IllegalArgumentException was not thrown.");
  }
 catch (  IllegalArgumentException expected) {
  }
}
