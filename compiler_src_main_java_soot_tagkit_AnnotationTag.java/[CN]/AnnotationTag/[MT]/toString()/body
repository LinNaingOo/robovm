{
  StringBuffer sb=new StringBuffer("Annotation: type: " + type + " num elems: "+ numElems+ " elems: ");
  if (elems != null) {
    Iterator<AnnotationElem> it=elems.iterator();
    while (it.hasNext()) {
      sb.append("\n");
      sb.append(it.next());
    }
  }
  sb.append("\n");
  return sb.toString();
}
