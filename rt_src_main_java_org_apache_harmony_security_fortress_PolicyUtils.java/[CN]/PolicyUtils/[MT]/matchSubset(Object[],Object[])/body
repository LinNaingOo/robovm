{
  if (what == null) {
    return true;
  }
  for (int i=0; i < what.length; i++) {
    if (what[i] != null) {
      if (where == null) {
        return false;
      }
      boolean found=false;
      for (int j=0; j < where.length; j++) {
        if (what[i].equals(where[j])) {
          found=true;
          break;
        }
      }
      if (!found) {
        return false;
      }
    }
  }
  return true;
}
