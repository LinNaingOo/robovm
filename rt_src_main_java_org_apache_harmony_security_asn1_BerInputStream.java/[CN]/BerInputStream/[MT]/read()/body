{
  if (offset == buffer.length) {
    throw new ASN1Exception(Messages.getString("security.13B"));
  }
  if (in == null) {
    return buffer[offset++] & 0xFF;
  }
 else {
    int octet=in.read();
    if (octet == -1) {
      throw new ASN1Exception(Messages.getString("security.13B"));
    }
    buffer[offset++]=(byte)octet;
    return octet;
  }
}
