{
  String name=mangleField(field) + "_getter";
  if (field.isStatic()) {
    return new FunctionBuilder(name,new FunctionType(getType(field.getType()),ENV_PTR)).linkage(_private).attribs(alwaysinline,optsize).build();
  }
 else {
    return new FunctionBuilder(name,new FunctionType(getType(field.getType()),ENV_PTR,OBJECT_PTR)).linkage(field.isPrivate() ? _private : external).attribs(alwaysinline,optsize).build();
  }
}
