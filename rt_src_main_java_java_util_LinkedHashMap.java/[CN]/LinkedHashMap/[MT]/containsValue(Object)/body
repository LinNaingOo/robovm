{
  LinkedHashMapEntry<K,V> entry=head;
  if (null == value) {
    while (null != entry) {
      if (null == entry.value) {
        return true;
      }
      entry=entry.chainForward;
    }
  }
 else {
    while (null != entry) {
      if (value.equals(entry.value)) {
        return true;
      }
      entry=entry.chainForward;
    }
  }
  return false;
}
