{
  if (nextF) {
    nextF=false;
    return nextch;
  }
  int ch=super.read();
  if (ch != '\\')   return ch;
  mini=new StringBuffer();
  ch=super.read();
  if (ch != 'u') {
    nextF=true;
    nextch=ch;
    return '\\';
  }
  mini.append("\\u");
  while (mini.length() < 6) {
    ch=super.read();
    mini.append((char)ch);
  }
  ch=Integer.parseInt(mini.substring(2).toString(),16);
  return ch;
}
