{
  if (null == m_selectPattern && sroot.getOptimizer()) {
    XPath newSelect=ElemVariable.rewriteChildToExpression(this);
    if (null != newSelect)     m_selectPattern=newSelect;
  }
  m_qnameID=sroot.getComposeState().getQNameID(m_qname);
  super.compose(sroot);
  java.util.Vector vnames=sroot.getComposeState().getVariableNames();
  if (null != m_selectPattern)   m_selectPattern.fixupVariables(vnames,sroot.getComposeState().getGlobalsSize());
}
