{
  try {
    new SignerStub("name").setKeyPair(null);
    fail("No expected NullPointerException");
  }
 catch (  NullPointerException e) {
  }
  try {
    KeyPair kp=new KeyPair(null,null);
    SignerStub s=new SignerStub("name");
    s.setKeyPair(kp);
  }
 catch (  InvalidParameterException e) {
