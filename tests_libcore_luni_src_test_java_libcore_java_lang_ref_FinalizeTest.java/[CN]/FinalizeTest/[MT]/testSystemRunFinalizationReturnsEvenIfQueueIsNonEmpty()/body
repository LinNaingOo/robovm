{
  AtomicInteger count=new AtomicInteger();
  AtomicBoolean keepGoing=new AtomicBoolean(true);
  createChainedFinalizer(count,keepGoing);
  FinalizationTester.induceFinalization();
  for (int i=0; i < 10; i++) {
    if (count.get() > 0) {
      break;
    }
    createChainedFinalizer(count,keepGoing);
    FinalizationTester.induceFinalization();
  }
  keepGoing.set(false);
  assertTrue(count.get() > 0);
}
