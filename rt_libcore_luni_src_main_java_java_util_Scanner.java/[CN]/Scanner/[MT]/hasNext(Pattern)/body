{
  checkOpen();
  checkNotNull(pattern);
  matchSuccessful=false;
  saveCurrentStatus();
  if (!setTokenRegion()) {
    recoverPreviousStatus();
    return false;
  }
  matcher.usePattern(pattern);
  boolean hasNext=false;
  if (matcher.matches()) {
    cachedNextIndex=findStartIndex;
    matchSuccessful=true;
    hasNext=true;
  }
  recoverPreviousStatus();
  return hasNext;
}
