{
  if (type != null) {
    return type + "(" + id+ ")";
  }
  if (this == ClassHierarchy.v().TOP) {
    return "TOP" + "(" + id + ")";
  }
  if (this == ClassHierarchy.v().R0_1) {
    return "R0_1" + "(" + id + ")";
  }
  if (this == ClassHierarchy.v().R0_127) {
    return "R0_127" + "(" + id + ")";
  }
  if (this == ClassHierarchy.v().R0_32767) {
    return "R0_32767" + "(" + id + ")";
  }
  return "ERROR!!!!";
}
