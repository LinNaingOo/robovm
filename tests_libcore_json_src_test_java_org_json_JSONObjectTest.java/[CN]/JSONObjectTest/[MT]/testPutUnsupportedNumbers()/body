{
  JSONObject object=new JSONObject();
  try {
    object.put("foo",Double.NaN);
    fail();
  }
 catch (  JSONException e) {
  }
  try {
    object.put("foo",Double.NEGATIVE_INFINITY);
    fail();
  }
 catch (  JSONException e) {
  }
  try {
    object.put("foo",Double.POSITIVE_INFINITY);
    fail();
  }
 catch (  JSONException e) {
  }
}
