{
  try {
    new JSONObject().put(null,false);
    fail();
  }
 catch (  JSONException e) {
  }
  try {
    new JSONObject().put(null,0.0d);
    fail();
  }
 catch (  JSONException e) {
  }
  try {
    new JSONObject().put(null,5);
    fail();
  }
 catch (  JSONException e) {
  }
  try {
    new JSONObject().put(null,5L);
    fail();
  }
 catch (  JSONException e) {
  }
  try {
    new JSONObject().put(null,"foo");
    fail();
  }
 catch (  JSONException e) {
  }
}
