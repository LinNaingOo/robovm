{
  JSONObject object=new JSONObject();
  try {
    object.accumulate("foo",Double.NaN);
    fail();
  }
 catch (  JSONException e) {
  }
  object.accumulate("foo",1);
  try {
    object.accumulate("foo",Double.NaN);
    fail();
  }
 catch (  JSONException e) {
  }
  object.accumulate("foo",2);
  try {
    object.accumulate("foo",Double.NaN);
    fail();
  }
 catch (  JSONException e) {
  }
}
