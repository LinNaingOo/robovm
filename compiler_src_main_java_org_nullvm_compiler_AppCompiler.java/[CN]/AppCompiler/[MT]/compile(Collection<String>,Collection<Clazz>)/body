{
  for (  Path path : config.getClazzes().getPaths()) {
    for (    Clazz clazz : path.listClasses()) {
      if (classCompiler.compile(clazz)) {
        compiled.add(clazz);
        if (clazz.getClazzInfo().isStruct()) {
          dirtyStructs.add(clazz.getInternalName());
        }
      }
    }
  }
}
