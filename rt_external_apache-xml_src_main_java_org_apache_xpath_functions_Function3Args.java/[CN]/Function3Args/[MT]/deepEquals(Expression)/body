{
  if (!super.deepEquals(expr))   return false;
  if (null != m_arg2) {
    if (null == ((Function3Args)expr).m_arg2)     return false;
    if (!m_arg2.deepEquals(((Function3Args)expr).m_arg2))     return false;
  }
 else   if (null != ((Function3Args)expr).m_arg2)   return false;
  return true;
}
