{
  if (!DEFSupported) {
    fail(NotSupportedMsg);
    return;
  }
  String provider=null;
  for (int i=0; i < validValues.length; i++) {
    try {
      Mac.getInstance(validValues[i],provider);
      fail("IllegalArgumentException must be thrown when provider is null");
    }
 catch (    IllegalArgumentException e) {
    }
    try {
      Mac.getInstance(validValues[i],"");
      fail("IllegalArgumentException must be thrown when provider is empty");
    }
 catch (    IllegalArgumentException e) {
    }
    for (int j=1; j < invalidValues.length; j++) {
      try {
        Mac.getInstance(validValues[i],invalidValues[j]);
        fail("NoSuchProviderException must be thrown (algorithm: ".concat(validValues[i]).concat(" provider: ").concat(invalidValues[j]).concat(")"));
      }
 catch (      NoSuchProviderException e) {
      }
    }
  }
}
