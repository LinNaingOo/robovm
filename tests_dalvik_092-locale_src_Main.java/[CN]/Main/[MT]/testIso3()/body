{
  Locale loc;
  loc=new Locale("en","US");
  System.out.println("loc: " + loc);
  System.out.println(" iso3=" + loc.getISO3Language());
  loc=new Locale("eng","USA");
  System.out.println("loc: " + loc);
  try {
    System.out.println(" iso3=" + loc.getISO3Language());
  }
 catch (  MissingResourceException mre) {
    System.err.println("couldn't get iso3 language");
  }
}
