{
  String s=soot.PhaseOptions.getString(options,"q");
  if (s.equalsIgnoreCase("auto"))   return q_auto;
  if (s.equalsIgnoreCase("trad"))   return q_trad;
  if (s.equalsIgnoreCase("bdd"))   return q_bdd;
  if (s.equalsIgnoreCase("debug"))   return q_debug;
  if (s.equalsIgnoreCase("trace"))   return q_trace;
  if (s.equalsIgnoreCase("numtrace"))   return q_numtrace;
  throw new RuntimeException("Invalid value " + s + " of phase option q");
}
