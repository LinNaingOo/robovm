{
  BitSet bs=new BitSet(128);
  bs.or(eightbs);
  for (int i=0; i < 8; i++) {
    assertTrue("OR failed to set bits",bs.get(i));
  }
  bs=new BitSet(0);
  bs.or(eightbs);
  for (int i=0; i < 8; i++) {
    assertTrue("OR(0) failed to set bits",bs.get(i));
  }
  eightbs.clear(5);
  bs=new BitSet(128);
  bs.or(eightbs);
  assertTrue("OR set a bit which should be off",!bs.get(5));
}
