{
  BitSet bs=new BitSet();
  assertEquals(bs.toString(),0,bs.length());
  bs.set(5);
  assertEquals(bs.toString(),6,bs.length());
  bs.set(10);
  assertEquals(bs.toString(),11,bs.length());
  bs.set(432);
  assertEquals(bs.toString(),433,bs.length());
  bs.set(300);
  assertEquals(bs.toString(),433,bs.length());
}
