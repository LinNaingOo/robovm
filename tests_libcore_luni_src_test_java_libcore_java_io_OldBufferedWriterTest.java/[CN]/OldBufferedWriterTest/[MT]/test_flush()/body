{
  bw.write("This should not cause a flush");
  assertTrue("Test 1: Bytes written without flush.",sw.toString().equals(""));
  bw.flush();
  assertEquals("Test 2: Bytes not flushed.","This should not cause a flush",sw.toString());
  bw.close();
  bw=new BufferedWriter(ssw);
  try {
    bw.flush();
    fail("Test 3: IOException expected.");
  }
 catch (  IOException e) {
  }
}
