{
  if (s1 == null || s2 == null) {
    return false;
  }
  if (s1 == s2) {
    return true;
  }
  int length=s1.length();
  if (length != s2.length()) {
    return false;
  }
  for (int i=0; i < length; i++) {
    char c1=s1.charAt(i);
    char c2=s2.charAt(i);
    if (c1 != c2 && toASCIIUpperCase(c1) != toASCIIUpperCase(c2)) {
      return false;
    }
  }
  return true;
}
