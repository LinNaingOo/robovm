{
  if (buf1 == null || buf2 == null) {
    return false;
  }
  if (buf1 == buf2) {
    return true;
  }
  if (buf1.length != buf2.length) {
    return false;
  }
  for (int i=0; i < buf1.length; i++) {
    byte b1=buf1[i];
    byte b2=buf2[i];
    if (b1 != b2 && toASCIIUpperCase(b1) != toASCIIUpperCase(b2)) {
      return false;
    }
  }
  return true;
}
