{
  HashSet hs2=new HashSet(Arrays.asList(objArray));
  for (int counter=0; counter < objArray.length; counter++)   assertTrue("HashSet does not contain correct elements",hs.contains(objArray[counter]));
  assertTrue("HashSet created from collection incorrect size",hs2.size() == objArray.length);
  try {
    new HashSet(null);
    fail("NullPointerException expected");
  }
 catch (  NullPointerException e) {
  }
}
