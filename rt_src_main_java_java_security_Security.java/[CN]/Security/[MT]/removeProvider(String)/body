{
  Provider p;
  if ((name == null) || (name.length() == 0)) {
    return;
  }
  p=getProvider(name);
  if (p == null) {
    return;
  }
  SecurityManager sm=System.getSecurityManager();
  if (sm != null) {
    sm.checkSecurityAccess("removeProvider." + name);
  }
  Services.removeProvider(p.getProviderNumber());
  renumProviders();
  p.setProviderNumber(-1);
}
