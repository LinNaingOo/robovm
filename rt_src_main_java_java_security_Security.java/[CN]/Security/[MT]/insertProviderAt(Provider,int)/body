{
  SecurityManager sm=System.getSecurityManager();
  if (sm != null) {
    sm.checkSecurityAccess("insertProvider." + provider.getName());
  }
  if (getProvider(provider.getName()) != null) {
    return -1;
  }
  int result=Services.insertProviderAt(provider,position);
  renumProviders();
  return result;
}
