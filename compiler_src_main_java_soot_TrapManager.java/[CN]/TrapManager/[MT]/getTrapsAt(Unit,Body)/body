{
  List<Trap> trapsList=new ArrayList<Trap>();
  Chain units=b.getUnits();
  Iterator trapsIt=b.getTraps().iterator();
  while (trapsIt.hasNext()) {
    Trap t=(Trap)trapsIt.next();
    Iterator it=units.iterator(t.getBeginUnit(),units.getPredOf(t.getEndUnit()));
    while (it.hasNext())     if (unit.equals(it.next()))     trapsList.add(t);
  }
  return trapsList;
}
