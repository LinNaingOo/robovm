{
  byte[] ord=original.getBytes();
  int quotes=0;
  for (  byte element : ord) {
    if (element == '\"')     quotes++;
  }
  if (quotes == 0)   return original;
  byte[] newsrc=new byte[ord.length + quotes];
  for (int i=0, j=0, n=ord.length; i < n; i++, j++) {
    if (ord[i] == '\"') {
      newsrc[j++]=(byte)'\\';
    }
    newsrc[j]=ord[i];
  }
  return new String(newsrc);
}
