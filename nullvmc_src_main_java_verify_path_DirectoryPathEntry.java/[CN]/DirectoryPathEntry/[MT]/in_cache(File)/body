{
  String dir=f.getParent();
  CacheEntry ent;
  if (dir == null)   throw new IllegalArgumentException();
  ent=(CacheEntry)seen_cache.get(dir);
  if (ent == null) {
    ent=new CacheEntry(dir);
    seen_cache.put(dir,ent);
  }
  if (ent.contains(f.getPath())) {
    return true;
  }
  if (ent.old()) {
    if (f.exists()) {
      seen_cache.remove(dir);
      return true;
    }
 else {
      ent.touch();
    }
  }
  return false;
}
