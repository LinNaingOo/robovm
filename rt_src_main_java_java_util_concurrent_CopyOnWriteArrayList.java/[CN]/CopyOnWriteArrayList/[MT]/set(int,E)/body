{
  lock.lock();
  try {
    int size=size();
    checkIndexExlusive(index,size);
    E[] data;
    data=newElementArray(size);
    E[] oldArr=getData();
    System.arraycopy(oldArr,0,data,0,size);
    E old=data[index];
    data[index]=e;
    setData(data);
    return old;
  }
  finally {
    lock.unlock();
  }
}
