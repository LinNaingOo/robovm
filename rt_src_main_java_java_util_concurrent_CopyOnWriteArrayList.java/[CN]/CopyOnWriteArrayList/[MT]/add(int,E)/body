{
  lock.lock();
  try {
    E[] data;
    E[] old=getData();
    int size=old.length;
    checkIndexInclusive(index,size);
    data=newElementArray(size + 1);
    System.arraycopy(old,0,data,0,index);
    data[index]=e;
    if (size > index) {
      System.arraycopy(old,index,data,index + 1,size - index);
    }
    setData(data);
  }
  finally {
    lock.unlock();
  }
}
