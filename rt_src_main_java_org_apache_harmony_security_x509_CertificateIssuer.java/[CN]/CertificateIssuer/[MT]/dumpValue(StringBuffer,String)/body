{
  buffer.append(prefix).append("Certificate Issuer: ");
  if (issuer == null) {
    try {
      issuer=getIssuer();
    }
 catch (    IOException e) {
      buffer.append("Unparseable (incorrect!) extension value:\n");
      super.dumpValue(buffer);
    }
  }
  buffer.append(issuer).append('\n');
}
