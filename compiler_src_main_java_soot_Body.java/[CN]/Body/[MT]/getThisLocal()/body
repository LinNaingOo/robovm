{
  Iterator<Unit> unitsIt=getUnits().iterator();
  while (unitsIt.hasNext()) {
    Unit s=unitsIt.next();
    if (s instanceof IdentityStmt && ((IdentityStmt)s).getRightOp() instanceof ThisRef)     return (Local)(((IdentityStmt)s).getLeftOp());
  }
  throw new RuntimeException("couldn't find identityref!" + " in " + getMethod());
}
