{
  StringBuilder sb=new StringBuilder();
  sb.append("c\"");
  for (int i=0; i < bytes.length; i++) {
    int b=bytes[i] & 0xff;
    if (b < ' ' || b > '~' || b == '"' || b == '\\') {
      sb.append(String.format("\\%02X",b));
    }
 else {
      sb.append((char)b);
    }
  }
  sb.append('"');
  return sb.toString();
}
