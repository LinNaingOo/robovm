{
  Map myMap=new TreeMap();
  for (int counter=0; counter < hmSize; counter++)   myMap.put(objArray2[counter],objArray[counter]);
  IdentityHashMap hm2=new IdentityHashMap(myMap);
  for (int counter=0; counter < hmSize; counter++)   assertTrue("Failed to construct correct IdentityHashMap",hm.get(objArray2[counter]) == hm2.get(objArray2[counter]));
  Map mockMap=new MockMap();
  hm2=new IdentityHashMap(mockMap);
  assertEquals("Size should be 0",0,hm2.size());
  try {
    new IdentityHashMap(null);
    fail("NullPointerException expected");
  }
 catch (  NullPointerException e) {
  }
}
