{
  final int prime=31;
  int result=1;
  result=prime * result + ((className == null) ? 0 : className.hashCode());
  result=prime * result + (inBootClasspath ? 1231 : 1237);
  result=prime * result + ((path == null) ? 0 : path.hashCode());
  result=prime * result + (weak ? 1231 : 1237);
  return result;
}
