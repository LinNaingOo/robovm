{
  int length=s.length();
  long handle=useNativeHeap ? VM.malloc(length + 1) : VM.allocateMemoryAtomic(length + 1);
  long address=handle;
  for (int i=0; i < length; i++) {
    char c=s.charAt(i);
    if (c < 0x80) {
      VM.setByte(address,(byte)c);
    }
 else {
      VM.setByte(address,(byte)'?');
    }
    address++;
  }
  return Struct.toStruct(BytePtr.class,handle);
}
