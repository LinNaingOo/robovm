{
  if (0 <= index && index <= count) {
    if (string == null) {
      string="null";
    }
    int min=string.length();
    if (min != 0) {
      move(min,index);
      string.getChars(0,min,value,index);
      count+=min;
    }
  }
 else {
    throw new StringIndexOutOfBoundsException(index);
  }
}
