{
  ByteBuffer[] writeBuffers=new ByteBuffer[2];
  try {
    writeOnlyFileChannel.write(writeBuffers,0,2);
    fail("should throw NullPointerException");
  }
 catch (  NullPointerException e) {
  }
  try {
    readWriteFileChannel.write(writeBuffers,0,2);
    fail("should throw NullPointerException");
  }
 catch (  NullPointerException e) {
  }
}
