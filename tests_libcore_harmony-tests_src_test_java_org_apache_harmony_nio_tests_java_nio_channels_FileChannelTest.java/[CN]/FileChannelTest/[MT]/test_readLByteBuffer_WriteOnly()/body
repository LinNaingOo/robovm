{
  ByteBuffer readBuffer=ByteBuffer.allocate(CAPACITY);
  try {
    writeOnlyFileChannel.read(readBuffer);
    fail();
  }
 catch (  NonReadableChannelException expected) {
  }
  readBuffer=null;
  try {
    writeOnlyFileChannel.read(readBuffer);
    fail();
  }
 catch (  NonReadableChannelException expected) {
  }
catch (  NullPointerException expected) {
  }
}
