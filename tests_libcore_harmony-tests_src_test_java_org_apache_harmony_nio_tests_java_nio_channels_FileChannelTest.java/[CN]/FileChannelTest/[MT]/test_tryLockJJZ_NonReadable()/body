{
  try {
    writeOnlyFileChannel.tryLock(0,10,true);
    fail("should throw NonReadableChannelException");
  }
 catch (  NonReadableChannelException e) {
  }
  try {
    writeOnlyFileChannel.tryLock(-1,0,true);
    fail("should throw NonReadableChannelException");
  }
 catch (  NonReadableChannelException e) {
  }
}
