{
  try {
    readOnlyFileChannel.lock(0,10,false);
    fail("should throw NonWritableChannelException");
  }
 catch (  NonWritableChannelException e) {
  }
  try {
    readOnlyFileChannel.lock(-1,0,false);
    fail("should throw NonWritableChannelException");
  }
 catch (  NonWritableChannelException e) {
  }
}
