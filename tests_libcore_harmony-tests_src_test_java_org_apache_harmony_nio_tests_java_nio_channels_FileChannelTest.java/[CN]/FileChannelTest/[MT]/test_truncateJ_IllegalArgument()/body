{
  try {
    readOnlyFileChannel.truncate(-1);
    fail("should throw IllegalArgumentException");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    writeOnlyFileChannel.truncate(-1);
    fail("should throw IllegalArgumentException");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    readWriteFileChannel.truncate(-1);
    fail("should throw IllegalArgumentException");
  }
 catch (  IllegalArgumentException e) {
  }
}
