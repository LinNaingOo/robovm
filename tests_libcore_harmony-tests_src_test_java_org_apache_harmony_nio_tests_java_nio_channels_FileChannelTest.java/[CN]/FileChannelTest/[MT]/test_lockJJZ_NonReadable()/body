{
  try {
    writeOnlyFileChannel.lock(0,10,true);
    fail("should throw NonReadableChannelException");
  }
 catch (  NonReadableChannelException e) {
  }
  try {
    writeOnlyFileChannel.lock(-1,0,true);
    fail("should throw NonReadableChannelException");
  }
 catch (  NonReadableChannelException e) {
  }
}
