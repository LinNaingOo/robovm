{
  Echo.println(" testNullPointerExceptionArrayModification()");
  try {
    Object[] array=null;
    array[1]=null;
    Echo.println("  NullPointerException expected");
  }
 catch (  NullPointerException e) {
    Echo.println("  NullPointerException thrown as expected");
  }
  try {
    int[] array=null;
    array[1]=10;
    Echo.println("  NullPointerException expected");
  }
 catch (  NullPointerException e) {
    Echo.println("  NullPointerException thrown as expected");
  }
}
