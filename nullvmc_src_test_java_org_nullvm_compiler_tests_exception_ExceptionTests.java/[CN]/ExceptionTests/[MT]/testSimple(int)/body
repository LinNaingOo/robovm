{
  Echo.print(" testSimple(");
  Echo.print(n);
  Echo.println(")");
  try {
    Echo.println("  try");
    if (n == 0) {
      Echo.println("   throw Throwable");
      throw new Throwable();
    }
    Echo.println("   return");
    return;
  }
 catch (  Throwable t) {
    Echo.println("  catch (Throwable)");
  }
 finally {
    Echo.println("  finally");
  }
  Echo.println("  return");
}
