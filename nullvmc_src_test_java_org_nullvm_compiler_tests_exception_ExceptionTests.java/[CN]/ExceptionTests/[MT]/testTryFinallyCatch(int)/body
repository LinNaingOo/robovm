{
  Echo.print(" testTryFinallyCatch(");
  Echo.print(n);
  Echo.println(")");
  try {
    try {
      Echo.println("   try");
      if (n == 0) {
        Echo.println("    throw Throwable");
        throw new RuntimeException();
      }
    }
  finally {
      Echo.println("   finally");
    }
    Echo.println("   return");
  }
 catch (  RuntimeException t) {
    Echo.println("  catch (RuntimeException)");
  }
  Echo.println("  return");
}
