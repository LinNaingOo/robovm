{
  if (connected) {
    throw new IllegalStateException(Messages.getString("luni.5E"));
  }
  if (field == null) {
    throw new NullPointerException(Messages.getString("luni.95"));
  }
  List<String> valuesList=new ArrayList<String>();
  valuesList.add(newValue);
  requestProperties.put(field,valuesList);
}
