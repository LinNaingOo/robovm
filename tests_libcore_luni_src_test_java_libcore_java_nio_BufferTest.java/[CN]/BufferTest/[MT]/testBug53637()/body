{
  MappedByteBuffer mapped=(MappedByteBuffer)allocateMapped(1);
  mapped.get();
  mapped.rewind();
  mapped.get();
  mapped.rewind();
  mapped.mark();
  mapped.get();
  mapped.reset();
  mapped.get();
  mapped.rewind();
  mapped.get();
  mapped.clear();
  mapped.get();
  mapped.rewind();
  mapped.get();
  mapped.flip();
  mapped.get();
}
