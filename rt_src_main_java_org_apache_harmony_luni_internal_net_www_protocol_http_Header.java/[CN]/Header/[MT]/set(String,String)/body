{
  if (key == null) {
    throw new NullPointerException();
  }
  LinkedList<String> list=keyTable.get(key);
  if (list == null) {
    add(key,value);
  }
 else {
    list.clear();
    list.add(value);
    for (int i=0; i < props.size(); i+=2) {
      String propKey=props.get(i);
      if (propKey != null && key.equals(propKey)) {
        props.set(i + 1,value);
      }
    }
  }
}
