{
  if (annotationType == null) {
    throw new NullPointerException("annotationType == null");
  }
  Annotation[] annos=getDeclaredAnnotations(false);
  for (int i=annos.length - 1; i >= 0; --i) {
    if (annos[i].annotationType() == annotationType) {
      return (T)annos[i];
    }
  }
  return null;
}
