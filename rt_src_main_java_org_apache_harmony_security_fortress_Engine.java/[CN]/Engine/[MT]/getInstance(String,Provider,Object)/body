{
  Provider.Service serv=null;
  if (algorithm == null) {
    throw new NoSuchAlgorithmException(Messages.getString("security.14B",serviceName));
  }
  serv=provider.getService(serviceName,algorithm);
  if (serv == null) {
    throw new NoSuchAlgorithmException(Messages.getString("security.14A",serviceName,algorithm));
  }
  spi=serv.newInstance(param);
  this.provider=provider;
}
