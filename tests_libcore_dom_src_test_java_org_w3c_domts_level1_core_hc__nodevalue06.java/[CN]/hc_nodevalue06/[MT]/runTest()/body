{
  Document newNode;
  String newValue;
  newNode=(Document)load("hc_staff",true);
  newValue=newNode.getNodeValue();
  assertNull("initiallyNull",newValue);
  newNode.setNodeValue("This should have no effect");
  newValue=newNode.getNodeValue();
  assertNull("nullAfterAttemptedChange",newValue);
}
