{
  this.threadPtr=threadPtr;
  this.daemon=daemon;
  this.threadGroup=group == null ? ThreadGroup.mainGroup : group;
synchronized (Thread.class) {
    id=++counter;
  }
  this.name=name == null ? "Thread-" + this.id : name;
