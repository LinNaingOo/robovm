{
  if (obj == null) {
    return false;
  }
  if (obj.getClass() != this.getClass()) {
    return false;
  }
  byte[] objIPaddress=((InetAddress)obj).ipaddress;
  for (int i=0; i < objIPaddress.length; i++) {
    if (objIPaddress[i] != this.ipaddress[i]) {
      return false;
    }
  }
  return true;
}
