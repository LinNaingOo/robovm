{
  CacheElement previous=null;
  CacheElement current=head;
  boolean notFound=true;
  while ((null != current) && (notFound=!(name.equals(current.hostName())))) {
    previous=current;
    current=current.next;
  }
  if (notFound) {
    return null;
  }
  moveToHead(current,previous);
  return current;
}
