{
  System.out.println("IntMath.jlmTests");
  assert(Math.abs(ii) == ii);
  assert(Math.abs(-ii) == ii);
  assert(Math.min(ii,-5) == -5);
  assert(Math.max(ii,-5) == ii);
  assert(Math.abs(ll) == ll);
  assert(Math.abs(-ll) == ll);
  assert(Math.min(ll,-5L) == -5L);
  assert(Math.max(ll,-5L) == ll);
}
