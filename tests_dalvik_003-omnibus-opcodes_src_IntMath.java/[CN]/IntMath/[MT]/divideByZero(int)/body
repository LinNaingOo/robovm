{
  System.out.println("IntMath.divideByZero");
  try {
    int x=100 / z;
    assert(false);
  }
 catch (  ArithmeticException ae) {
  }
  try {
    int x=100 % z;
    assert(false);
  }
 catch (  ArithmeticException ae) {
  }
  try {
    long x=100L / z;
    assert(false);
  }
 catch (  ArithmeticException ae) {
  }
  try {
    long x=100L % z;
    assert(false);
  }
 catch (  ArithmeticException ae) {
  }
}
