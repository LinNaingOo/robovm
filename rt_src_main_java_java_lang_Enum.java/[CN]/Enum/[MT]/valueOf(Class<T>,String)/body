{
  if ((enumType == null) || (name == null)) {
    throw new NullPointerException(Messages.getString("luni.4D"));
  }
  T[] values=getValues(enumType);
  if (values == null) {
    throw new IllegalArgumentException(Messages.getString("luni.4E",enumType));
  }
  for (  T enumConst : values) {
    if (enumConst.name.equals(name)) {
      return enumConst;
    }
  }
  throw new IllegalArgumentException(Messages.getString("luni.4F",name,enumType));
}
