{
  if (Options.v().whole_shimple()) {
    ShimpleTransformer.v().transform();
    getPack("wspp").apply();
    getPack("cg").apply();
    getPack("wstp").apply();
    getPack("wsop").apply();
  }
 else {
    getPack("wjpp").apply();
    getPack("cg").apply();
    getPack("wjtp").apply();
    getPack("wjop").apply();
    getPack("wjap").apply();
  }
  PaddleHook.v().finishPhases();
}
