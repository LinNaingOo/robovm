{
  try {
    if (isNativeConnected) {
      netImpl.recvConnectedDatagram(fd,pack,pack.getData(),pack.getOffset(),pack.getLength(),receiveTimeout,true);
      updatePacketRecvAddress(pack);
    }
 else {
      netImpl.receiveDatagram(fd,pack,pack.getData(),pack.getOffset(),pack.getLength(),receiveTimeout,true);
    }
  }
 catch (  InterruptedIOException e) {
    throw new SocketTimeoutException(e.toString());
  }
  return pack.getPort();
}
