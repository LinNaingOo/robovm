{
  if (isNativeConnected) {
    netImpl.sendConnectedDatagram(fd,packet.getData(),packet.getOffset(),packet.getLength(),bindToDevice);
  }
 else {
    netImpl.sendDatagram(fd,packet.getData(),packet.getOffset(),packet.getLength(),packet.getPort(),bindToDevice,trafficClass,packet.getAddress());
  }
}
