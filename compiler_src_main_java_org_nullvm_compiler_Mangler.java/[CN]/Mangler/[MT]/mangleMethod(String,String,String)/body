{
  StringBuilder sb=new StringBuilder();
  sb.append(mangleString(owner));
  sb.append("_");
  sb.append(mangleString(name));
  if (!desc.startsWith("()")) {
    sb.append("__");
    sb.append(mangleString(desc.substring(1,desc.indexOf(')'))));
  }
  sb.append("__");
  sb.append(mangleString(desc.substring(desc.indexOf(')') + 1)));
  return sb.toString();
}
