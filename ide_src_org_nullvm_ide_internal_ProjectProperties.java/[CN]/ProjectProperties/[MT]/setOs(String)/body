{
  this.os=os;
  if (os == null) {
    osCombo.select(0);
  }
 else   if (os.equals(NullVMPlugin.OS_AUTO)) {
    osCombo.select(1);
  }
 else {
    for (int i=0; i < osCombo.getItemCount(); i++) {
      if (os.equals(osCombo.getItem(i))) {
        osCombo.select(i);
        break;
      }
    }
  }
}
