{
  if (seconds < 0) {
    throw new IllegalArgumentException("seconds < 0");
  }
  timeout=seconds * 1000;
  for (Enumeration<IdKey> en=sessions.keys(); en.hasMoreElements(); ) {
    IdKey key=en.nextElement();
    SSLSessionImpl ses=(sessions.get(key));
    if (!ses.isValid()) {
      sessions.remove(key);
    }
  }
}
