{
  Object o=new Object();
  tVector.set(23,o);
  assertTrue("Failed to set Object",tVector.get(23) == o);
  try {
    tVector.set(-5,"Wrong position");
    fail("ArrayIndexOutOfBoundsException expected");
  }
 catch (  ArrayIndexOutOfBoundsException e) {
  }
  try {
    tVector.set(tVector.size() + 1,"Wrong position");
    fail("ArrayIndexOutOfBoundsException expected");
  }
 catch (  ArrayIndexOutOfBoundsException e) {
  }
}
