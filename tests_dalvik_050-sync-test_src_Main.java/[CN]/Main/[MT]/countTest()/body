{
  CpuThread one, two;
  one=new CpuThread(1);
  two=new CpuThread(2);
  one.start();
  try {
    Thread.sleep(100);
  }
 catch (  InterruptedException ie) {
    System.out.println("INTERRUPT!");
    ie.printStackTrace();
  }
  two.start();
  try {
    one.join();
    two.join();
  }
 catch (  InterruptedException ie) {
    System.out.println("INTERRUPT!");
    ie.printStackTrace();
  }
  System.out.println("main: all done");
}
