{
  System.out.println("IntMath.testFloatCompare");
  if (minus > plus)   assert(false);
  if (plus < minus)   assert(false);
  if (plus == minus)   assert(false);
  if (plus != plus2)   assert(false);
  if (plus <= nan)   assert(false);
  if (plus >= nan)   assert(false);
  if (minus <= nan)   assert(false);
  if (minus >= nan)   assert(false);
  if (nan >= plus)   assert(false);
  if (nan <= plus)   assert(false);
  if (nan == nan)   assert(false);
}
