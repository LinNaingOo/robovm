{
  System.out.println("IntMath.testIntCompare");
  if (minus > plus)   assert(false);
  if (minus >= plus)   assert(false);
  if (plus < minus)   assert(false);
  if (plus <= minus)   assert(false);
  if (plus == minus)   assert(false);
  if (plus != plus2)   assert(false);
  if (plus != minus) {
    assert(true);
  }
 else {
    assert(false);
  }
  if (minus > 0)   assert(false);
  if (minus >= 0)   assert(false);
  if (plus < 0)   assert(false);
  if (plus <= 0)   assert(false);
  if (plus == 0)   assert(false);
  if (zero != 0)   assert(false);
  if (zero == 0) {
    assert(true);
  }
 else {
    assert(false);
  }
}
