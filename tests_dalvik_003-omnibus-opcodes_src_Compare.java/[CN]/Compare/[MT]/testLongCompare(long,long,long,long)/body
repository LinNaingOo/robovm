{
  System.out.println("IntMath.testLongCompare");
  if (minus > plus)   assert(false);
  if (plus < minus)   assert(false);
  if (plus == minus)   assert(false);
  if (plus >= plus + 1)   assert(false);
  if (minus >= minus + 1)   assert(false);
  if (plus != minus) {
    assert(true);
  }
 else {
    assert(false);
  }
  if (plus > alsoPlus)   assert(false);
  if (alsoPlus < plus)   assert(false);
  if (alsoPlus == plus)   assert(false);
  if (minus < alsoMinus)   assert(false);
  if (alsoMinus > minus)   assert(false);
  if (alsoMinus == minus)   assert(false);
}
