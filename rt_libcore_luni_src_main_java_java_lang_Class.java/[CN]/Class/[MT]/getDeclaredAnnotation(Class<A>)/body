{
  if (annotationClass == null) {
    throw new NullPointerException("annotationClass == null");
  }
  Annotation[] annos=getDeclaredAnnotations();
  for (int i=annos.length - 1; i >= 0; --i) {
    if (annos[i].annotationType() == annotationClass) {
      return (A)annos[i];
    }
  }
  return null;
}
