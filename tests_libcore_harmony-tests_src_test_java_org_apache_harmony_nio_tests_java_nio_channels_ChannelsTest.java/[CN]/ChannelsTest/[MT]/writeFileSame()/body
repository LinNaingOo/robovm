{
  this.fouts=new FileOutputStream(tmpFile);
  byte[] bit=new byte[1];
  bit[0]=80;
  this.fouts.write(bit);
  this.fouts.flush();
  String writebuf="";
  for (int val=0; val < this.writebufSize / 2; val++) {
    writebuf=writebuf + ((char)(val + 64));
  }
  this.fouts.write(writebuf.getBytes());
}
