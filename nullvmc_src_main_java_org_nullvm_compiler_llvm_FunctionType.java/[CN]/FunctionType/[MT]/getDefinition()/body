{
  StringBuilder sb=new StringBuilder();
  sb.append(returnType.toString());
  sb.append(" (");
  for (int i=0; i < parameterTypes.length; i++) {
    if (i > 0) {
      sb.append(", ");
    }
    sb.append(parameterTypes[i].toString());
  }
  if (varargs) {
    if (parameterTypes.length > 0) {
      sb.append(", ");
    }
    sb.append("...");
  }
  sb.append(")*");
  return sb.toString();
}
