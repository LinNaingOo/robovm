{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  DataLayout other=(DataLayout)obj;
  if (ref == null) {
    if (other.ref != null) {
      return false;
    }
  }
 else   if (!ref.equals(other.ref)) {
    return false;
  }
  return true;
}
