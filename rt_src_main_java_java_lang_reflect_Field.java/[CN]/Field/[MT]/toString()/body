{
  Class<?> type=getType();
  Class<?> declaringClass=getDeclaringClass();
  String name=getName();
  StringBuilder sb=new StringBuilder(Modifier.toString(getModifiers()));
  if (sb.length() > 0) {
    sb.append(' ');
  }
  sb.append(type.getName());
  sb.append(' ');
  sb.append(declaringClass.getName());
  sb.append('.');
  sb.append(name);
  return sb.toString();
}
