{
  if (archive == null) {
    File a=new File(cacheDir,jarName);
    if (!a.exists() || hasChangedAfter(a.lastModified())) {
      try {
        createArchive(path.getDir(),a,false);
      }
 catch (      IOException e) {
        throw new RuntimeException(e);
      }
    }
    archive=a;
  }
  return archive;
}
