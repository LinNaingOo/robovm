{
  CmdLineParser parser=new CmdLineParser(this);
  try {
    parser.parseArgument(args);
    if (help) {
      printUsageAndExit(parser,null);
    }
    run();
  }
 catch (  Throwable t) {
    if (verbose && !(t instanceof CmdLineException)) {
      t.printStackTrace();
    }
    printUsageAndExit(parser,t.getMessage());
  }
}
