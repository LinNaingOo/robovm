{
  String host=getHost();
  if (host.matches("^(x86.64|amd64).*")) {
    return Arch.x86_64;
  }
  if (host.matches("^(x86|i\\d86).*")) {
    return Arch.i386;
  }
  if (host.matches("^arm.*")) {
    return Arch.arm;
  }
  throw new IllegalArgumentException("Unrecognized arch in Host string: " + host);
}
