{
  for (int i=0; i < nodes.size(); i++) {
    if ((!refsOnly) && isNodeLocal.get(i).booleanValue()) {
      return false;
    }
 else     if (((EquivalentValue)nodes.get(i)).getValue().getType() instanceof RefLikeType && isNodeLocal.get(i).booleanValue()) {
      return false;
    }
  }
  return true;
}
