{
  FastStringBuffer fsb=fsb();
  int to=m_start + toffset;
  int tlim=m_start + m_length;
  int po=0;
  int pc=prefix.length();
  if ((toffset < 0) || (toffset > m_length - pc)) {
    return false;
  }
  while (--pc >= 0) {
    if (fsb.charAt(to) != prefix.charAt(po)) {
      return false;
    }
    to++;
    po++;
  }
  return true;
}
