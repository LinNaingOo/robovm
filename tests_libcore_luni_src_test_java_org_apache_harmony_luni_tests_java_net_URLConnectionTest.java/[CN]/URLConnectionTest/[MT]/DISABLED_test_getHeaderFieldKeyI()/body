{
  String hf;
  boolean foundResponse=false;
  for (int i=0; i < 100; i++) {
    hf=uc.getHeaderFieldKey(i);
    if (hf != null && hf.toLowerCase().equals("content-type")) {
      foundResponse=true;
      break;
    }
  }
  assertTrue("Could not find header field key containing \"content-type\"",foundResponse);
}
