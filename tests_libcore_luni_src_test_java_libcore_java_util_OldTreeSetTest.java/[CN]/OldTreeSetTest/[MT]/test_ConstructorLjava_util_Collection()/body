{
  TreeSet myTreeSet=new TreeSet(Arrays.asList(objArray));
  assertTrue("TreeSet incorrect size",myTreeSet.size() == objArray.length);
  for (int counter=0; counter < objArray.length; counter++)   assertTrue("TreeSet does not contain correct elements",myTreeSet.contains(objArray[counter]));
  HashMap hm=new HashMap();
  hm.put("First",new Integer(1));
  hm.put(new Integer(2),"two");
  try {
    new TreeSet(hm.values());
    fail("ClassCastException expected");
  }
 catch (  ClassCastException e) {
  }
  try {
    new TreeSet((Collection)null);
    fail("NullPointerException expected");
  }
 catch (  NullPointerException e) {
  }
}
