{
  if (closed) {
    throw new IllegalStateException(Messages.getString("luni.33"));
  }
  connect();
  if (jarInput != null) {
    return jarInput;
  }
  if (jarEntry == null) {
    throw new IOException(Messages.getString("luni.34"));
  }
  return jarInput=new JarURLConnectionInputStream(jarFile.getInputStream(jarEntry),jarFile);
}
