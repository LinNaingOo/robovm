{
  if (target_pc >= end_pc || target_pc < pc_offset)   throw new verify.VerificationException("control flow reaches outside of the code");
  int logic_pc=target_pc - pc_offset;
  if (state.pc() != -1)   throw new InternalError("pc mismatch in abstract state");
  state.set_pc(target_pc);
  AbstractState s=findApplicableState(state,target_pc);
  if (s != null) {
    boolean need_recheck=s.merge(ctx,state);
    if (!need_recheck)     return;
    if (!unchecked.contains(s))     unchecked.enqueue(s,pc_offset,log3);
 else     if (log3)     log(" ==> " + logic_pc + "  (already there)");
    return;
  }
  state.next_this_pc=pc_states[logic_pc];
  pc_states[logic_pc]=state;
  unchecked.enqueue(state,pc_offset,log3);
}
