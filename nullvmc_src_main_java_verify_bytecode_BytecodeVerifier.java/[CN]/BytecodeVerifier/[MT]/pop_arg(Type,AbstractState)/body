{
  if (argtype.tag == T_DOUBLE) {
    state.pop(SC_DOUBLE,SC_DOUBLE2);
    return ctx.DOUBLE;
  }
 else   if (argtype.tag == T_LONG) {
    state.pop(SC_LONG,SC_LONG2);
    return ctx.LONG;
  }
 else {
    Type valueType=state.pop(argtype.storageClass);
    argtype.checkAssignmentFrom(valueType);
    return valueType;
  }
}
