{
  int argLenOfStep=getArgLengthOfStep(opPosOfStep);
  if (argLenOfStep == 3) {
    int index=m_opMap.elementAt(opPosOfStep + 4);
    if (index >= 0)     return (String)m_tokenQueue.elementAt(index);
 else     if (OpCodes.ELEMWILDCARD == index)     return NodeTest.WILD;
 else     return null;
  }
 else   return null;
}
