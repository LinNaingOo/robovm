{
  if (!super.deepEquals(expr))   return false;
  if (null != m_arg0) {
    if (null == ((FunctionOneArg)expr).m_arg0)     return false;
    if (!m_arg0.deepEquals(((FunctionOneArg)expr).m_arg0))     return false;
  }
 else   if (null != ((FunctionOneArg)expr).m_arg0)   return false;
  return true;
}
