{
  try {
    Constructor<T> constructor=getDeclaredConstructor();
    return constructor.newInstance();
  }
 catch (  NoSuchMethodException e) {
    e.printStackTrace();
    throw new InstantiationException(e.getMessage());
  }
catch (  InvocationTargetException e) {
    e.printStackTrace();
    throw new InstantiationException(e.getMessage());
  }
}
