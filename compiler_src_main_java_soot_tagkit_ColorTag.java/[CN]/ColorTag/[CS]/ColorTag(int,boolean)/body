{
switch (color) {
case RED:
{
      red=255;
      green=0;
      blue=0;
      break;
    }
case GREEN:
{
    red=45;
    green=255;
    blue=84;
    break;
  }
case YELLOW:
{
  red=255;
  green=248;
  blue=35;
  break;
}
case BLUE:
{
red=174;
green=210;
blue=255;
break;
}
case ORANGE:
{
red=255;
green=163;
blue=0;
break;
}
case PURPLE:
{
red=159;
green=34;
blue=193;
break;
}
default :
{
red=220;
green=220;
blue=220;
break;
}
}
foreground=fg;
}
