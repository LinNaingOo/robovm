{
  this.arch=arch;
  if (arch == null) {
    archCombo.select(0);
  }
 else   if (arch.equals(RoboVMPlugin.ARCH_AUTO)) {
    archCombo.select(1);
  }
 else {
    for (int i=0; i < archCombo.getItemCount(); i++) {
      if (arch.equals(archCombo.getItem(i))) {
        archCombo.select(i);
        break;
      }
    }
  }
}
