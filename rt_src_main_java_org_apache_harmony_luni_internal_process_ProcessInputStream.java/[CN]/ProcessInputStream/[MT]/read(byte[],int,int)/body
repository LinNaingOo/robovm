{
synchronized (this) {
    if (handle == -1) {
      return -1;
    }
    if (nbytes > buffer.length || nbytes < 0) {
      throw new ArrayIndexOutOfBoundsException(Messages.getString("luni.18",nbytes));
    }
    if (offset < 0 || offset > buffer.length - nbytes) {
      throw new ArrayIndexOutOfBoundsException(Messages.getString("luni.12",offset));
    }
    return readImpl(buffer,offset,nbytes,handle);
  }
}
