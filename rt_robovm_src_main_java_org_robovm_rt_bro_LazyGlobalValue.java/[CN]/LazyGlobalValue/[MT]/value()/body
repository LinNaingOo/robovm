{
  try {
    if (!constant) {
      return (T)getter.invoke(null);
    }
    if (cachedValue == NO_VALUE) {
      cachedValue=(T)getter.invoke(null);
    }
    return cachedValue;
  }
 catch (  Error|RuntimeException e) {
    throw e;
  }
catch (  IllegalAccessException|InvocationTargetException e) {
    throw new Error(e);
  }
}
