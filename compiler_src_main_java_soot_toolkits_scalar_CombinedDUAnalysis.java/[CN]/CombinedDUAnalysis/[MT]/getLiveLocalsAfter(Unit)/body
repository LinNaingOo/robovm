{
  List ret=liveLocalsAfter.get(u);
  if (ret == null) {
    HashSet hs=new HashSet();
    for (Iterator vbIt=((FlowSet)getFlowAfter(u)).iterator(); vbIt.hasNext(); ) {
      final ValueBox vb=(ValueBox)vbIt.next();
      hs.add(vb.getValue());
    }
    liveLocalsAfter.put(u,ret=new ArrayList(hs));
  }
  return ret;
}
