{
switch (class0) {
case SC_INT:
    return "integer";
case SC_FLOAT:
  return "float";
case SC_DOUBLE:
return "double/1";
case SC_DOUBLE2:
return "double/2";
case SC_LONG:
return "long/1";
case SC_LONG2:
return "long/2";
case SC_NEW:
return "new object";
case SC_ADDR:
return "reference";
default :
throw new InternalError("no such storage class " + class0);
}
}
