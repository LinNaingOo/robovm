{
  assertFalse(buf.hasArray());
  try {
    buf.array();
    fail();
  }
 catch (  ReadOnlyBufferException expected) {
  }
catch (  UnsupportedOperationException expected) {
  }
  try {
    buf.arrayOffset();
    fail();
  }
 catch (  ReadOnlyBufferException expected) {
  }
catch (  UnsupportedOperationException expected) {
  }
}
