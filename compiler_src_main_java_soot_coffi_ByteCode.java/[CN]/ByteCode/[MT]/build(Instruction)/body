{
  Instruction i;
  i=insts;
  icount=0;
  while (i != null) {
    icount++;
    i=i.next;
  }
  if (icount > 0) {
    instructions=new Instruction[icount];
    int k;
    k=0;
    i=insts;
    while (i != null) {
      instructions[k]=i;
      k++;
      i=i.next;
    }
    i=insts;
    while (i != null) {
      i.offsetToPointer(this);
      i=i.next;
    }
  }
}
