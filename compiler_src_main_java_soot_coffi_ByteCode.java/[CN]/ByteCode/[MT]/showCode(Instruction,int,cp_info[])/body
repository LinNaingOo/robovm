{
  int i;
  Instruction j=inst;
  String pref;
  i=startinst;
  while (j != null) {
    if (i > 999)     pref="";
 else     if (i > 99)     pref=" ";
 else     if (i > 9)     pref="  ";
 else     pref="   ";
    G.v().out.print(pref + i + ": ");
    G.v().out.println(j.toString(constant_pool));
    i=j.nextOffset(i);
    j=j.next;
  }
}
