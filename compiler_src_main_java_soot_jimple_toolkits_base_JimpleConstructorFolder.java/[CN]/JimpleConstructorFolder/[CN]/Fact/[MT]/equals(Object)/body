{
  if (!(other instanceof Fact))   return false;
  Fact o=(Fact)other;
  if (!stmtToVar.equals(o.stmtToVar))   return false;
  if (alloc == null && o.alloc != null)   return false;
  if (alloc != null && o.alloc == null)   return false;
  if (alloc != null && !alloc.equals(o.alloc))   return false;
  return true;
}
