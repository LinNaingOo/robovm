{
  ListIterator<String> colNameIt=colNames.listIterator();
  for (  String t : values) {
    assertEquals(t,res.getString(colNameIt.next()));
  }
  res.next();
  for (  String name : colNames) {
    assertNull(res.getString(name));
  }
  try {
    res.getString("bla");
    fail("Exception expected");
  }
 catch (  SQLException e) {
  }
}
