{
  for (int i=1; i <= typeMap.length; i++) {
    if (supported[i - 1]) {
      Object value=res.getObject(i);
      assertTrue("value " + value.getClass().getName() + " does not correspond "+ typeMap[i - 1]+ "at "+ i,value.getClass().equals(typeMap[i - 1]));
    }
  }
  try {
    res.getObject(500);
    fail("Exception expected");
  }
 catch (  SQLException e) {
  }
  res.next();
  for (int i=1; i <= typeMap.length; i++) {
    Object value=res.getObject(i);
    assertNull(value);
  }
}
