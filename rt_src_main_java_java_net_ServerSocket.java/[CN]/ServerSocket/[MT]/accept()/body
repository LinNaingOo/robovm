{
  checkClosedAndCreate(false);
  if (!isBound()) {
    throw new SocketException(Messages.getString("luni.6F"));
  }
  Socket aSocket=new Socket();
  try {
    implAccept(aSocket);
  }
 catch (  SecurityException e) {
    aSocket.close();
    throw e;
  }
catch (  IOException e) {
    aSocket.close();
    throw e;
  }
  return aSocket;
}
