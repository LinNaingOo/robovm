{
  try {
    Method mth=TestMethod.class.getDeclaredMethod("invokeExceptionTest",new Class[0]);
    Object[] args={Object.class};
    Object ret=mth.invoke(new TestMethod(),new Object[0]);
  }
 catch (  InvocationTargetException e) {
    assertTrue("Returned incorrect cause",e.getCause() instanceof NullPointerException);
    return;
  }
catch (  Exception e) {
    fail("Exception during InvocationTargetException test : " + e.getMessage());
  }
  fail("Failed to throw exception");
}
