{
  assertEquals(expected.length,actualCB.length());
  for (int i=0; i < actualCB.length(); ++i) {
    char actual=actualCB.get();
    if (actual != expected[i]) {
      String detail=String.format("Mismatch at index %d: %d instead of expected %d.\n",i,(int)actual,(int)expected[i]);
      fail(detail);
    }
  }
}
