{
  boolean match=true;
  boolean lenMatch=true;
  byte actual;
  int len=actualBB.remaining();
  if (expected.length != len) {
    lenMatch=false;
    if (expected.length < len)     len=expected.length;
  }
  for (int i=0; i < len; i++) {
    actual=actualBB.get();
    if (actual != expected[i]) {
      String detail=String.format("Mismatch at index %d: %d instead of expected %d.\n",i,actual & 0xff,expected[i] & 0xff);
      match=false;
      fail(msg + ": " + detail);
    }
  }
  assertTrue(msg,match);
  assertTrue(msg + "(IN LENGTH ALSO!)",lenMatch);
}
