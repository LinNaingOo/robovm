{
  if (sp < 1)   throw new verify.VerificationException("stack underflow, missing argument!");
  Type[] wstate=storage;
  if (wstate[sp - 1].storageClass != type0.storageClass)   throw new verify.VerificationException("erroneous argument " + (wstate[sp - 1]) + " to "+ type0+ " op");
  wstate[sp - 1]=type0;
}
