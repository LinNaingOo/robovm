{
  Type[] wstate=storage;
  if (sp < 2)   throw new verify.VerificationException("stack underflow in " + type0 + " binop");
  if (wstate[sp - 1].storageClass != type0.storageClass || wstate[sp - 2].storageClass != type0.storageClass) {
    throw new verify.VerificationException("erroneous arguments (" + wstate[sp - 2] + ", "+ wstate[sp - 1]+ ") to "+ type0+ "-only binop");
  }
  sp-=1;
  wstate[sp - 1]=type0;
}
