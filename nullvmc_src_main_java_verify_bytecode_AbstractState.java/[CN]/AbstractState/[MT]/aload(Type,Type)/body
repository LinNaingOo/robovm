{
  pop(SC_INT);
  Type array=pop(SC_ADDR);
  Type elem=elemType;
  if (!array.isAnonymousArray()) {
    if (array.tag != T_ARRAY)     throw new verify.VerificationException("non-array operand to aaload insn");
    elem=((ArrayType)array).elementType;
    if (elemType.storageClass == elem.storageClass)     ;
 else     if (elem != elemType)     throw new verify.VerificationException("" + elem + "[] found where "+ elemType+ "[] was expected");
  }
  if (extra != null)   push(elem,extra);
 else   push(elem);
}
