{
  if (sp < 2)   throw new verify.VerificationException("stack underflow, missing argument!");
  Type[] wstate=storage;
  if (wstate[sp - 2].storageClass != type0.storageClass || wstate[sp - 1].storageClass != type1.storageClass)   throw new verify.VerificationException("erroneous argument " + (wstate[sp - 2]) + " to "+ type0+ " op");
  wstate[sp - 2]=type0;
  wstate[sp - 1]=type1;
}
