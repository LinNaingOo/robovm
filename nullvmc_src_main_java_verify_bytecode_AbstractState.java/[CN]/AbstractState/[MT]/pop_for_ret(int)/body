{
  AbstractCallStack next;
  int i=0;
  do {
    next=jsrStack;
    if (next == null)     throw new verify.VerificationException("jsr/ret callstack mismatch.  A ret operation is " + "performed, for a register which was initialized " + "in a different jsr calling sequence. ");
    i+=1;
    jsrStack=jsrStack.next;
  }
 while (next.return_pc != return_pc);
  if (i > 1)   System.out.println("@@@@ interesting, multi level jsr @@@@");
}
