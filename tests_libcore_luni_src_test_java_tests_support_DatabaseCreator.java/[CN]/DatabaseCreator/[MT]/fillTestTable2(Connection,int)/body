{
  Statement statement=conn.createStatement();
  for (int id=0; id < numberOfRecords; id++) {
    double value=id + DatabaseCreator.defaultDouble;
    String insertQuery="INSERT INTO " + DatabaseCreator.TEST_TABLE2 + " (finteger, ftext, fcharacter, fdecimal, fnumeric,"+ " fsmallint, ffloat, freal, fdouble)"+ " VALUES ("+ id+ ", '"+ DatabaseCreator.defaultString+ id+ "',"+ " '"+ DatabaseCreator.defaultCharacter+ id+ "', "+ value+ ", "+ value+ ","+ value+ ", "+ value+ ", "+ value+ ","+ value+ ")";
    statement.execute(insertQuery);
  }
}
