{
  assertEquals(0,memory.order(ByteOrder.nativeOrder()).asIntBuffer().get(0));
  try {
    valuedEnumGetter();
    fail("IllegalArgumentException expected");
  }
 catch (  IllegalArgumentException e) {
  }
  valuedEnumSetter(TestValuedEnum.V100);
