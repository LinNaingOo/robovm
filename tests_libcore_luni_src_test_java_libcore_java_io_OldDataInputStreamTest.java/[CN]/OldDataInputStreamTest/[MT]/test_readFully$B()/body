{
  byte rbytes[]=new byte[testLength];
  os.write(fileString.getBytes());
  os.close();
  openDataInputStream();
  dis.readFully(rbytes);
  assertTrue("Test 1: Incorrect data written or read.",new String(rbytes,0,testLength).equals(fileString));
  dis.close();
  try {
    dis.readFully(rbytes);
    fail("Test 2: IOException expected.");
  }
 catch (  IOException e) {
  }
  openDataInputStream();
  dis.readByte();
  try {
    dis.readFully(rbytes);
    fail("Test 3: EOFException expected.");
  }
 catch (  EOFException e) {
  }
}
