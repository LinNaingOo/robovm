{
  os.writeByte((byte)-127);
  os.close();
  openDataInputStream();
  assertEquals("Test 1: Incorrect byte written or read;",129,dis.readUnsignedByte());
  try {
    dis.readUnsignedByte();
    fail("Test 2: EOFException expected.");
  }
 catch (  EOFException e) {
  }
  dis.close();
  try {
    dis.readUnsignedByte();
    fail("Test 3: IOException expected.");
  }
 catch (  IOException e) {
  }
}
