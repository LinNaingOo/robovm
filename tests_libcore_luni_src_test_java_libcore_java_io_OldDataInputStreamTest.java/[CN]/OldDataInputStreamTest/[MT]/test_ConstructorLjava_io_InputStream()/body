{
  try {
    os.writeChar('t');
    os.close();
    openDataInputStream();
  }
 catch (  IOException e) {
    fail("IOException during constructor test : " + e.getMessage());
  }
 finally {
    try {
      dis.close();
    }
 catch (    IOException e) {
      fail("IOException during constructor test : " + e.getMessage());
    }
  }
}
