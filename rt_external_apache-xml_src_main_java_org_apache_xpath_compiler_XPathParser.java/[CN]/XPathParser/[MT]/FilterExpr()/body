{
  int opPos=m_ops.getOp(OpMap.MAPINDEX_LENGTH);
  int filterMatch;
  if (PrimaryExpr()) {
    if (tokenIs('[')) {
      insertOp(opPos,2,OpCodes.OP_LOCATIONPATH);
      while (tokenIs('[')) {
        Predicate();
      }
      filterMatch=FILTER_MATCH_PREDICATES;
    }
 else {
      filterMatch=FILTER_MATCH_PRIMARY;
    }
  }
 else {
    filterMatch=FILTER_MATCH_FAILED;
  }
  return filterMatch;
}
