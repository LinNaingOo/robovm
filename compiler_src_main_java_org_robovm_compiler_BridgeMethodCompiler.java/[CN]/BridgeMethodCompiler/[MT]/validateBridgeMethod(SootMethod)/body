{
  if (!canMarshal(method)) {
    throw new IllegalArgumentException("No @Marshaler for return type of" + " @Bridge annotated method " + method.getName() + " found");
  }
  for (int i=0; i < method.getParameterCount(); i++) {
    if (!canMarshal(method,i)) {
      throw new IllegalArgumentException("No @Marshaler for parameter " + (i + 1) + " of @Bridge annotated method "+ method.getName()+ " found");
    }
  }
}
