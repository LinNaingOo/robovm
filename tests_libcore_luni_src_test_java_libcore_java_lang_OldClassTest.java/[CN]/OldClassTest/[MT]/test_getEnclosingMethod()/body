{
  Method clazz=ExtendTestClass.class.getEnclosingMethod();
  assertNull(clazz);
  PublicTestClass ptc=new PublicTestClass();
  try {
    assertEquals("getEnclosingMethod returns incorrect method.",PublicTestClass.class.getMethod("getLocalClass",(Class[])null),ptc.getLocalClass().getClass().getEnclosingMethod());
  }
 catch (  NoSuchMethodException nsme) {
    fail("NoSuchMethodException was thrown.");
  }
}
