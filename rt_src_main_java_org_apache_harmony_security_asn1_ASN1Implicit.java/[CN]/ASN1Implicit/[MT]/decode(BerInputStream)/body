{
  if (!checkTag(in.tag)) {
    throw new ASN1Exception(Messages.getString("security.100",new Object[]{in.tagOffset,Integer.toHexString(id),Integer.toHexString(in.tag)}));
  }
  if (id == in.tag) {
    in.tag=type.id;
  }
 else {
    in.tag=type.constrId;
  }
  in.content=type.decode(in);
  if (in.isVerify) {
    return null;
  }
  return getDecodedObject(in);
}
