{
  Process p;
  try {
    ProcessBuilder pb=new ProcessBuilder("cat","/dev/random");
    p=pb.start();
  }
 catch (  IOException ioe) {
    System.err.println("cmd failed: " + ioe.getMessage());
    p=null;
  }
  try {
    testDexClassLoader();
  }
  finally {
    if (p != null)     p.destroy();
    try {
      Thread.sleep(500);
    }
 catch (    Exception ex) {
    }
  }
  System.out.println("done");
}
