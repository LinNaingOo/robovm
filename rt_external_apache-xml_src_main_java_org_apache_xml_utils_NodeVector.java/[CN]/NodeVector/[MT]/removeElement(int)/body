{
  if (null == m_map)   return false;
  for (int i=0; i < m_firstFree; i++) {
    int node=m_map[i];
    if (node == s) {
      if (i > m_firstFree)       System.arraycopy(m_map,i + 1,m_map,i - 1,m_firstFree - i);
 else       m_map[i]=DTM.NULL;
      m_firstFree--;
      return true;
    }
  }
  return false;
}
