{
  if (c == '\u01c5' || c == '\u01c8' || c == '\u01cb' || c == '\u01f2') {
    return true;
  }
  if (c >= '\u1f88' && c <= '\u1ffc') {
    if (c > '\u1faf') {
      return c == '\u1fbc' || c == '\u1fcc' || c == '\u1ffc';
    }
    int last=c & 0xf;
    return last >= 8 && last <= 0xf;
  }
  return false;
}
