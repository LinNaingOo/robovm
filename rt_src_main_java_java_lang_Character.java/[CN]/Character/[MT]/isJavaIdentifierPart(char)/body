{
  if (c < 128) {
    return (typeTags[c] & ISJAVAPART) != 0;
  }
  int type=getType(c);
  return (type >= UPPERCASE_LETTER && type <= OTHER_LETTER) || type == CURRENCY_SYMBOL || type == CONNECTOR_PUNCTUATION || (type >= DECIMAL_DIGIT_NUMBER && type <= LETTER_NUMBER) || type == NON_SPACING_MARK || type == COMBINING_SPACING_MARK || (c >= 0x80 && c <= 0x9f) || type == FORMAT;
}
