{
  if (this == other)   return;
  long[] otherBits=other.bits;
  int j;
  for (j=otherBits.length - 1; j >= 0; j--) {
    if (otherBits[j] != 0L)     break;
  }
  expand(j << 6);
  int i=j + 1;
  for (; j >= 0; j--) {
    bits[j]=otherBits[j];
  }
  for (; i < bits.length; i++) {
    bits[i]=0L;
  }
}
