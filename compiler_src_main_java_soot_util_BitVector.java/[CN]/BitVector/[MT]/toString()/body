{
  StringBuffer ret=new StringBuffer();
  ret.append('{');
  boolean start=true;
  BitSetIterator it=new BitSetIterator(bits);
  while (it.hasNext()) {
    int bit=it.next();
    if (!start)     ret.append(", ");
    start=false;
    ret.append(bit);
  }
  ret.append('}');
  return ret.toString();
}
