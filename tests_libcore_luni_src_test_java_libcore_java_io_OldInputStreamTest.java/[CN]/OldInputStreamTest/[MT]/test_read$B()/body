{
  byte[] b=new byte[10];
  byte[] ref=testString.getBytes();
  boolean equal=true;
  int bytesRead=0;
  int i;
  assertEquals("Test 1: Incorrect count of bytes read.",is.read(b),10);
  for (i=0; i < 10; i++) {
    equal&=(b[i] == ref[i]);
  }
  assertTrue("Test 1: Wrong bytes read.",equal);
  b=new byte[ref.length];
  bytesRead=is.read(b);
  assertEquals("Test 2: Incorrect count of bytes read.",bytesRead,ref.length - 10);
  for (i=0; i < bytesRead; i++) {
    equal&=(b[i] == ref[i + 10]);
  }
  assertTrue("Test 2: Wrong bytes read.",equal);
  bytesRead=is.read(b);
  assertEquals("Test 3:",bytesRead,-1);
}
