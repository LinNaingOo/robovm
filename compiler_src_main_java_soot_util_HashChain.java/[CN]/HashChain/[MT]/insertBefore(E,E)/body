{
  if (toInsert == null)   throw new RuntimeException("Bad idea! You tried to insert " + "a null object into a Chain!");
  if (map.containsKey(toInsert))   throw new RuntimeException("Chain already contains object.");
  Link temp=map.get(point);
  if (temp == null) {
    throw new RuntimeException("Insertion point not found in chain!");
  }
  stateCount++;
  Link newLink=temp.insertBefore(toInsert);
  map.put(toInsert,newLink);
  objectIndexes=null;
}
