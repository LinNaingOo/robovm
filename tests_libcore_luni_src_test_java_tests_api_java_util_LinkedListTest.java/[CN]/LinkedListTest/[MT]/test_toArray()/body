{
  ll.add(null);
  Object[] obj=ll.toArray();
  assertEquals("Returned array of incorrect size",objArray.length + 1,obj.length);
  for (int i=0; i < obj.length - 1; i++)   assertTrue("Returned incorrect array: " + i,obj[i] == objArray[i]);
  assertNull("Returned incorrect array--end isn't null",obj[obj.length - 1]);
}
