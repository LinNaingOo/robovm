{
  ll.add(new Integer(99));
  assertEquals("Returned incorrect index",100,ll.lastIndexOf(objArray[99]));
  assertEquals("Returned index for invalid Object",-1,ll.lastIndexOf(new Object()));
  ll.add(20,null);
  ll.add(24,null);
  assertTrue("Last index of null should be 20, but got: " + ll.lastIndexOf(null),ll.lastIndexOf(null) == 24);
}
