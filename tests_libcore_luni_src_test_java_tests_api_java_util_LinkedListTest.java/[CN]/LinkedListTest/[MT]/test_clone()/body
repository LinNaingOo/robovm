{
  Object x=ll.clone();
  assertTrue("Cloned list was inequal to cloned",x.equals(ll));
  for (int i=0; i < ll.size(); i++)   assertTrue("Cloned list contains incorrect elements",ll.get(i).equals(((LinkedList)x).get(i)));
  ll.addFirst(null);
  x=ll.clone();
  assertTrue("List with a null did not clone properly",ll.equals(x));
}
