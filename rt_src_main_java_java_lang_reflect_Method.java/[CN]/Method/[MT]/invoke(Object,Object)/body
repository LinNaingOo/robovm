{
  if (!Modifier.isStatic(getModifiers())) {
    if (receiver == null) {
      throw new NullPointerException();
    }
    if (!getDeclaringClass().isInstance(receiver)) {
      throw new IllegalArgumentException("receiver is not an instance " + "of this method's declaring class");
    }
  }
  if (args == null) {
    args=EmptyArray.OBJECT;
  }
  Class<?>[] pTypes=getParameterTypes(false);
  if (args.length != pTypes.length) {
    throw new IllegalArgumentException("wrong number of arguments");
  }
  if (!flag) {
    if (!checkAccessibleFast(this)) {
      Class<?> caller=VM.getStackClasses(0,1)[0];
      if (!checkAccessible(caller,this)) {
        throw new IllegalAccessException(String.format("Attempt to access method %s.%s(%s) from class %s",getDeclaringClass().getName(),getName(),toString(parameterTypes),caller.getName()));
      }
    }
  }
  return internalInvoke(method,pTypes,receiver,args);
}
