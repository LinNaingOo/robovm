{
  StringBuilder sb=new StringBuilder();
  sb.append("!{");
  for (int i=0; i < values.length; i++) {
    if (i > 0) {
      sb.append(", ");
    }
    if (values[i] == null) {
      sb.append("null");
    }
 else {
      sb.append(values[i].getType());
      sb.append(' ');
      sb.append(values[i]);
    }
  }
  sb.append('}');
  return sb.toString();
}
