{
  int expectedPos=0;
  int pos=it.first();
  assertEquals(expectedPos,pos);
  while (pos != BreakIterator.DONE) {
    expectedPos+=5;
    pos=it.next();
    assertEquals(expectedPos,pos);
    expectedPos+=1;
    if (expectedPos > s.length()) {
      expectedPos=BreakIterator.DONE;
    }
    pos=it.next();
    assertEquals(expectedPos,pos);
  }
}
