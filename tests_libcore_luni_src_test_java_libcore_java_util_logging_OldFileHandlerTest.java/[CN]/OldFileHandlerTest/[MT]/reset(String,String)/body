{
  File file;
  try {
    file=new File(homepath + SEP + filename);
    if (file.isFile()) {
      file.delete();
    }
 else     if (file.isDirectory()) {
      File[] files=file.listFiles();
      for (int i=0; i < files.length; i++) {
        files[i].delete();
      }
      file.delete();
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  try {
    file=new File(homepath + SEP + filename+ ".lck");
    file.delete();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
