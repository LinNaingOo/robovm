{
  if (trustAnchors.isEmpty()) {
    throw new InvalidAlgorithmParameterException(Messages.getString("security.6D"));
  }
  for (Iterator i=trustAnchors.iterator(); i.hasNext(); ) {
    if (!(i.next() instanceof TrustAnchor)) {
      throw new ClassCastException(Messages.getString("security.6E"));
    }
  }
}
