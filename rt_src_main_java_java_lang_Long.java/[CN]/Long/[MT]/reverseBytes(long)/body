{
  long b7=lng >>> 56;
  long b6=(lng >>> 40) & 0xFF00L;
  long b5=(lng >>> 24) & 0xFF0000L;
  long b4=(lng >>> 8) & 0xFF000000L;
  long b3=(lng & 0xFF000000L) << 8;
  long b2=(lng & 0xFF0000L) << 24;
  long b1=(lng & 0xFF00L) << 40;
  long b0=lng << 56;
  return (b0 | b1 | b2| b3| b4| b5| b6| b7);
}
