{
  if (c == null)   throw new NullPointerException();
  if (c == this)   throw new IllegalArgumentException();
  if (maxElements <= 0)   return 0;
  final ReentrantLock lock=this.lock;
  lock.lock();
  try {
    int n=Math.min(size,maxElements);
    for (int i=0; i < n; i++) {
      c.add((E)queue[0]);
      dequeue();
    }
    return n;
  }
  finally {
    lock.unlock();
  }
}
