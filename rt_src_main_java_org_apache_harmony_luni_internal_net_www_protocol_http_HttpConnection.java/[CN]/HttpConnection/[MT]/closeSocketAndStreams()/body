{
  if (usingSecureSocket) {
    if (null != sslOutputStream) {
      OutputStream temp=sslOutputStream;
      sslOutputStream=null;
      try {
        temp.close();
      }
 catch (      Exception ex) {
      }
    }
    if (null != sslInputStream) {
      InputStream temp=sslInputStream;
      sslInputStream=null;
      try {
        temp.close();
      }
 catch (      Exception ex) {
      }
    }
    if (null != sslSocket) {
      Socket temp=sslSocket;
      sslSocket=null;
      try {
        temp.close();
      }
 catch (      Exception ex) {
      }
    }
  }
  if (null != outputStream) {
    OutputStream temp=outputStream;
    outputStream=null;
    try {
      temp.close();
    }
 catch (    Exception ex) {
    }
  }
  if (null != inputStream) {
    InputStream temp=inputStream;
    inputStream=null;
    try {
      temp.close();
    }
 catch (    Exception ex) {
    }
  }
  if (null != socket) {
    Socket temp=socket;
    socket=null;
    try {
      temp.close();
    }
 catch (    Exception ex) {
    }
  }
}
