{
  assertFeature("XMLVersion",null);
  assertFeature("XMLVersion","");
  assertFeature("XMLVersion","1.0");
  assertFeature("XMLVersion","1.1");
  assertFeature("XMLVERSION","1.1");
  assertFeature("+XMLVersion","1.1");
  assertNoFeature("XMLVersion","1.2");
  assertNoFeature("XMLVersion","2.0");
  assertNoFeature("XMLVersion","2.0");
}
