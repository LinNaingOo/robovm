{
  if (choices == null || choices.length == 0) {
    throw new IllegalArgumentException(Messages.getString("auth.1C"));
  }
  for (int i=0; i < choices.length; i++) {
    if (choices[i] == null || choices[i].length() == 0) {
      throw new IllegalArgumentException(Messages.getString("auth.1C"));
    }
  }
  this.choices=choices;
}
