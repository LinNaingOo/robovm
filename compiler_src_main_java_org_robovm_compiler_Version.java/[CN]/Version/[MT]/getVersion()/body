{
  InputStream is=null;
  try {
    is=Version.class.getResourceAsStream("/version.properties");
    Properties props=new Properties();
    props.load(is);
    return props.getProperty("version");
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
 finally {
    IOUtils.closeQuietly(is);
  }
}
