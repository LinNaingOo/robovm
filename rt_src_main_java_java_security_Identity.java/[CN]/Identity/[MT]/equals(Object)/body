{
  if (this == obj) {
    return true;
  }
  if (!(obj instanceof Identity)) {
    return false;
  }
  Identity i=(Identity)obj;
  if ((name == i.name || (name != null && name.equals(i.name))) && (scope == i.scope || (scope != null && scope.equals(i.scope)))) {
    return true;
  }
  return identityEquals(i);
}
