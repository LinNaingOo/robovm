{
  Iterator it=signerInfos.iterator();
  while (it.hasNext()) {
    SignerInformation signer=(SignerInformation)it.next();
    SignerId sid=signer.getSID();
    List list=(ArrayList)table.get(sid);
    if (list == null) {
      list=new ArrayList(1);
      table.put(sid,list);
    }
    list.add(signer);
  }
  this.all=new ArrayList(signerInfos);
}
