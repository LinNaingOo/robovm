{
  String proxyName;
  InetSocketAddress addr=(InetSocketAddress)proxy.address();
  proxyName=addr.getHostName();
  if (null == proxyName) {
    proxyName=addr.getAddress().getHostAddress();
  }
  InetAddress anAddr=netImpl.getHostByName(proxyName,NetUtil.preferIPv6Addresses());
  return anAddr;
}
