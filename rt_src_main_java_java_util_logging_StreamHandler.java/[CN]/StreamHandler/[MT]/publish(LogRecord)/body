{
  try {
    if (this.isLoggable(record)) {
      if (this.writerNotInitialized) {
        initializeWritter();
      }
      String msg=null;
      try {
        msg=getFormatter().format(record);
      }
 catch (      Exception e) {
        getErrorManager().error(Messages.getString("logging.17"),e,ErrorManager.FORMAT_FAILURE);
      }
      write(msg);
    }
  }
 catch (  Exception e) {
    getErrorManager().error(Messages.getString("logging.18"),e,ErrorManager.GENERIC_FAILURE);
  }
}
