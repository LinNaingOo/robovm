{
  if (strIndex != matchResult.getRightBound() && ((strIndex == 0 || (matchResult.hasAnchoringBounds() && strIndex == matchResult.getLeftBound())) || lt.isAfterLineTerminator(testString.charAt(strIndex - 1),testString.charAt(strIndex)))) {
    return next.matches(strIndex,testString,matchResult);
  }
  return -1;
}
