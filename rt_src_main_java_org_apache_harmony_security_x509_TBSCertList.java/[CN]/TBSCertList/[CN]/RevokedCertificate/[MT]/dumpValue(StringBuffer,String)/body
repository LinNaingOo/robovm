{
  buffer.append(prefix).append("Certificate Serial Number: ").append(userCertificate).append('\n');
  buffer.append(prefix).append("Revocation Date: ").append(revocationDate);
  if (crlEntryExtensions != null) {
    buffer.append('\n').append(prefix).append("CRL Entry Extensions: [");
    crlEntryExtensions.dumpValue(buffer,prefix + "  ");
    buffer.append(prefix).append(']');
  }
}
