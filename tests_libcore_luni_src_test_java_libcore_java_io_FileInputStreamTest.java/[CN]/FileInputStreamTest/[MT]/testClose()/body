{
  File tmp=makeFile();
  FileInputStream fis=new FileInputStream(tmp);
  fis.close();
  fis.close();
  try {
    fis.available();
    fail();
  }
 catch (  IOException expected) {
  }
  try {
    fis.read();
    fail();
  }
 catch (  IOException expected) {
  }
  try {
    fis.read(new byte[1],0,1);
    fail();
  }
 catch (  IOException expected) {
  }
  try {
    fis.skip(1);
    fail();
  }
 catch (  IOException expected) {
  }
  try {
    fis.skip(0);
    fail();
  }
 catch (  IOException expected) {
  }
  fis.read(new byte[0],0,0);
}
