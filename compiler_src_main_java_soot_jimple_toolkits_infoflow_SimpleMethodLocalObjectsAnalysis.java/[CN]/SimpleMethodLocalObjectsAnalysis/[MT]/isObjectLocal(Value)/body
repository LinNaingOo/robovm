{
  EquivalentValue source=new CachedEquivalentValue(new AbstractDataSource(new String("SHARED")));
  if (infoFlowGraph.containsNode(source)) {
    List sinks=infoFlowGraph.getSuccsOf(source);
    if (printMessages)     G.v().out.println("      Requested value " + local + " is "+ (!sinks.contains(new CachedEquivalentValue(local)) ? "Local" : "Shared")+ " in "+ sm+ " ");
    return !sinks.contains(new CachedEquivalentValue(local));
  }
 else {
    if (printMessages)     G.v().out.println("      Requested value " + local + " is Local (LIKE ALL VALUES) in "+ sm+ " ");
    return true;
  }
}
