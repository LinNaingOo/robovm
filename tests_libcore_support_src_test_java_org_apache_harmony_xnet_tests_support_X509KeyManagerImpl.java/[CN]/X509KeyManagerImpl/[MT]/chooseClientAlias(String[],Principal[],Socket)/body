{
  String alias=null;
  if (as == null || as.length == 0) {
    return null;
  }
  try {
    if (as.length == 1 && as[0].equals(client)) {
      if (socket == null) {
        alias=keyTest.getCertificateAlias(cert[0]);
      }
 else {
        alias=keyTest.getCertificateAlias(cert[1]);
      }
      return alias;
    }
  }
 catch (  Exception ex) {
    throw new IllegalArgumentException(ex.getMessage());
  }
  return null;
}
