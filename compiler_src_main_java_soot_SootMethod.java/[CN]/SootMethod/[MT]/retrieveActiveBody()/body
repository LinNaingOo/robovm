{
  declaringClass.checkLevel(SootClass.BODIES);
  if (declaringClass.isPhantomClass())   throw new RuntimeException("cannot get resident body for phantom class : " + getSignature() + "; maybe you want to call c.setApplicationClass() on this class!");
  if (!hasActiveBody()) {
    setActiveBody(this.getBodyFromMethodSource("jb"));
    ms=null;
  }
  return getActiveBody();
}
