{
  StringBuffer buffer=new StringBuffer();
  Type t=returnType;
  buffer.append(t.toString());
  buffer.append(' ');
  buffer.append(Scene.v().quotedNameOf(name) + "(");
  Iterator typeIt=params.iterator();
  if (typeIt.hasNext()) {
    t=(Type)typeIt.next();
    buffer.append(t);
    while (typeIt.hasNext()) {
      buffer.append(",");
      t=(Type)typeIt.next();
      buffer.append(t);
    }
  }
  buffer.append(")");
  return buffer.toString();
}
