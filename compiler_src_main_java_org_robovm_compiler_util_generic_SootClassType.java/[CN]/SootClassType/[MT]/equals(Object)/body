{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  SootClassType other=(SootClassType)obj;
  if (sootClass == null) {
    if (other.sootClass != null) {
      return false;
    }
  }
 else   if (!sootClass.equals(other.sootClass)) {
    return false;
  }
  return true;
}
