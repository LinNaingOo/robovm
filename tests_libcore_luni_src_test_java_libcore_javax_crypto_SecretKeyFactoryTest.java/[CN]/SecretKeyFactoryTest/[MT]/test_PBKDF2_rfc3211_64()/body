{
  char[] password="password".toCharArray();
  byte[] salt=new byte[]{(byte)0x12,(byte)0x34,(byte)0x56,(byte)0x78,(byte)0x78,(byte)0x56,(byte)0x34,(byte)0x12};
  int iterations=5;
  int keyLength=64;
  byte[] expected=new byte[]{(byte)0xD1,(byte)0xDA,(byte)0xA7,(byte)0x86,(byte)0x15,(byte)0xF2,(byte)0x87,(byte)0xE6};
  test_PBKDF2_UTF8(password,salt,iterations,keyLength,expected);
  test_PBKDF2_8BIT(password,salt,iterations,keyLength,expected);
}
