{
  int n;
  if (-1 == index) {
    int pos=m_next;
    while (DTM.NULL != (n=nextNode()))     ;
    m_next=pos;
  }
 else   if (m_next == index) {
    return;
  }
 else   if (hasCache() && m_next < getVector().size()) {
    m_next=index;
  }
 else   if ((null == getVector()) && (index < m_next)) {
    while ((m_next >= index) && DTM.NULL != (n=previousNode()))     ;
  }
 else {
    while ((m_next < index) && DTM.NULL != (n=nextNode()))     ;
  }
}
