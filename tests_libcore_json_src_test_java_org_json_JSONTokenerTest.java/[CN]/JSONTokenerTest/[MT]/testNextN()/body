{
  JSONTokener abcdeTokener=new JSONTokener("ABCDEF");
  assertEquals("",abcdeTokener.next(0));
  try {
    abcdeTokener.next(7);
    fail();
  }
 catch (  JSONException e) {
  }
  assertEquals("ABC",abcdeTokener.next(3));
  try {
    abcdeTokener.next(4);
    fail();
  }
 catch (  JSONException e) {
  }
}
