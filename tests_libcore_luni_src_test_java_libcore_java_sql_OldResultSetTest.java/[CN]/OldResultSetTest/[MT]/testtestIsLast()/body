{
  assertFalse(target.isLast());
  target.absolute(-1);
  assertTrue(target.isLast());
  assertFalse(emptyTarget.isLast());
  assertFalse(emptyTarget.next());
  assertFalse(emptyTarget.isLast());
  try {
    target.close();
    target.isLast();
    fail("Should get SQLException");
  }
 catch (  SQLException e) {
  }
}
