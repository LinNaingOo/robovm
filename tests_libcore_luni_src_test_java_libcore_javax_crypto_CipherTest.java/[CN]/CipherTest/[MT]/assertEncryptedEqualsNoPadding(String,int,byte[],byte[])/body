{
  if (provider.equals("BC") && mode == Cipher.DECRYPT_MODE) {
    int nonZeroOffset=0;
    for (    byte b : expected) {
      if (b != 0) {
        break;
      }
      nonZeroOffset++;
    }
    expected=Arrays.copyOfRange(expected,nonZeroOffset,expected.length);
  }
  assertEquals("Encrypted should match expected",Arrays.toString(expected),Arrays.toString(actual));
}
