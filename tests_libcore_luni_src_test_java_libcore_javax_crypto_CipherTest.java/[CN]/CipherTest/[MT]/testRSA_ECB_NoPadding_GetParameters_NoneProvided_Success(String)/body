{
  KeyFactory kf=KeyFactory.getInstance("RSA");
  RSAPublicKeySpec pubKeySpec=new RSAPublicKeySpec(RSA_2048_modulus,RSA_2048_publicExponent);
  final PublicKey pubKey=kf.generatePublic(pubKeySpec);
