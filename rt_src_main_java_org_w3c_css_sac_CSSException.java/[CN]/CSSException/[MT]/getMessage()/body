{
  if (s != null) {
    return s;
  }
 else   if (e != null) {
    return e.getMessage();
  }
 else {
switch (code) {
case SAC_UNSPECIFIED_ERR:
      return S_SAC_UNSPECIFIED_ERR;
case SAC_NOT_SUPPORTED_ERR:
    return S_SAC_NOT_SUPPORTED_ERR;
case SAC_SYNTAX_ERR:
  return S_SAC_SYNTAX_ERR;
default :
return null;
}
}
}
