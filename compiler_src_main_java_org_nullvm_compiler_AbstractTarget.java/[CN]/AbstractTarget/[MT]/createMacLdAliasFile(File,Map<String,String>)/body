{
  PrintWriter w=null;
  try {
    w=new PrintWriter(aliasFile,"ASCII");
    for (    Entry<String,String> alias : aliases.entrySet()) {
      w.print("_");
      w.print(alias.getValue());
      w.print(" _");
      w.print(alias.getKey());
      w.print("\n");
    }
  }
  finally {
    IOUtils.closeQuietly(w);
  }
}
