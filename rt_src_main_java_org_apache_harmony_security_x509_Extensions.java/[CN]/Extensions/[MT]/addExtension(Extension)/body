{
  encoding=null;
  if (extensions == null) {
    extensions=new ArrayList();
  }
  extensions.add(extn);
  if (oidMap != null) {
    oidMap.put(extn.getExtnID(),extn);
  }
  if (critical != null) {
    String oid=extn.getExtnID();
    if (extn.getCritical()) {
      if (!SUPPORTED_CRITICAL.contains(oid)) {
        hasUnsupported=true;
      }
      critical.add(oid);
    }
 else {
      noncritical.add(oid);
    }
  }
}
