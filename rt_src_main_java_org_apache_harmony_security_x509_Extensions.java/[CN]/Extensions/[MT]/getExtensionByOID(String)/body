{
  if (extensions == null) {
    return null;
  }
  if (oidMap == null) {
    oidMap=new HashMap();
    Iterator it=extensions.iterator();
    while (it.hasNext()) {
      Extension extn=(Extension)it.next();
      oidMap.put(extn.getExtnID(),extn);
    }
  }
  return (Extension)oidMap.get(oid);
}
