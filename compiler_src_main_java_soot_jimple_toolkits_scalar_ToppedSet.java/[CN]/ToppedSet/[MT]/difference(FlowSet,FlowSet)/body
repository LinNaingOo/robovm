{
  ToppedSet other=(ToppedSet)o, dest=(ToppedSet)d;
  if (isTop()) {
    if (other.isTop())     dest.clear();
 else     if (other.underlyingSet instanceof BoundedFlowSet)     ((BoundedFlowSet)other.underlyingSet).complement(dest);
 else     throw new RuntimeException("can't take difference!");
  }
 else {
    if (other.isTop())     dest.clear();
 else     underlyingSet.difference(other.underlyingSet,dest.underlyingSet);
  }
}
