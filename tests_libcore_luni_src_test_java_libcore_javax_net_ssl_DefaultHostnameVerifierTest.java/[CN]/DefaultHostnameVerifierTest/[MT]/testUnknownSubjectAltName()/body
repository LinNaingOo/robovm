{
  assertTrue(verifier.verify("imap.g.com",new StubX509Certificate("").addSubjectAlternativeName(ALT_UNKNOWN,"random string 1").addSubjectAlternativeName(ALT_UNKNOWN,"random string 2").addSubjectAlternativeName(ALT_DNS_NAME,"a.b.c.d").addSubjectAlternativeName(ALT_DNS_NAME,"*.google.com").addSubjectAlternativeName(ALT_DNS_NAME,"imap.g.com").addSubjectAlternativeName(ALT_IPA_NAME,"2.33.44.55").addSubjectAlternativeName(ALT_UNKNOWN,"random string 3")));
  assertTrue(verifier.verify("2.33.44.55",new StubX509Certificate("").addSubjectAlternativeName(ALT_UNKNOWN,"random string 1").addSubjectAlternativeName(ALT_UNKNOWN,"random string 2").addSubjectAlternativeName(ALT_DNS_NAME,"a.b.c.d").addSubjectAlternativeName(ALT_DNS_NAME,"*.google.com").addSubjectAlternativeName(ALT_DNS_NAME,"imap.g.com").addSubjectAlternativeName(ALT_IPA_NAME,"2.33.44.55").addSubjectAlternativeName(ALT_UNKNOWN,"random string 3")));
  assertFalse(verifier.verify("g.com",new StubX509Certificate("").addSubjectAlternativeName(ALT_UNKNOWN,"random string 1").addSubjectAlternativeName(ALT_UNKNOWN,"random string 2").addSubjectAlternativeName(ALT_DNS_NAME,"a.b.c.d").addSubjectAlternativeName(ALT_DNS_NAME,"*.google.com").addSubjectAlternativeName(ALT_DNS_NAME,"imap.g.com").addSubjectAlternativeName(ALT_IPA_NAME,"2.33.44.55").addSubjectAlternativeName(ALT_UNKNOWN,"random string 3")));
  assertFalse(verifier.verify("2.33.44.1",new StubX509Certificate("").addSubjectAlternativeName(ALT_UNKNOWN,"random string 1").addSubjectAlternativeName(ALT_UNKNOWN,"random string 2").addSubjectAlternativeName(ALT_DNS_NAME,"a.b.c.d").addSubjectAlternativeName(ALT_DNS_NAME,"*.google.com").addSubjectAlternativeName(ALT_DNS_NAME,"imap.g.com").addSubjectAlternativeName(ALT_IPA_NAME,"2.33.44.55").addSubjectAlternativeName(ALT_UNKNOWN,"random string 3")));
}
