  assertFalse(verifier.verify("www.example.com",cert));
  assertFalse(verifier.verify("www2.example.com",cert));
  assertTrue(verifier.verify("www.example2.com",cert));
  assertTrue(verifier.verify("abc.example2.com",cert));
