{
  assertTrue(verifier.verify("1.2.3.4",new StubX509Certificate("").addSubjectAlternativeName(ALT_IPA_NAME,"1.2.3.4")));
  assertFalse(verifier.verify("1.2.3.5",new StubX509Certificate("").addSubjectAlternativeName(ALT_IPA_NAME,"1.2.3.4")));
  assertTrue(verifier.verify("192.168.100.1",new StubX509Certificate("").addSubjectAlternativeName(ALT_IPA_NAME,"1.2.3.4").addSubjectAlternativeName(ALT_IPA_NAME,"192.168.100.1")));
}
