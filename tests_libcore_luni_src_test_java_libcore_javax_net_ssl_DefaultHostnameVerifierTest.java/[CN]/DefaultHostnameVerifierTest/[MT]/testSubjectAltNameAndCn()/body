{
  assertFalse(verifier.verify("imap.g.com",new StubX509Certificate("").addSubjectAlternativeName(ALT_DNS_NAME,"a.y.com")));
  assertFalse(verifier.verify("imap.g.com",new StubX509Certificate("cn=imap.g.com").addSubjectAlternativeName(ALT_DNS_NAME,"a.y.com")));
  assertTrue(verifier.verify("imap.g.com",new StubX509Certificate("").addSubjectAlternativeName(ALT_DNS_NAME,"imap.g.com")));
}
