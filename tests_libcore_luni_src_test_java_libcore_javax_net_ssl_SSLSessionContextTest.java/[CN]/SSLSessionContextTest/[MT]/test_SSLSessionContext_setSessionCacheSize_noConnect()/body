{
  TestSSLContext c=TestSSLContext.create();
  assertNoConnectSetSessionCacheSizeBehavior(TestSSLContext.EXPECTED_DEFAULT_CLIENT_SSL_SESSION_CACHE_SIZE,c.clientContext.getClientSessionContext());
  assertNoConnectSetSessionCacheSizeBehavior(TestSSLContext.EXPECTED_DEFAULT_SERVER_SSL_SESSION_CACHE_SIZE,c.serverContext.getServerSessionContext());
  c.close();
}
