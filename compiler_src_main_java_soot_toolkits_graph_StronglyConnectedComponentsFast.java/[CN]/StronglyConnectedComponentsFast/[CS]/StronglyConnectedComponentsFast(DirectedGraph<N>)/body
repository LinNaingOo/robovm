{
  this.g=g;
  s=new Stack<N>();
  List<N> heads=g.getHeads();
  if (heads.size() > 1)   throw new RuntimeException("Cannot compute SCCs for graph with number of heads = " + heads.size());
  indexForNode=new HashMap<N,Integer>();
  lowlinkForNode=new HashMap<N,Integer>();
  recurse(heads.get(0));
  indexForNode=null;
  lowlinkForNode=null;
  s=null;
  g=null;
}
