{
  int index=hashCodeChar(key);
  while (keyTable[index] != null) {
    if (equals(keyTable[index],key)) {
      return valueTable[index]=value;
    }
    index=(index + 1) % keyTable.length;
  }
  keyTable[index]=key;
  valueTable[index]=value;
  if (++elementSize > threshold) {
    rehash();
  }
  return value;
}
