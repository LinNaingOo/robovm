{
  if (toBeChecked == null || toBeChecked.length != opi.length) {
    return false;
  }
  for (int i=0; i < opi.length; i++) {
    if (opi[i].getPrime().equals(toBeChecked[i].getPrime()) && opi[i].getExponent().equals(toBeChecked[i].getExponent()) && opi[i].getCrtCoefficient().equals(toBeChecked[i].getCrtCoefficient())) {
      continue;
    }
    return false;
  }
  return true;
}
