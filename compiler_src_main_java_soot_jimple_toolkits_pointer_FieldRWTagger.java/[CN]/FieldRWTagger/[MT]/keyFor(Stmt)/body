{
  if (s.containsInvokeExpr()) {
    if (optionNaive)     throw new RuntimeException("shouldn't get here");
    Iterator it=cg.edgesOutOf(s);
    if (!it.hasNext()) {
      return Collections.EMPTY_LIST;
    }
    ArrayList ret=new ArrayList();
    while (it.hasNext()) {
      ret.add(it.next());
    }
    return ret;
  }
 else {
    return s;
  }
}
