{
  super.setUp();
  try {
    conn.setAutoCommit(false);
    assertFalse(conn.getAutoCommit());
    String query="select * from zoo";
    st=conn.createStatement();
    st.execute(query);
    rs=st.getResultSet();
    rsmd=rs.getMetaData();
  }
 catch (  SQLException e) {
    fail("Couldn't get ResultSetMetaData object");
  }
}
