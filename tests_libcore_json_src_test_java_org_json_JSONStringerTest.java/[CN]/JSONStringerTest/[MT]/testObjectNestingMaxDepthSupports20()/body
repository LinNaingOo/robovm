{
  JSONStringer stringer=new JSONStringer();
  for (int i=0; i < 20; i++) {
    stringer.object();
    stringer.key("a");
  }
  stringer.value(false);
  for (int i=0; i < 20; i++) {
    stringer.endObject();
  }
  assertEquals("{\"a\":{\"a\":{\"a\":{\"a\":{\"a\":{\"a\":{\"a\":{\"a\":{\"a\":{\"a\":" + "{\"a\":{\"a\":{\"a\":{\"a\":{\"a\":{\"a\":{\"a\":{\"a\":{\"a\":{\"a\":false" + "}}}}}}}}}}}}}}}}}}}}",stringer.toString());
  stringer=new JSONStringer();
  for (int i=0; i < 20; i++) {
    stringer.object();
    stringer.key("a");
  }
}
