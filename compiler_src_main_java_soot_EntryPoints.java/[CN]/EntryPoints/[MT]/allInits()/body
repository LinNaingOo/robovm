{
  List<SootMethod> ret=new ArrayList<SootMethod>();
  for (Iterator clIt=Scene.v().getClasses().iterator(); clIt.hasNext(); ) {
    final SootClass cl=(SootClass)clIt.next();
    for (    SootMethod m : cl.getMethods()) {
      if (m.getName().equals("<init>")) {
        ret.add(m);
      }
    }
  }
  return ret;
}
