{
  server.enqueue(new MockResponse().addHeader("Cache-Control: max-age=60").addHeader("Content-Location: /bar").setBody("A"));
  server.enqueue(new MockResponse().setBody("B"));
  server.play();
  assertEquals("A",readAscii(server.getUrl("/foo").openConnection()));
  assertEquals("B",readAscii(server.getUrl("/bar").openConnection()));
}
