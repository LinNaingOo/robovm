{
  int max=size();
  StringBuilder buf=new StringBuilder();
  buf.append("{");
  for (int i=0; i < max; ++i) {
    if (keyTable[i] != null) {
      buf.append(keyTable[i]).append("->").append(valueTable[i]);
    }
    if (i < max) {
      buf.append(", ");
    }
  }
  buf.append("}");
  return buf.toString();
}
