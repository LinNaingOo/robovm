{
  if (null != this.channel1) {
    try {
      this.channel1.close();
    }
 catch (    Exception e) {
    }
  }
  if (null != this.channel2) {
    try {
      this.channel2.close();
    }
 catch (    Exception e) {
    }
  }
  if (null != this.datagramSocket1) {
    try {
      this.datagramSocket1.close();
    }
 catch (    Exception e) {
    }
  }
  if (null != this.datagramSocket2) {
    try {
      this.datagramSocket2.close();
    }
 catch (    Exception e) {
    }
  }
  localAddr1=null;
  localAddr2=null;
  super.tearDown();
}
