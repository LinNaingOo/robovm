{
  String name=method.getName();
  List<?> parameterTypes=method.getParameterTypes();
  soot.Type returnType=method.getReturnType();
  while (c.hasSuperclass()) {
    if (c.getSuperclass().declaresMethod(name,parameterTypes,returnType)) {
      return true;
    }
    c=c.getSuperclass();
  }
  return false;
}
