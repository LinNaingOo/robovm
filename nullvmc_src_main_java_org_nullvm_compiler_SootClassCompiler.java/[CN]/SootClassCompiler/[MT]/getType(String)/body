{
switch (desc.charAt(0)) {
case 'Z':
    return I8;
case 'B':
  return I8;
case 'S':
return I16;
case 'C':
return I16;
case 'I':
return I32;
case 'J':
return I64;
case 'F':
return FLOAT;
case 'D':
return DOUBLE;
case 'V':
return VOID;
case 'L':
return OBJECT_PTR;
case '[':
return OBJECT_PTR;
}
throw new IllegalArgumentException();
}
