{
  StringBuilder sb=new StringBuilder();
  for (  Path path : clazzes.getPaths()) {
    if (sb.length() > 0) {
      sb.append(File.pathSeparator);
    }
    try {
      sb.append(path.getFile().getCanonicalPath());
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
  }
  return sb.toString();
}
