{
  StringBuilder sb=new StringBuilder("\"str_");
  for (int i=0; i < bytes.length; i++) {
    byte b=bytes[i];
    if (b < ' ' || b > '~' || b == '"' || b == '\\' || b == '%' || b == '@') {
      sb.append(String.format("%%%02X",b));
    }
 else {
      sb.append((char)b);
    }
  }
  sb.append("\"");
  return sb.toString();
}
