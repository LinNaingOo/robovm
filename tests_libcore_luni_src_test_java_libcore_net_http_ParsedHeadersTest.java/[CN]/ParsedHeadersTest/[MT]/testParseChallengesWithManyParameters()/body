{
  RawHeaders headers=new RawHeaders();
  headers.add("WWW-Authenticate","Digest realm=\"abc\"," + " qop=\"auth,auth-int\"," + " nonce=\"dcd98b7102dd2f0e8b11d0f600bfb0c093\","+ " opaque=\"5ccc069c403ebaf9f0171e9517f40e41\","+ " Basic realm=\"def\"");
  assertEquals(Arrays.asList(new Challenge("Digest","abc"),new Challenge("Basic","def")),HeaderParser.parseChallenges(headers,"WWW-Authenticate"));
}
