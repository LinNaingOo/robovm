{
  Object value=AccessController.doPrivileged(new PrivilegedAction<Object>(){
    public Object run(){
      Object result=null;
      java.lang.reflect.Field field=null;
      try {
        field=target.getClass().getDeclaredField(fieldName);
        field.setAccessible(true);
        result=field.get(target);
      }
 catch (      Exception e1) {
        return null;
      }
      return result;
    }
  }
);
  return value;
}
