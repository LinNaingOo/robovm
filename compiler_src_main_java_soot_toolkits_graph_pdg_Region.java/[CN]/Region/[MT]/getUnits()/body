{
  if (this.m_units == null) {
    this.m_units=new LinkedList<Unit>();
    for (Iterator<Block> itr=this.m_blocks.iterator(); itr.hasNext(); ) {
      Block b=itr.next();
      for (Iterator<Unit> itr1=b.iterator(); itr1.hasNext(); ) {
        Unit u=itr1.next();
        ((LinkedList<Unit>)this.m_units).addLast(u);
      }
    }
  }
  return this.m_units;
}
