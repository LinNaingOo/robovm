{
  if (range == 0) {
    throw new IllegalArgumentException("Illegal range argument value: " + range);
  }
  int index=0;
  while (index < MAX_INDEX) {
    if (range == (1 << index)) {
      return index;
    }
    index++;
  }
  throw new IllegalArgumentException("Illegal range argument value: " + range);
}
