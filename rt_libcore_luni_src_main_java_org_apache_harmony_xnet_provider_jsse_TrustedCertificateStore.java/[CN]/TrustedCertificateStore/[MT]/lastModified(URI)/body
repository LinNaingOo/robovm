{
  if ("file".equals(file.getScheme())) {
    return new File(file).lastModified();
  }
  if ("jar".equals(file.getScheme())) {
    try {
      JarURLConnection conn=(JarURLConnection)file.toURL().openConnection();
      return conn.getJarEntry().getTime();
    }
 catch (    IOException e) {
    }
  }
  return 0L;
}
