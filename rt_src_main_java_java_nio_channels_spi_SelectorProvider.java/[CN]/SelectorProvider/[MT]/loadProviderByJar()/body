{
  Enumeration<URL> enumeration=null;
  ClassLoader classLoader=AccessController.doPrivileged(new PrivilegedAction<ClassLoader>(){
    public ClassLoader run(){
      return ClassLoader.getSystemClassLoader();
    }
  }
);
  try {
    enumeration=classLoader.getResources(PROVIDER_IN_JAR_RESOURCE);
  }
 catch (  IOException e) {
    throw new Error(e);
  }
  if (null == enumeration) {
    return null;
  }
  while (enumeration.hasMoreElements()) {
    BufferedReader br=null;
    String className=null;
    try {
      br=new BufferedReader(new InputStreamReader((enumeration.nextElement()).openStream()));
    }
 catch (    Exception e) {
      continue;
    }
    try {
      while ((className=br.readLine()) != null) {
        className=className.trim();
        int siteComment=className.indexOf(SYMBOL_COMMENT);
        className=(-1 == siteComment) ? className : className.substring(0,siteComment);
        if (0 < className.length()) {
          return (SelectorProvider)classLoader.loadClass(className).newInstance();
        }
      }
    }
 catch (    Exception e) {
      throw new Error(e);
    }
 finally {
      try {
        br.close();
      }
 catch (      IOException ioe) {
      }
    }
  }
  return null;
}
