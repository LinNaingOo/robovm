{
  return AccessController.doPrivileged(new PrivilegedAction<SelectorProvider>(){
    public SelectorProvider run(){
      try {
        final String className=System.getProperty(PROVIDER_IN_SYSTEM_PROPERTY);
        if (null != className) {
          Class<?> spClass=ClassLoader.getSystemClassLoader().loadClass(className);
          return (SelectorProvider)spClass.newInstance();
        }
        return null;
      }
 catch (      Exception e) {
        throw new Error(e);
      }
    }
  }
);
}
