{
  if (fully_loaded)   return;
  if (!i_am_loaded) {
    try {
      ClassInfo info=context.loader.provide(class_name);
      if (info == null)       throw new verify.VerificationException("cannot load class " + this);
      loadFrom(info);
    }
 catch (    IllegalTypeNameException x) {
    }
  }
  fully_loaded=true;
  if (super_class != null)   super_class.load();
  if (interfaces != null) {
    for (int i=0; i < interfaces.length; i++) {
      interfaces[i].load();
    }
  }
}
