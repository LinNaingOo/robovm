{
  if (this == otherClass) {
    return true;
  }
  if (super_class == null)   load();
  if (super_class != null)   if (super_class.isSubtypeOf(otherClass)) {
    return true;
  }
  if (interfaces != null) {
    for (int i=0; i < interfaces.length; i++) {
      if (interfaces[i].isSubtypeOf(otherClass)) {
        return true;
      }
    }
  }
  return false;
}
