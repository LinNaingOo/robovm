{
  StringBuilder sb=new StringBuilder();
  sb.append(result);
  sb.append(" = landingpad ");
  sb.append(result.getType());
  sb.append(" personality ");
  sb.append(personalityFn.getType());
  sb.append(' ');
  sb.append(personalityFn);
  if (cleanup) {
    sb.append(" cleanup");
  }
  for (  Clause clause : clauses) {
    sb.append(' ');
    sb.append(clause);
  }
  return sb.toString();
}
