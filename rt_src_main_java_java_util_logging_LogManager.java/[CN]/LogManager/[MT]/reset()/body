{
  checkAccess();
  props=new Properties();
  Enumeration<String> names=getLoggerNames();
  while (names.hasMoreElements()) {
    String name=names.nextElement();
    Logger logger=getLogger(name);
    if (logger != null) {
      logger.reset();
    }
  }
  Logger root=loggers.get("");
  if (null != root) {
    root.setLevel(Level.INFO);
  }
}
