{
  PhaseOptions.v().getPM().notifyAddPack();
  Iterator it=opts.iterator();
  while (it.hasNext()) {
    Transform tr=(Transform)it.next();
    if (tr.getPhaseName().equals(phaseName)) {
      opts.insertAfter(t,tr);
      return;
    }
  }
  throw new RuntimeException("phase " + phaseName + " not found!");
}
