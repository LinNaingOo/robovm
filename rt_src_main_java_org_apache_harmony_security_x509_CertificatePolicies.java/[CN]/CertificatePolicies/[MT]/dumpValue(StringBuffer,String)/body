{
  buffer.append(prefix).append("CertificatePolicies [\n");
  for (Iterator it=policyInformations.iterator(); it.hasNext(); ) {
    buffer.append(prefix);
    buffer.append("  ");
    ((PolicyInformation)it.next()).dumpValue(buffer);
    buffer.append('\n');
  }
  buffer.append(prefix).append("]\n");
}
