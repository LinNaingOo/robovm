{
  install(getCa1(),getAliasSystemCa1());
  assertRootCa(getCa1(),getAliasSystemCa1());
  assertAliases(getAliasSystemCa1());
  store.deleteCertificateEntry(getAliasSystemCa1());
  assertEmpty();
  assertDeleted(getCa1(),getAliasSystemCa1());
  store.deleteCertificateEntry(getAliasSystemCa1());
  assertEmpty();
  assertDeleted(getCa1(),getAliasSystemCa1());
}
