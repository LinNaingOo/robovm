{
  StringBuilder builder=new StringBuilder();
  builder.append("Resource [targetPath=").append(targetPath).append(", directory=").append(directory).append(", includes=").append(includes).append(", excludes=").append(excludes).append(", flatten=").append(flatten).append(", ignoreDefaultExcludes=").append(ignoreDefaultExcludes).append(", skipPngCrush=").append(skipPngCrush).append(", path=").append(path).append("]");
  return builder.toString();
}
