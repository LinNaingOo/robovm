{
  PatchingChain<Unit> units=currentBody.getUnits();
  for (  Unit u : units) {
    if (u instanceof GotoStmt || u instanceof IfStmt) {
      Unit target=u instanceof GotoStmt ? ((GotoStmt)u).getTarget() : ((IfStmt)u).getTarget();
      if (target == unit) {
        return true;
      }
    }
  }
  return false;
}
