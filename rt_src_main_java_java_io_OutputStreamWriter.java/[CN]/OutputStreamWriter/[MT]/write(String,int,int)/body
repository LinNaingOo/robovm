{
synchronized (lock) {
    if (count < 0) {
      throw new IndexOutOfBoundsException();
    }
    if (offset > str.length() - count || offset < 0) {
      throw new StringIndexOutOfBoundsException();
    }
    checkStatus();
    CharBuffer chars=CharBuffer.wrap(str,offset,count + offset);
    convert(chars);
  }
}
