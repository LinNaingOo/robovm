{
  assert(xararar instanceof Object);
  assert(xararar instanceof Object[]);
  assert(!(xararar instanceof Classes));
  assert(!(xararar instanceof Classes[]));
  assert(!(xararar instanceof Classes[][]));
  assert(xararar instanceof Classes[][][]);
  assert(!(xararar instanceof MoreClasses[][][]));
  assert(xararar instanceof Object[][][]);
  assert(xararar instanceof Serializable);
  assert(xararar instanceof Serializable[]);
  assert(xararar instanceof Serializable[][]);
  assert(!(xararar instanceof Serializable[][][]));
}
