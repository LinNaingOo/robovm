{
  if (failed) {
    if (errorMessage != null) {
      fail(errorMessage);
    }
 else {
      fail("init failed");
    }
  }
  test.runTest();
  int mutationCount=test.getMutationCount();
  if (mutationCount != 0) {
    fail("Document loaded with willBeModified='false' was modified in course of test.");
  }
}
