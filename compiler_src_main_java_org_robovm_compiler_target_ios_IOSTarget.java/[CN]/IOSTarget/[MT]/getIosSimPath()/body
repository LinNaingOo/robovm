{
  if (iosSimPath == null) {
    try {
      File path=File.createTempFile("ios-sim","");
      FileUtils.copyURLToFile(IOSTarget.class.getResource("/ios-sim"),path);
      path.setExecutable(true);
      path.deleteOnExit();
      iosSimPath=path;
    }
 catch (    IOException e) {
      throw new Error(e);
    }
  }
  return iosSimPath;
}
