{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  Clazz other=(Clazz)obj;
  if (className == null) {
    if (other.className != null) {
      return false;
    }
  }
 else   if (!className.equals(other.className)) {
    return false;
  }
  if (path == null) {
    if (other.path != null) {
      return false;
    }
  }
 else   if (!path.equals(other.path)) {
    return false;
  }
  return true;
}
