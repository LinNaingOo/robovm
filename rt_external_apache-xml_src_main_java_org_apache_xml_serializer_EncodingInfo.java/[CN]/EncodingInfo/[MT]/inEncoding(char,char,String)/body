{
  boolean isInEncoding;
  try {
    char cArray[]=new char[2];
    cArray[0]=high;
    cArray[1]=low;
    String s=new String(cArray);
    byte[] bArray=s.getBytes(encoding);
    isInEncoding=inEncoding(high,bArray);
  }
 catch (  Exception e) {
    isInEncoding=false;
  }
  return isInEncoding;
}
