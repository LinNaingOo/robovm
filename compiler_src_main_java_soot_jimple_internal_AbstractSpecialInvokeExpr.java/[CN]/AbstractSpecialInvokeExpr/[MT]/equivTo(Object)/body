{
  if (o instanceof AbstractSpecialInvokeExpr) {
    AbstractSpecialInvokeExpr ie=(AbstractSpecialInvokeExpr)o;
    if (!(baseBox.getValue().equivTo(ie.baseBox.getValue()) && getMethod().equals(ie.getMethod()) && argBoxes.length == ie.argBoxes.length))     return false;
    for (    ValueBox element : argBoxes)     if (!(element.getValue().equivTo(element.getValue())))     return false;
    return true;
  }
  return false;
}
