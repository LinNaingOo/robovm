{
  if (buffer == null) {
    throw new NullPointerException();
  }
  if (count < 0 || offset < 0 || offset > buffer.length || count > buffer.length - offset) {
    throw new IndexOutOfBoundsException();
  }
  if (count == 0) {
    return;
  }
  openCheck();
  fileSystem.write(fd.descriptor,buffer,offset,count);
}
