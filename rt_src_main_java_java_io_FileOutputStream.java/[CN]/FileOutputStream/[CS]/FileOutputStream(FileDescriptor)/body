{
  super();
  if (fd == null) {
    throw new NullPointerException(Messages.getString("luni.B6"));
  }
  SecurityManager security=System.getSecurityManager();
  if (security != null) {
    security.checkWrite(fd);
  }
  this.fd=fd;
  innerFD=false;
  channel=FileChannelFactory.getFileChannel(this,fd.descriptor,IFileSystem.O_WRONLY);
}
