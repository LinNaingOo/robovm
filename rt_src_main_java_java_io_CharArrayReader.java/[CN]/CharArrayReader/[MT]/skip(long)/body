{
synchronized (lock) {
    if (isClosed()) {
      throw new IOException(Messages.getString("luni.A9"));
    }
    if (n <= 0) {
      return 0;
    }
    long skipped=0;
    if (n < this.count - pos) {
      pos=pos + (int)n;
      skipped=n;
    }
 else {
      skipped=this.count - pos;
      pos=this.count;
    }
    return skipped;
  }
}
