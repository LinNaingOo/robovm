{
  Locale arabic=new Locale("ar");
  if (!Support_Locale.isLocaleAvailable(arabic)) {
    return;
  }
  assertEquals("12 \u0661\u0662\u0663\u0664\u0665\u0666\u0667\u0668\u0669\u0660 34",String.format(arabic,"12 %d 34",1234567890));
  assertEquals("12 \u0661\u066c\u0662\u0663\u0664\u066c\u0665\u0666\u0667\u066c\u0668\u0669\u0660 34",String.format(arabic,"12 %,d 34",1234567890));
  assertEquals("12 \u0661\u066b\u0662\u0663\u0660\u0627\u0633+\u0660\u0660 34",String.format(arabic,"12 %.3e 34",1.23));
  assertEquals("12 \u0661\u066b\u0662\u0663\u0660 34",String.format(arabic,"12 %.3f 34",1.23));
  assertEquals("12 \u0661\u066b\u0662\u0663 34",String.format(arabic,"12 %.3g 34",1.23));
  Calendar c=Calendar.getInstance(TimeZone.getTimeZone("GMT-08:00"));
  c.setTimeInMillis(0);
  assertEquals("12 \u0661\u0666:\u0660\u0660:\u0660\u0660 34",String.format(arabic,"12 %tT 34",c));
  assertEquals("1234",String.format(arabic,"1234"));
  assertEquals("1234",String.format(arabic,"%s","1234"));
  assertEquals("1234",String.format(arabic,"%s",1234));
  assertEquals("2322",String.format(arabic,"%o",1234));
  assertEquals("4d2",String.format(arabic,"%x",1234));
  assertEquals("0x1.0p0",String.format(arabic,"%a",1.0));
}
