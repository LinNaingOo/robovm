{
  if (modCount == fullList.modCount) {
    if (0 <= location && location <= size) {
      fullList.add(location + offset,object);
      size++;
      modCount=fullList.modCount;
    }
 else {
      throw new IndexOutOfBoundsException();
    }
  }
 else {
    throw new ConcurrentModificationException();
  }
}
