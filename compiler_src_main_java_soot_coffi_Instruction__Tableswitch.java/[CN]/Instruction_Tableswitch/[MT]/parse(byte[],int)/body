{
  int i, j;
  i=index % 4;
  if (i != 0)   pad=(byte)(4 - i);
 else   pad=(byte)0;
  index+=pad;
  default_offset=getInt(bc,index);
  index+=4;
  low=getInt(bc,index);
  index+=4;
  high=getInt(bc,index);
  index+=4;
  i=high - low + 1;
  if (i > 0) {
    jump_offsets=new int[i];
    j=0;
    do {
      jump_offsets[j]=getInt(bc,index);
      index+=4;
      j++;
    }
 while (j < i);
  }
  return index;
}
