{
  this.m_methodBody=cfg.getBody();
  this.m_cfg=cfg;
  this.m_method=m;
  this.m_class=c;
  if (Options.v().verbose())   G.v().out.println("[RegionAnalysis]~~~~~~~~~~~~~~~ Begin Region Analsis for method: " + m.getName() + " ~~~~~~~~~~~~~~~~~~~~");
  this.findWeakRegions();
  if (Options.v().verbose())   G.v().out.println("[RegionAnalysis]~~~~~~~~~~~~~~~ End:" + m.getName() + " ~~~~~~~~~~~~~~~~~~~~");
}
