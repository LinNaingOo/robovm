{
  SocketChannel channel=SelectorProvider.provider().openSocketChannel();
  assertNotNull(channel);
  assertSame(SelectorProvider.provider(),channel.provider());
  channel=SocketChannel.open();
  assertNotNull(channel);
  assertSame(SelectorProvider.provider(),channel.provider());
  MockSocketChannel chan=new MockSocketChannel(SelectorProvider.provider());
  assertTrue(chan.isConstructorCalled);
}
