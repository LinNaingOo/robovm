{
  if (tr == null || columnIndex < 1 || columnIndex > tr.ncolumns) {
    throw new SQLException("column " + columnIndex + " not found");
  }
  byte ret[]=null;
  String rd[]=(String[])tr.rows.elementAt(row);
  lastg=rd[columnIndex - 1];
  if (lastg != null) {
    ret=SQLite.StringEncoder.decode(lastg);
  }
  return ret;
}
