{
  this.lexemes=new Lexer(pattern,flags);
  this.flags=flags;
  this.pattern=pattern;
  start=processExpression(-1,this.flags,null);
  if (!lexemes.isEmpty()) {
    throw new PatternSyntaxException(Messages.getString("regex.08"),lexemes.toString(),lexemes.getIndex());
  }
  finalizeCompile();
  return this;
}
