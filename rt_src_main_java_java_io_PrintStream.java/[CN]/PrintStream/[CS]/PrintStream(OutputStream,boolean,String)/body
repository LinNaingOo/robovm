{
  super(out);
  if (out == null || enc == null) {
    throw new NullPointerException();
  }
  this.autoflush=autoflush;
  try {
    if (!Charset.isSupported(enc)) {
      throw new UnsupportedEncodingException(enc);
    }
  }
 catch (  IllegalCharsetNameException e) {
    throw new UnsupportedEncodingException(enc);
  }
  encoding=enc;
}
