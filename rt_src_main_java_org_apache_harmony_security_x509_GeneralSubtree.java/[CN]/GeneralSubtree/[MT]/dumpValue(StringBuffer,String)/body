{
  buffer.append(prefix).append("General Subtree: [\n");
  buffer.append(prefix).append("  base: ").append(base).append('\n');
  buffer.append(prefix).append("  minimum: ").append(minimum).append('\n');
  if (maximum >= 0) {
    buffer.append(prefix).append("  maximum: ").append(maximum).append('\n');
  }
  buffer.append(prefix).append("]\n");
}
