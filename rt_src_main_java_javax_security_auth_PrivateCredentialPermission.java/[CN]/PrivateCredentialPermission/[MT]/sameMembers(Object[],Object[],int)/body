{
  if (ar1 == null && ar2 == null) {
    return true;
  }
  if (ar1 == null || ar2 == null) {
    return false;
  }
  boolean found;
  for (int i=0; i < length; i++) {
    found=false;
    for (int j=0; j < length; j++) {
      if (ar1[i].equals(ar2[j])) {
        found=true;
        break;
      }
    }
    if (!found) {
      return false;
    }
  }
  return true;
}
