{
  if (conn.isClosed()) {
    System.out.println("DatabaseMetaDataTest.insertNewRecord() : closed");
  }
  String insertQuery="INSERT INTO " + DatabaseCreator.TEST_TABLE1 + " (id, field1, field2, field3) VALUES("+ id+ ", '"+ "value"+ id+ "', "+ id+ ", "+ id+ ")";
  id++;
  statement.execute(insertQuery);
}
