{
  int SIndex=ch - SBase;
  if (SIndex < 0 || SIndex >= SCount) {
    return null;
  }
 else {
    int L=LBase + SIndex / NCount;
    int V=VBase + (SIndex % NCount) / TCount;
    int T=SIndex % TCount;
    int decomp[];
    if (T == 0) {
      decomp=new int[]{L,V};
    }
 else {
      T=TBase + T;
      decomp=new int[]{L,V,T};
    }
    return decomp;
  }
}
