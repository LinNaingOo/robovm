{
  if (this == object) {
    return true;
  }
  if (object instanceof Map) {
    Map<?,?> map=(Map<?,?>)object;
    if (size() != map.size()) {
      return false;
    }
    Set<Map.Entry<K,V>> entries=entrySet();
    for (    Map.Entry<?,?> e : map.entrySet()) {
      if (!entries.contains(e)) {
        return false;
      }
    }
    return true;
  }
  return false;
}
