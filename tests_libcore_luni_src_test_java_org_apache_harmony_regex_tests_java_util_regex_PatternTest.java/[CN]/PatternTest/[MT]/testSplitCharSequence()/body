{
  String s[];
  Pattern pat=Pattern.compile("b");
  s=pat.split("abccbadfebb");
  assertEquals(s.length,3);
  s=pat.split("");
  assertEquals(s.length,1);
  pat=Pattern.compile("");
  s=pat.split("");
  assertEquals(s.length,1);
  s=pat.split("abccbadfe");
  assertEquals(s.length,10);
  String s1="";
  String[] arr=s1.split(":");
  assertEquals(arr.length,1);
}
