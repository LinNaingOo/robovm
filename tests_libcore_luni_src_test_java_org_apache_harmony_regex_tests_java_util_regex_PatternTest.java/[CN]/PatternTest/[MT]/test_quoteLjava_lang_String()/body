{
  for (  String aPattern : testPatterns) {
    Pattern p=Pattern.compile(aPattern);
    try {
      assertEquals("quote was wrong for plain text","\\Qtest\\E",p.quote("test"));
      assertEquals("quote was wrong for text with quote sign","\\Q\\Qtest\\E",p.quote("\\Qtest"));
      assertEquals("quote was wrong for quotted text","\\Q\\Qtest\\E\\\\E\\Q\\E",p.quote("\\Qtest\\E"));
    }
 catch (    Exception e) {
      fail("Unexpected exception: " + e);
    }
  }
}
