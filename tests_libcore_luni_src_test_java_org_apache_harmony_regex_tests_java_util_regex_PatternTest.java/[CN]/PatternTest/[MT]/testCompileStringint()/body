{
  String pattern="b)a";
  try {
    Pattern.compile(pattern);
    fail("Expected a PatternSyntaxException when compiling pattern: " + pattern);
  }
 catch (  PatternSyntaxException e) {
  }
  pattern="bcde)a";
  try {
    Pattern.compile(pattern);
    fail("Expected a PatternSyntaxException when compiling pattern: " + pattern);
  }
 catch (  PatternSyntaxException e) {
  }
  pattern="bbg())a";
  try {
    Pattern pat=Pattern.compile(pattern);
    fail("Expected a PatternSyntaxException when compiling pattern: " + pattern);
  }
 catch (  PatternSyntaxException e) {
  }
  pattern="cdb(?i))a";
  try {
    Pattern pat=Pattern.compile(pattern);
    fail("Expected a PatternSyntaxException when compiling pattern: " + pattern);
  }
 catch (  PatternSyntaxException e) {
  }
  pattern="(b\\1)a";
  Pattern.compile(pattern);
}
