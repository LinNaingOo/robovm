{
  String baseString="jwkerhjwehrkwjehrkwjhrwkjehrjwkehrjkwhrkwehrkwhrkwrhwkhrwkjehr";
  String pattern=".*(..).*\\1.*";
  assertTrue(Pattern.compile(pattern).matcher(baseString).matches());
  baseString="saa";
  pattern=".*(.)\\1";
  assertTrue(Pattern.compile(pattern).matcher(baseString).matches());
  assertTrue(Pattern.compile(pattern).matcher(baseString).find());
}
