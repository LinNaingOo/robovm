{
  if (node.type() == null) {
    if (node == ClassHierarchy.v().R0_1) {
      return BooleanType.v();
    }
 else     if (node == ClassHierarchy.v().R0_127) {
      return ByteType.v();
    }
 else     if (node == ClassHierarchy.v().R0_32767) {
      return ShortType.v();
    }
  }
  return node.type();
}
