{
  Value key=stmt.getKey();
  if (key instanceof Local) {
    if (!ClassHierarchy.v().typeNode(((Local)key).getType()).hasAncestor_1(ClassHierarchy.v().INT)) {
      if (fix) {
        stmt.setKey(insertCast((Local)key,IntType.v(),stmt));
      }
 else {
        error("Type Error(18)");
      }
    }
  }
}
