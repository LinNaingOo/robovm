{
  FileOutputStream fos=new FileOutputStream(File.createTempFile("FileOutputStreamTest","tmp"));
  fos.close();
  fos.close();
  fos.flush();
  byte[] bytes="hello".getBytes();
  try {
    fos.write(bytes);
    fail();
  }
 catch (  IOException expected) {
  }
  try {
    fos.write(bytes,0,2);
    fail();
  }
 catch (  IOException expected) {
  }
  try {
    fos.write(42);
    fail();
  }
 catch (  IOException expected) {
  }
  fos.write(new byte[0],0,0);
}
