{
  ensureServerClosed();
  this.channel1.configureBlocking(false);
  statusNotConnected_NotPending();
  assertFalse(this.channel1.connect(localAddr1));
  statusNotConnected_Pending();
  try {
    assertFalse(this.channel1.finishConnect());
    statusNotConnected_Pending();
  }
 catch (  ConnectException e) {
  }
}
