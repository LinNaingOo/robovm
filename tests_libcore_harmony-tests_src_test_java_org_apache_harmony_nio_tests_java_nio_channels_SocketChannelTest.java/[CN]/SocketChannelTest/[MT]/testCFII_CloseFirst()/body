{
  this.channel1.close();
  statusChannelClosed();
  ensureServerOpen();
  try {
    this.channel1.connect(localAddr1);
    fail("Should throw ClosedChannelException.");
  }
 catch (  ClosedChannelException e) {
  }
  statusChannelClosed();
  try {
    this.channel1.finishConnect();
    fail("Should throw ClosedChannelException.");
  }
 catch (  ClosedChannelException e) {
  }
  statusChannelClosed();
  try {
    this.channel1.configureBlocking(false);
    fail("Should throw ClosedChannelException.");
  }
 catch (  ClosedChannelException e) {
  }
  statusChannelClosed();
}
