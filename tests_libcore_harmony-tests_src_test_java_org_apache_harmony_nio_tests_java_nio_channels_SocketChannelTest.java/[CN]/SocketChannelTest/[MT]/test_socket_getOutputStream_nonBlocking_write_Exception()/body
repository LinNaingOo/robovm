{
  byte[] buf=new byte[1];
  channel1.connect(this.localAddr1);
  OutputStream os=channel1.socket().getOutputStream();
  channel1.configureBlocking(false);
  try {
    os.write(1);
    fail();
  }
 catch (  IllegalBlockingModeException expected) {
  }
  try {
    os.write(null);
    fail();
  }
 catch (  NullPointerException expected) {
  }
  try {
    os.write(buf,-1,1);
    fail();
  }
 catch (  IndexOutOfBoundsException expected) {
  }
  try {
    os.write(buf,0,-1);
    fail();
  }
 catch (  IndexOutOfBoundsException expected) {
  }
  try {
    os.write(buf,0,2);
    fail();
  }
 catch (  IndexOutOfBoundsException expected) {
  }
  try {
    os.write(buf,2,0);
    fail();
  }
 catch (  IndexOutOfBoundsException expected) {
  }
  try {
    os.write(null,0,0);
    fail();
  }
 catch (  NullPointerException expected) {
  }
  os.close();
  try {
    os.write(1);
    fail();
  }
 catch (  IllegalBlockingModeException expected) {
  }
  try {
    os.write(null);
    fail();
  }
 catch (  NullPointerException expected) {
  }
  try {
    os.write(buf,-1,1);
    fail();
  }
 catch (  IndexOutOfBoundsException expected) {
  }
  try {
    os.write(buf,0,-1);
    fail();
  }
 catch (  IndexOutOfBoundsException expected) {
  }
  try {
    os.write(buf,0,2);
    fail();
  }
 catch (  IndexOutOfBoundsException expected) {
  }
  try {
    os.write(buf,2,0);
    fail();
  }
 catch (  IndexOutOfBoundsException expected) {
  }
