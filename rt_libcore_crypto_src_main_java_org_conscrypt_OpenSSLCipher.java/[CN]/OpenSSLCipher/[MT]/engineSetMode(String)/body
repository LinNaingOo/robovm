{
  final Mode mode;
  try {
    mode=Mode.valueOf(modeStr.toUpperCase(Locale.US));
  }
 catch (  IllegalArgumentException e) {
    NoSuchAlgorithmException newE=new NoSuchAlgorithmException("No such mode: " + modeStr);
    newE.initCause(e);
    throw newE;
  }
  checkSupportedMode(mode);
  this.mode=mode;
}
