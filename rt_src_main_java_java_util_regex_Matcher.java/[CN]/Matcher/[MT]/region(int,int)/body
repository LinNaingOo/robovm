{
  if (start > end || start < 0 || end < 0 || start > string.length() || end > string.length()) {
    throw new IndexOutOfBoundsException(Messages.getString("regex.02",Integer.toString(start),Integer.toString(end)));
  }
  this.leftBound=start;
  this.rightBound=end;
  matchResult.reset(null,start,end);
  appendPos=0;
  replacement=null;
  return this;
}
