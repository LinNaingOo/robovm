{
  if (dest != this && dest != other)   dest.clear();
  if (dest != this) {
    Iterator thisIt=toList().iterator();
    while (thisIt.hasNext())     dest.add(thisIt.next());
  }
  if (dest != other) {
    Iterator otherIt=other.toList().iterator();
    while (otherIt.hasNext())     dest.add(otherIt.next());
  }
}
