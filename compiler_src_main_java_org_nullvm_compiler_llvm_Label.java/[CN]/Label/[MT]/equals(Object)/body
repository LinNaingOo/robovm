{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  Label other=(Label)obj;
  if (tag == null) {
    if (other.tag != null) {
      return false;
    }
  }
 else   if (!tag.equals(other.tag)) {
    return false;
  }
  return true;
}
