{
  if (type.tag == T_ARRAY) {
    ArrayType other=(ArrayType)type;
    if (other.elementType == elementType) {
      return;
    }
 else     if (elementType.storageClass == T_ADDR) {
      elementType.checkAssignmentFrom(other.elementType);
      return;
    }
  }
  throw new IncompatibleTypesException(type,this);
}
