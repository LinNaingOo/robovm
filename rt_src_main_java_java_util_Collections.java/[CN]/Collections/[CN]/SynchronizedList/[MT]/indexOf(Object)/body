{
  final int size;
  final Object[] array;
synchronized (mutex) {
    size=list.size();
    array=new Object[size];
    list.toArray(array);
  }
  if (null != object)   for (int i=0; i < size; i++) {
    if (object.equals(array[i])) {
      return i;
    }
  }
 else {
    for (int i=0; i < size; i++) {
      if (null == array[i]) {
        return i;
      }
    }
  }
  return -1;
}
