{
  Invokestatic invokestatic=new Invokestatic(getInternalName(sootMethod.getDeclaringClass()),marshalerClassName,"toNative",String.format("(Ljava/lang/Object;J%s)V",arrayDimensionsDescriptor(dimensions.length)));
  trampolines.add(invokestatic);
  Variable handle=fn.newVariable(I64);
  fn.add(new Ptrtoint(handle,destPtr,I64));
  List<Value> args=new ArrayList<>();
  args.add(env);
  args.add(object);
  args.add(handle.ref());
  args.addAll(arrayDimensionsValues(dimensions));
  call(fn,invokestatic.getFunctionRef(),args);
}
