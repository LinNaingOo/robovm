{
  if (buf.hasArray()) {
    assertNotNull(buf.array());
  }
 else {
    if (buf.isReadOnly()) {
      try {
        buf.array();
        fail("Should throw Exception");
      }
 catch (      UnsupportedOperationException e) {
      }
    }
 else {
      try {
        buf.array();
        fail("Should throw Exception");
      }
 catch (      UnsupportedOperationException e) {
      }
    }
  }
}
