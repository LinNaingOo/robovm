{
  byte array[]=null;
  try {
    ByteBuffer.wrap(array,-1,0);
    fail("Should throw NPE");
  }
 catch (  NullPointerException e) {
  }
  try {
    ByteBuffer.wrap(new byte[10],Integer.MAX_VALUE,2);
    fail("Should throw IndexOutOfBoundsException");
  }
 catch (  IndexOutOfBoundsException e) {
  }
}
