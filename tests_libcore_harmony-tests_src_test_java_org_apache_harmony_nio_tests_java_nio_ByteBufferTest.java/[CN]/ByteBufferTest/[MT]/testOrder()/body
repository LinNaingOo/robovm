{
  assertEquals(ByteOrder.BIG_ENDIAN,buf.order());
  buf.order(ByteOrder.LITTLE_ENDIAN);
  assertEquals(ByteOrder.LITTLE_ENDIAN,buf.order());
  buf.order(ByteOrder.BIG_ENDIAN);
  assertEquals(ByteOrder.BIG_ENDIAN,buf.order());
  buf.order((ByteOrder)null);
  assertEquals(ByteOrder.LITTLE_ENDIAN,buf.order());
  buf.order(ByteOrder.BIG_ENDIAN);
}
