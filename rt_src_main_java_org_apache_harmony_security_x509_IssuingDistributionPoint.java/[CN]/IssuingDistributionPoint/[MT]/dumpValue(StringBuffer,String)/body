{
  buffer.append(prefix).append("Issuing Distribution Point: [\n");
  if (distributionPoint != null) {
    distributionPoint.dumpValue(buffer,"  " + prefix);
  }
  buffer.append(prefix).append("  onlyContainsUserCerts: ").append(onlyContainsUserCerts).append('\n');
  buffer.append(prefix).append("  onlyContainsCACerts: ").append(onlyContainsCACerts).append('\n');
  if (onlySomeReasons != null) {
    onlySomeReasons.dumpValue(buffer,prefix + "  ");
  }
  buffer.append(prefix).append("  indirectCRL: ").append(indirectCRL).append('\n');
  buffer.append(prefix).append("  onlyContainsAttributeCerts: ").append(onlyContainsAttributeCerts).append('\n');
}
