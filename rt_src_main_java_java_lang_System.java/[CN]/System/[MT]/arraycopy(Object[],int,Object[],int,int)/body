{
  arraycopyCheckBounds(src.length,srcPos,dst.length,dstPos,length);
  if (length > 0) {
    if (src != dst || srcPos > dstPos || srcPos + length <= dstPos) {
      for (int i=0; i < length; ++i) {
        dst[dstPos + i]=src[srcPos + i];
      }
    }
 else {
      for (int i=length - 1; i >= 0; --i) {
        dst[dstPos + i]=src[srcPos + i];
      }
    }
  }
}
