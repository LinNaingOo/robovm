{
  pos++;
  beg=pos;
  end=beg;
  while (true) {
    if (pos == length) {
      throw new IOException(Messages.getString("security.192"));
    }
    if (chars[pos] == '"') {
      pos++;
      break;
    }
 else     if (chars[pos] == '\\') {
      chars[end]=getEscaped();
    }
 else {
      chars[end]=chars[pos];
    }
    pos++;
    end++;
  }
  for (; pos < length && chars[pos] == ' '; pos++) {
  }
  return new String(chars,beg,end - beg);
}
