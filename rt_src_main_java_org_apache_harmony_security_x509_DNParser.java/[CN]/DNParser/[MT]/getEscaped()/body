{
  pos++;
  if (pos == length) {
    throw new IOException(Messages.getString("security.192"));
  }
switch (chars[pos]) {
case '"':
case '\\':
    hasQE=true;
case ',':
case '=':
case '+':
case '<':
case '>':
case '#':
case ';':
case ' ':
case '*':
case '%':
case '_':
  return chars[pos];
default :
return getUTF8();
}
}
