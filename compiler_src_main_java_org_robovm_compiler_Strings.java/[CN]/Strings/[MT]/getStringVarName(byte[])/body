{
  StringBuilder sb=new StringBuilder("str_");
  for (int i=0; i < bytes.length; i++) {
    byte b=bytes[i];
    if (b >= 'a' && b <= 'z' || b >= 'A' && b <= 'Z' || b >= '0' && b <= '9') {
      sb.append((char)b);
    }
 else {
      sb.append(String.format("_%02X",b));
    }
  }
  return sb.toString();
}
