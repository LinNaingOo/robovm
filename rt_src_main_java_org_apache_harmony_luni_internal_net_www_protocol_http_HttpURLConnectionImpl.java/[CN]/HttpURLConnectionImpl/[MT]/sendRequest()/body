{
  byte[] request=createRequest();
  if (!connected) {
    connect();
  }
  if (null != cacheResponse) {
    return true;
  }
  socketOut.write(request);
  sentRequest=true;
  if (os != null && os.isCached()) {
    socketOut.write(os.toByteArray());
  }
  if (os == null || os.isCached()) {
    readServerResponse();
    return true;
  }
  return false;
}
