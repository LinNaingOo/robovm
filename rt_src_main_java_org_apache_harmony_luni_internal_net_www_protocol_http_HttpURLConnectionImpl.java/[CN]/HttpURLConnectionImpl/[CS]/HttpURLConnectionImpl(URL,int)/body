{
  super(url);
  defaultPort=port;
  reqHeader=(Header)defaultReqHeader.clone();
  try {
    uri=url.toURI();
  }
 catch (  URISyntaxException e) {
  }
  responseCache=AccessController.doPrivileged(new PrivilegedAction<ResponseCache>(){
    public ResponseCache run(){
      return ResponseCache.getDefault();
    }
  }
);
}
