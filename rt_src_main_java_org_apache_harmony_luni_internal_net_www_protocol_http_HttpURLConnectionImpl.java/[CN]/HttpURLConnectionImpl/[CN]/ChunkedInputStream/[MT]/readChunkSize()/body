{
  if (atEnd) {
    return;
  }
  if (bytesRemaining == 0) {
    readln();
  }
  String size=readln();
  int index=size.indexOf(";");
  if (index >= 0) {
    size=size.substring(0,index);
  }
  bytesRemaining=Integer.parseInt(size.trim(),16);
  if (bytesRemaining == 0) {
    atEnd=true;
    readHeaders();
  }
}
