{
  PurityGraphBox b=new PurityGraphBox();
  String c=method.getDeclaringClass().toString();
  String m=method.getName();
  b.g=PurityGraph.conservativeGraph(method,true);
  String[][] o=PurityInterproceduralAnalysis.pureMethods;
  for (  String[] element : o)   if (m.equals(element[1]) && c.startsWith(element[0]))   b.g=PurityGraph.freshGraph(method);
  o=PurityInterproceduralAnalysis.alterMethods;
  for (  String[] element : o)   if (m.equals(element[1]) && c.startsWith(element[0]))   b.g=PurityGraph.conservativeGraph(method,false);
  return b;
}
