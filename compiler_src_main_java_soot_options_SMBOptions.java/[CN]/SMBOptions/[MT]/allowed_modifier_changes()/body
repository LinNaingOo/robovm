{
  String s=soot.PhaseOptions.getString(options,"allowed-modifier-changes");
  if (s.equalsIgnoreCase("unsafe"))   return allowed_modifier_changes_unsafe;
  if (s.equalsIgnoreCase("safe"))   return allowed_modifier_changes_safe;
  if (s.equalsIgnoreCase("none"))   return allowed_modifier_changes_none;
  throw new RuntimeException("Invalid value " + s + " of phase option allowed-modifier-changes");
}
