{
  ByteOrder or=ByteOrder.nativeOrder();
  String cmpTo=createString(str,2 * len);
  if ((byteOrder == 1 && or == ByteOrder.BIG_ENDIAN) || (byteOrder == 2 && or == ByteOrder.LITTLE_ENDIAN)) {
    return strObj.compareTo(cmpTo);
  }
  try {
    if (byteOrder == 1) {
      cmpTo=new String(cmpTo.getBytes("UTF-16BE"),"UTF-16BE");
    }
 else {
      cmpTo=new String(cmpTo.getBytes("UTF-16LE"),"UTF-16LE");
    }
  }
 catch (  UnsupportedEncodingException e) {
    throw new InternalError(Messages.getString("misc.6"));
  }
  return strObj.compareTo(cmpTo);
}
