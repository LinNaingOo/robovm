{
  buffer.append(prefix).append("PolicyConstraints: [\n");
  if (requireExplicitPolicy != null) {
    buffer.append(prefix).append("  requireExplicitPolicy: ").append(requireExplicitPolicy).append('\n');
  }
  if (inhibitPolicyMapping != null) {
    buffer.append(prefix).append("  inhibitPolicyMapping: ").append(inhibitPolicyMapping).append('\n');
  }
  buffer.append(prefix).append("]\n");
}
