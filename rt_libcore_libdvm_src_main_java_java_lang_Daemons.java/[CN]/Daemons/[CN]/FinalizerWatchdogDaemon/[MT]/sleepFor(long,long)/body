{
  while (true) {
    long elapsedNanos=System.nanoTime() - startNanos;
    long sleepNanos=durationNanos - elapsedNanos;
    long sleepMills=sleepNanos / NANOS_PER_MILLI;
    if (sleepMills <= 0) {
      return;
    }
    try {
      Thread.sleep(sleepMills);
    }
 catch (    InterruptedException e) {
      if (!isRunning()) {
        return;
      }
    }
  }
}
