{
  super();
  if (in == null) {
    throw new NullPointerException(Messages.getString("auth.2C"));
  }
  try {
    dn=(Name)Name.ASN1.decode(in);
  }
 catch (  IOException e) {
    IllegalArgumentException iae=new IllegalArgumentException(Messages.getString("auth.2B"));
    iae.initCause(e);
    throw iae;
  }
}
