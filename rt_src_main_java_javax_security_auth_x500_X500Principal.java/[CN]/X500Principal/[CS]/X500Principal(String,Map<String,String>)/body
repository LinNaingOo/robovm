{
  super();
  if (name == null) {
    throw new NullPointerException(Messages.getString("auth.00"));
  }
  try {
    dn=new Name(substituteNameFromMap(name,keywordMap));
  }
 catch (  IOException e) {
    IllegalArgumentException iae=new IllegalArgumentException(Messages.getString("auth.2D"));
    iae.initCause(e);
    throw iae;
  }
}
