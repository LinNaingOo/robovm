{
  if (o == null)   return false;
  final Object[] items=this.items;
  final ReentrantLock lock=this.lock;
  lock.lock();
  try {
    if (count > 0) {
      final int putIndex=this.putIndex;
      int i=takeIndex;
      do {
        if (o.equals(items[i])) {
          removeAt(i);
          return true;
        }
      }
 while ((i=inc(i)) != putIndex);
    }
    return false;
  }
  finally {
    lock.unlock();
  }
}
