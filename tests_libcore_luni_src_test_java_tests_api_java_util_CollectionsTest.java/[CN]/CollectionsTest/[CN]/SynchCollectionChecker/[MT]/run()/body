{
  while (numberOfChecks < totalToRun) {
synchronized (col) {
      if (!(col.isEmpty() || col.containsAll(normalCountingList) || col.containsAll(offsetCountingList)))       result=false;
      col.clear();
    }
    if (offset)     col.addAll(offsetCountingList);
 else     col.addAll(normalCountingList);
    numberOfChecks++;
  }
}
