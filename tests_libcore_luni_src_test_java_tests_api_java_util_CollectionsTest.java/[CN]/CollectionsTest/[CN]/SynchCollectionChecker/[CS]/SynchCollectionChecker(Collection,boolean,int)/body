{
  totalToRun=totalChecks;
  col=c;
  colSize=c.size();
  normalCountingList=new ArrayList(colSize);
  offsetCountingList=new ArrayList(colSize);
  for (int counter=0; counter < colSize; counter++)   normalCountingList.add(new Integer(counter));
  for (int counter=0; counter < colSize; counter++)   offsetCountingList.add(new Integer(counter + colSize));
  col.clear();
  if (offset)   col.addAll(offsetCountingList);
 else   col.addAll(normalCountingList);
}
