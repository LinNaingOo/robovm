{
  if (types == null) {
    types=new String[certificate_types.length];
    for (int i=0; i < types.length; i++) {
switch (certificate_types[i]) {
case 1:
        types[i]="RSA";
      break;
case 2:
    types[i]="DSA";
  break;
case 3:
types[i]="DH_RSA";
break;
case 4:
types[i]="DH_DSA";
break;
default :
fatalAlert(AlertProtocol.DECODE_ERROR,"DECODE ERROR: incorrect CertificateRequest");
}
}
}
return types;
}
