{
synchronized (lock) {
    if (isOpen()) {
      if (count <= 0) {
        return 0;
      }
      long skipped=0;
      if (count < this.count - pos) {
        pos=pos + (int)count;
        skipped=count;
      }
 else {
        skipped=this.count - pos;
        pos=this.count;
      }
      return skipped;
    }
    throw new IOException("StringReader is closed");
  }
}
