{
  boolean different=false;
  final int mx=s.length();
  char[] chars=new char[mx];
  for (int i=0; i < mx; i++) {
    char ch=s.charAt(i);
    if ('a' <= ch && ch <= 'z') {
      ch=(char)(ch + ('A' - 'a'));
      different=true;
    }
    chars[i]=ch;
  }
  final String upper;
  if (different)   upper=String.valueOf(chars);
 else   upper=s;
  return upper;
}
