{
  for (int i=0; i < intCtorTestParameters.length; i++) {
    ECFieldF2mDomainParams tp=intCtorTestParameters[i];
    try {
      new ECFieldF2m(tp.m);
      if (tp.x != null) {
        fail(getName() + ", set " + i+ " FAILED: expected exception has not been thrown");
      }
    }
 catch (    Exception e) {
      if (tp.x == null || !e.getClass().isInstance(tp.x)) {
        fail(getName() + ", set " + i+ " FAILED: unexpected "+ e);
      }
    }
  }
}
