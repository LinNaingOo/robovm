{
  if (!visited.contains(node)) {
    visited.add(node);
    for (    Node child : node.strongEdges) {
      visitReachableNodes(child,visited,treeShakerMode);
    }
    if (treeShakerMode == TreeShakerMode.none) {
      for (      Node child : node.weakEdges) {
        visitReachableNodes(child,visited,treeShakerMode);
      }
    }
  }
}
