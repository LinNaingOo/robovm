{
  String key=owner + "." + name+ desc;
  MethodNode node=methodNodes.get(key);
  if (node == null) {
    node=new MethodNode(owner,name,desc,weaklyLinked,stronglyLinked);
    methodNodes.put(key,node);
  }
 else {
    if (weaklyLinked) {
      node.weaklyLinked=true;
    }
    if (stronglyLinked) {
      node.stronglyLinked=true;
    }
  }
  return node;
}
