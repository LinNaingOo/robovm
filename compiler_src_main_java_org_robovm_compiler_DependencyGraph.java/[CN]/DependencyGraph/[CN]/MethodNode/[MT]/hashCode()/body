{
  final int prime=31;
  int result=1;
  result=prime * result + ((desc == null) ? 0 : desc.hashCode());
  result=prime * result + ((name == null) ? 0 : name.hashCode());
  result=prime * result + ((owner == null) ? 0 : owner.hashCode());
  result=prime * result + (weaklyLinked ? 1231 : 1237);
  return result;
}
