{
  assertLines("A\nB\nC","A","B","C");
  assertLines("A\rB\rC","A","B","C");
  assertLines("A\r\nB\r\nC","A","B","C");
  assertLines("A\n\rB\n\rC","A","","B","","C");
  assertLines("A\n\nB\n\nC","A","","B","","C");
  assertLines("A\r\rB\r\rC","A","","B","","C");
  assertLines("A\n\n","A","");
  assertLines("A\n\r","A","");
  assertLines("A\r\r","A","");
  assertLines("A\r\n","A");
  assertLines("A\r\n\r\n","A","");
}
