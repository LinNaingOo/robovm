{
  int outputSize=bao.size();
  oos.writeInt(42);
  assertTrue("Test 1: Primitive data unexpectedly written to the target stream.",bao.size() == outputSize);
  oos.close();
  assertTrue("Test 2: Primitive data has not been written to the the target stream.",bao.size() > outputSize);
  try {
    oos_ioe.close();
    fail("Test 3: IOException expected.");
  }
 catch (  IOException e) {
  }
}
