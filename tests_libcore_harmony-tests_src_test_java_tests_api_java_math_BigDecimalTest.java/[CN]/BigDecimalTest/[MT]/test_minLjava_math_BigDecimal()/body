{
  BigDecimal min1=new BigDecimal(-12345.4D);
  BigDecimal min2=new BigDecimal(-12345.39D);
  assertTrue("-12345.39 should have been returned",min1.min(min2).equals(min1));
  min1=new BigDecimal(value2,5);
  min2=new BigDecimal(value2,0);
  assertTrue("123345.6 should have been returned",min1.min(min2).equals(min1));
}
