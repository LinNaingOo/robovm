{
switch (atype) {
case 4:
    return BooleanType.v();
case 5:
  return CharType.v();
case 6:
return FloatType.v();
case 7:
return DoubleType.v();
case 8:
return ByteType.v();
case 9:
return ShortType.v();
case 10:
return IntType.v();
case 11:
return LongType.v();
default :
throw new RuntimeException("Undefined 'atype' in NEWARRAY byte instruction");
}
}
