{
switch (opcode) {
case Opcodes.ALOAD:
    push1(loadr(var));
  break;
case Opcodes.ASTORE:
store(pop1("op"),var);
break;
case Opcodes.ILOAD:
push1(loadi(var));
break;
case Opcodes.ISTORE:
store(pop1("op"),var);
break;
case Opcodes.LLOAD:
push2(loadl(var));
break;
case Opcodes.LSTORE:
store(pop2("op"),var);
break;
case Opcodes.FLOAD:
push1(loadf(var));
break;
case Opcodes.FSTORE:
store(pop1("op"),var);
break;
case Opcodes.DLOAD:
push2(loadd(var));
break;
case Opcodes.DSTORE:
store(pop2("op"),var);
break;
case Opcodes.RET:
Var ret=load("i8*",var);
out.format("    indirectbr i8* %s\n",ret);
break;
default :
throw new RuntimeException("Opcode not implemented: " + opcodeNames[opcode]);
}
}
