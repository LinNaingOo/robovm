{
  if (page1 == null) {
    page1=new NewJavaProjectWizardPageOne(){
      @Override public String getCompilerCompliance(){
        return JavaCore.VERSION_1_5;
      }
      @Override public IClasspathEntry[] getDefaultClasspathEntries(){
        return new IClasspathEntry[]{JavaCore.newContainerEntry(new Path(NullVMClasspathContainer.ID))};
      }
      @Override protected Control createJRESelectionControl(      Composite composite){
        return new Label(composite,NONE);
      }
    }
;
    page1.setTitle(page1.getTitle().replace("Java","NullVM"));
  }
  addPage(page1);
  if (page2 == null) {
    page2=new NewJavaProjectWizardPageTwo(page1);
  }
  addPage(page2);
}
