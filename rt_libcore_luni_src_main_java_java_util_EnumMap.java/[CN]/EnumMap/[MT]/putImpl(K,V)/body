{
  if (key == null) {
    throw new NullPointerException("key == null");
  }
  keyType.cast(key);
  int keyOrdinal=key.ordinal();
  if (!hasMapping[keyOrdinal]) {
    hasMapping[keyOrdinal]=true;
    mappingsCount++;
  }
  V oldValue=(V)values[keyOrdinal];
  values[keyOrdinal]=value;
  return oldValue;
}
