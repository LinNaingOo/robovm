{
  RSAMulti rsam=new RSAMulti(publicExponent,primeExponentP,primeExponentQ,primeP,primeQ,crtCoefficient,null);
  try {
    assertNull("Object RSAOtherPrimeInfo is not NULL",rsam.getOtherPrimeInfo());
  }
 catch (  Exception e) {
    fail("Unexpected exception: " + e);
  }
  rsam=new RSAMulti(publicExponent,primeExponentP,primeExponentQ,primeP,primeQ,crtCoefficient,opi);
  try {
    assertEquals(rsam.getOtherPrimeInfo(),opi);
  }
 catch (  Exception e) {
    fail("Unexpected exception: " + e);
  }
}
