{
  if (c.isArray()) {
    return c.getName().replace('.','/').toCharArray();
  }
  if (c.isPrimitive()) {
    if (c == void.class) {
      return new char[]{'V'};
    }
    if (c == int.class) {
      return new char[]{'I'};
    }
    if (c == boolean.class) {
      return new char[]{'Z'};
    }
    if (c == byte.class) {
      return new char[]{'B'};
    }
    if (c == char.class) {
      return new char[]{'C'};
    }
    if (c == short.class) {
      return new char[]{'S'};
    }
    if (c == long.class) {
      return new char[]{'J'};
    }
    if (c == float.class) {
      return new char[]{'F'};
    }
    if (c == double.class) {
      return new char[]{'D'};
    }
  }
  return ("L" + c.getName().replace('.','/') + ";").toCharArray();
}
