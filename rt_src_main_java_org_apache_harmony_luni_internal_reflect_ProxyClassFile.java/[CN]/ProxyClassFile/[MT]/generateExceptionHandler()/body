{
  writeUnsignedByte(OPC_astore_0);
  writeUnsignedByte(OPC_aload_0);
  writeUnsignedByte(OPC_getstatic);
  writeUnsignedShort(constantPool.literalIndex("java/lang/System","err",PrintStream.class));
  Method m=null;
