    m=Exception.class.getMethod("printStackTrace",new Class[]{PrintStream.class});
  }
 catch (  Exception e) {
    e.printStackTrace(System.err);
  }
  writeUnsignedByte(OPC_invokevirtual);
  writeUnsignedShort(constantPool.literalIndex(m));
}
