{
  try {
    hw1.substring(-1,hw1.length());
    fail("IndexOutOfBoundsException was not thrown.");
  }
 catch (  IndexOutOfBoundsException ioobe) {
  }
  try {
    hw1.substring(Integer.MAX_VALUE,hw1.length());
    fail("IndexOutOfBoundsException was not thrown.");
  }
 catch (  IndexOutOfBoundsException ioobe) {
  }
  try {
    hw1.substring(0,Integer.MAX_VALUE);
    fail("IndexOutOfBoundsException was not thrown.");
  }
 catch (  IndexOutOfBoundsException ioobe) {
  }
}
