{
  assertFalse("Returned true for negative offset.",hw1.regionMatches(-1,hw2,2,5));
  assertFalse("Returned true for negative offset.",hw1.regionMatches(2,hw2,-1,5));
  assertFalse("Returned true for toffset+len is greater than the length.",hw1.regionMatches(5,hw2,2,6));
  assertFalse("Returned true for ooffset+len is greater than the length.",hw1.regionMatches(2,hw2,5,6));
}
