{
  int i=s.length();
  if (i == 0 || i > Manifest.LINE_LENGTH_LIMIT - 2) {
    throw new IllegalArgumentException();
  }
  name=new byte[i];
  for (; --i >= 0; ) {
    char ch=s.charAt(i);
    if (!((ch >= 'a' && ch <= 'z') || (ch >= 'A' && ch <= 'Z') || ch == '_' || ch == '-' || (ch >= '0' && ch <= '9'))) {
      throw new IllegalArgumentException(s);
    }
    name[i]=(byte)ch;
  }
}
