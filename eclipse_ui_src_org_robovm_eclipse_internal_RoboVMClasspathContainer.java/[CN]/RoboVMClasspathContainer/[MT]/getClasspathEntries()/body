{
  File f=RoboVMPlugin.getRoboVMHome().getRtPath();
  IPath sourceAttachment=null;
  if (f.isFile()) {
    sourceAttachment=new Path(new File(f.getParentFile(),"robovm-rt-sources.jar").getAbsolutePath());
  }
 else {
    sourceAttachment=new Path(new File(f.getParentFile().getParent(),"src/main/java").getAbsolutePath());
  }
  return new IClasspathEntry[]{JavaCore.newLibraryEntry(new Path(f.getAbsolutePath()),sourceAttachment,new Path(""),new IAccessRule[]{},new IClasspathAttribute[]{},false)};
}
