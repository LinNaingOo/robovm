{
  byte[] seed=new byte[LENGTH];
  byte[] bytes1=new byte[LENGTH];
  byte[] bytes2=new byte[LENGTH];
  boolean b;
  for (int i=0; i < seed.length; i++) {
    seed[i]=(byte)i;
  }
  sr.setSeed(seed);
  sr.setSeed(seed);
  sr2.setSeed(seed);
  sr.nextBytes(bytes1);
  sr2.nextBytes(bytes2);
  b=true;
  for (int j=0; j < bytes1.length; j++) {
    b&=bytes1[j] == bytes2[j];
  }
  assertFalse("unexpected: sequences are equal",b);
}
