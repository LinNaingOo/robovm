{
  if (null == value) {
    for (int i=0; i < enumSize; i++) {
      if (hasMapping[i] && null == values[i]) {
        return true;
      }
    }
  }
 else {
    for (int i=0; i < enumSize; i++) {
      if (hasMapping[i] && value.equals(values[i])) {
        return true;
      }
    }
  }
  return false;
}
