{
  setPhantomRefs(true);
  if (!getPhantomRefs() && SourceLocator.v().getClassSource(className) == null) {
    setPhantomRefs(false);
    return null;
  }
  SootResolver resolver=SootResolver.v();
  SootClass toReturn=resolver.resolveClass(className,desiredLevel);
  setPhantomRefs(false);
  return toReturn;
}
