{
  if (!c.isInScene())   throw new RuntimeException();
  classes.remove(c);
  if (c.isLibraryClass()) {
    libraryClasses.remove(c);
  }
 else   if (c.isPhantomClass()) {
    phantomClasses.remove(c);
  }
 else   if (c.isApplicationClass()) {
    applicationClasses.remove(c);
  }
  c.getType().setSootClass(null);
  c.setInScene(false);
  modifyHierarchy();
}
