{
  assertTrue("database supports TYPE_FORWARD_ONLY type",meta.supportsResultSetType(ResultSet.TYPE_FORWARD_ONLY));
  assertFalse("database doesn't support TYPE_SCROLL_INSENSITIVE type",meta.supportsResultSetType(ResultSet.TYPE_SCROLL_INSENSITIVE));
  assertFalse("database supports TYPE_SCROLL_SENSITIVE type",meta.supportsResultSetType(ResultSet.TYPE_SCROLL_SENSITIVE));
  assertFalse("database supports unknown type",meta.supportsResultSetType(100));
}
