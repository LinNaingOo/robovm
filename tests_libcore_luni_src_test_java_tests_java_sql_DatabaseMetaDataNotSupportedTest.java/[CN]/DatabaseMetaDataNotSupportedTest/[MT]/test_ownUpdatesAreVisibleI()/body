{
  assertTrue("result set's own updates are visible for TYPE_FORWARD_ONLY type",meta.ownUpdatesAreVisible(ResultSet.TYPE_FORWARD_ONLY));
  assertTrue("result set's own updates are visible for TYPE_SCROLL_INSENSITIVE type",meta.ownUpdatesAreVisible(ResultSet.TYPE_SCROLL_INSENSITIVE));
  assertTrue("result set's own updates are visible for TYPE_SCROLL_SENSITIVE type",meta.ownUpdatesAreVisible(ResultSet.TYPE_SCROLL_SENSITIVE));
  assertFalse("result set's own updates are visible for unknown type",meta.ownUpdatesAreVisible(100));
}
