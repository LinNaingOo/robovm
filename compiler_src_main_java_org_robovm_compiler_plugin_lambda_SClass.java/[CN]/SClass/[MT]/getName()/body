{
  if (isPrimitive()) {
switch (getDescriptor().charAt(0)) {
case 'Z':
      return "boolean";
case 'B':
    return "byte";
case 'S':
  return "short";
case 'C':
return "char";
case 'I':
return "int";
case 'J':
return "long";
case 'F':
return "float";
case 'D':
return "double";
default :
return "void";
}
}
 else if (isArray()) {
return getDescriptor();
}
 else {
String d=getDescriptor();
d=d.substring(1,d.length() - 1);
return d.replace('/','.');
}
}
