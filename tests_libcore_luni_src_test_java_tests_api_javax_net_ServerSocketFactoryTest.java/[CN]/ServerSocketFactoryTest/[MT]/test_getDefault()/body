{
  ServerSocketFactory sf=ServerSocketFactory.getDefault();
  ServerSocket s;
  try {
    s=sf.createServerSocket(0);
    s.close();
  }
 catch (  IOException e) {
  }
  try {
    s=sf.createServerSocket(0,50);
    s.close();
  }
 catch (  IOException e) {
  }
  try {
    s=sf.createServerSocket(0,50,InetAddress.getLocalHost());
    s.close();
  }
 catch (  IOException e) {
  }
}
