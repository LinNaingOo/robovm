{
  Object obj;
  observable.addObserver(observer=new TestObserver());
  observable.notifyObservers();
  assertEquals("Notified when unchanged",0,((TestObserver)observer).updateCount());
  ((TestObservable)observable).doChange();
  observable.notifyObservers(obj=new Object());
  assertEquals("Failed to notify",1,((TestObserver)observer).updateCount());
  assertTrue("Failed to pass Object arg",((TestObserver)observer).objv.elementAt(0).equals(obj));
}
