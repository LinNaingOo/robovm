{
  int i, j;
  i=index % 4;
  if (i != 0)   pad=(byte)(4 - i);
 else   pad=(byte)0;
  index+=pad;
  default_offset=getInt(bc,index);
  index+=4;
  npairs=getInt(bc,index);
  index+=4;
  if (npairs > 0) {
    match_offsets=new int[npairs * 2];
    j=0;
    do {
      match_offsets[j]=getInt(bc,index);
      j++;
      index+=4;
      match_offsets[j]=getInt(bc,index);
      index+=4;
      j++;
    }
 while (j < npairs * 2);
  }
  return index;
}
