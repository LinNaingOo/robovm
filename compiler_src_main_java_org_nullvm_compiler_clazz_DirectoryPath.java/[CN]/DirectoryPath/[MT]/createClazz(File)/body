{
  return new Clazz(f.getAbsolutePath().substring(dir.getAbsolutePath().length() + 1),this){
    byte[] bytes=null;
    public byte[] getBytes() throws IOException {
      if (bytes == null) {
        bytes=FileUtils.readFileToByteArray(f);
      }
      return bytes;
    }
    public long lastModified(){
      return f.lastModified();
    }
  }
;
}
