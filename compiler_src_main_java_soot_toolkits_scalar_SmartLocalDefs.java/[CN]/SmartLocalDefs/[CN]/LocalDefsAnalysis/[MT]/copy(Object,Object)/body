{
  HashSet sourceSet=(HashSet)source;
  HashSet<Object> destSet=(HashSet<Object>)dest;
  if (destSet.size() > 0)   destSet.retainAll(sourceSet);
  if (sourceSet.size() > 0) {
    for (Iterator its=sourceSet.iterator(); its.hasNext(); ) {
      Object o=its.next();
      if (!destSet.contains(o)) {
        destSet.add(o);
      }
    }
  }
}
