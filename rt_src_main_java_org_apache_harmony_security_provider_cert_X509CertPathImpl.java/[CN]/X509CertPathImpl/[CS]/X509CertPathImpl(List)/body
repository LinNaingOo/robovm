{
  super("X.509");
  int size=certs.size();
  certificates=new ArrayList(size);
  for (int i=0; i < size; i++) {
    Object cert=certs.get(i);
    if (!(cert instanceof X509Certificate)) {
      throw new CertificateException(Messages.getString("security.15D"));
    }
    certificates.add(cert);
  }
}
