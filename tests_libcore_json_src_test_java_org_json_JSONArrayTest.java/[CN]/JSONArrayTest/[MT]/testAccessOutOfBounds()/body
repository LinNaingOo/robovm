{
  JSONArray array=new JSONArray();
  array.put("foo");
  assertEquals(null,array.opt(3));
  assertEquals(null,array.opt(-3));
  assertEquals("",array.optString(3));
  assertEquals("",array.optString(-3));
  try {
    array.get(3);
    fail();
  }
 catch (  JSONException e) {
  }
  try {
    array.get(-3);
    fail();
  }
 catch (  JSONException e) {
  }
  try {
    array.getString(3);
    fail();
  }
 catch (  JSONException e) {
  }
  try {
    array.getString(-3);
    fail();
  }
 catch (  JSONException e) {
  }
}
