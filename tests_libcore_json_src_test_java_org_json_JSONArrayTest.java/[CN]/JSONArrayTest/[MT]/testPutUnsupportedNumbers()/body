{
  JSONArray array=new JSONArray();
  try {
    array.put(Double.NaN);
    fail();
  }
 catch (  JSONException e) {
  }
  try {
    array.put(0,Double.NEGATIVE_INFINITY);
    fail();
  }
 catch (  JSONException e) {
  }
  try {
    array.put(0,Double.POSITIVE_INFINITY);
    fail();
  }
 catch (  JSONException e) {
  }
}
