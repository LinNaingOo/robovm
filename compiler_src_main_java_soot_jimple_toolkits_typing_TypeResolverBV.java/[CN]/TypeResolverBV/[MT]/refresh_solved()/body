{
  unsolved=new BitVector();
  for (BitSetIterator varIt=unsolved.iterator(); varIt.hasNext(); ) {
    final TypeVariableBV var=typeVariableForId(varIt.next());
    if (var.depth() == 0) {
      if (var.type() == null) {
        unsolved.set(var.id());
      }
 else {
        solved.set(var.id());
      }
    }
  }
}
