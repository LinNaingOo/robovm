{
  TreeSet<TypeVariableBV> workList=new TreeSet<TypeVariableBV>();
  for (  TypeVariableBV var : typeVariableList) {
    if (var.type() != null) {
      workList.add(var);
    }
  }
  TypeVariableBV.computeApprox(workList);
  for (  TypeVariableBV var : typeVariableList) {
    if (var.approx() == NULL) {
      var.union(typeVariable(NULL));
    }
 else     if (var.approx() == null) {
      var.union(typeVariable(NULL));
    }
  }
}
