{
  super.compose(sroot);
  StylesheetRoot.ComposeState cstate=sroot.getComposeState();
  java.util.Vector vnames=cstate.getVariableNames();
  if (null != m_name_avt)   m_name_avt.fixupVariables(vnames,cstate.getGlobalsSize());
  if (null != m_namespace_avt)   m_namespace_avt.fixupVariables(vnames,cstate.getGlobalsSize());
}
