{
  URL base=new URL("http://host/a/b/c");
  assertEquals("http://",new URL(base,"http://").toString());
  assertEquals("http://",new URL(base,"//").toString());
  assertEquals("https:",new URL(base,"https:").toString());
  assertEquals("https:/",new URL(base,"https:/").toString());
  assertEquals("https://",new URL(base,"https://").toString());
}
