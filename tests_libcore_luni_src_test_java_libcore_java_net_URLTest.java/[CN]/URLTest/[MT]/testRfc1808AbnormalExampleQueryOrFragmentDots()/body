{
  URL base=new URL("http://a/b/c/d;p?q");
  assertEquals("http://a/b/c/g?y/./x",new URL(base,"g?y/./x").toString());
  assertEquals("http://a/b/c/g?y/../x",new URL(base,"g?y/../x").toString());
  assertEquals("http://a/b/c/g#s/./x",new URL(base,"g#s/./x").toString());
  assertEquals("http://a/b/c/g#s/../x",new URL(base,"g#s/../x").toString());
}
