{
  try {
    PrintWriter file=new PrintWriter(new FileOutputStream(new File(output_dir,"types")));
    for (Iterator it=Scene.v().getApplicationClasses().iterator(); it.hasNext(); ) {
      handleClass(file,(SootClass)it.next());
    }
    for (Iterator it=Scene.v().getLibraryClasses().iterator(); it.hasNext(); ) {
      handleClass(file,(SootClass)it.next());
    }
    file.close();
  }
 catch (  IOException e) {
    throw new RuntimeException("Couldn't dump reaching types." + e);
  }
}
