{
  s=new Scanner("test");
  try {
    s.useRadix(Character.MIN_RADIX - 1);
    fail();
  }
 catch (  IllegalArgumentException expected) {
  }
  try {
    s.useRadix(Character.MAX_RADIX + 1);
    fail();
  }
 catch (  IllegalArgumentException expected) {
  }
  s.useRadix(11);
  assertEquals(11,s.radix());
}
