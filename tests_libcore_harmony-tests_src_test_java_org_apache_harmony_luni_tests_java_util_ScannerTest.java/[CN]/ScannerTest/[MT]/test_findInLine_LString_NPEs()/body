{
  s=new Scanner("test");
  try {
    s.findInLine((String)null);
    fail();
  }
 catch (  NullPointerException expected) {
  }
  s.close();
  try {
    s.findInLine((String)null);
    fail();
  }
 catch (  NullPointerException expected) {
  }
  try {
    s.findInLine("test");
    fail();
  }
 catch (  IllegalStateException expected) {
  }
}
