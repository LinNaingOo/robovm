{
  if (o == null)   return false;
  final E[] items=this.items;
  final ReentrantLock lock=this.lock;
  lock.lock();
  try {
    int i=takeIndex;
    int k=0;
    while (k++ < count) {
      if (o.equals(items[i]))       return true;
      i=inc(i);
    }
    return false;
  }
  finally {
    lock.unlock();
  }
}
