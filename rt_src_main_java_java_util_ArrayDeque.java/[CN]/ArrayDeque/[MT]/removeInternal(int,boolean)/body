{
  int cursor=current;
  if (frontShift) {
    while (cursor != front) {
      int next=circularSmallerPos(cursor);
      elements[cursor]=elements[next];
      cursor=next;
    }
    front=circularBiggerPos(front);
  }
 else {
    while (cursor != rear) {
      int next=circularBiggerPos(cursor);
      elements[cursor]=elements[next];
      cursor=next;
    }
    rear=circularSmallerPos(rear);
  }
  elements[cursor]=null;
  resetStatus(false);
}
