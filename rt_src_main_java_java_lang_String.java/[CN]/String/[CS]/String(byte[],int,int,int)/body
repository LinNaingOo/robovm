{
  if (data != null) {
    if (start >= 0 && 0 <= length && length <= data.length - start) {
      offset=0;
      value=new char[length];
      count=length;
      high<<=8;
      for (int i=0; i < count; i++) {
        value[i]=(char)(high + (data[start++] & 0xff));
      }
    }
 else {
      throw new StringIndexOutOfBoundsException();
    }
  }
 else {
    throw new NullPointerException();
  }
}
