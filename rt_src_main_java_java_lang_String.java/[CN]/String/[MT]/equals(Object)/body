{
  if (object == this) {
    return true;
  }
  if (object instanceof String) {
    String s=(String)object;
    int hash=hashCode;
    int shash=s.hashCode;
    if (count != s.count || (hash != shash && hash != 0 && shash != 0)) {
      return false;
    }
    for (int i=0; i < count; ++i) {
      if (value[offset + i] != s.value[s.offset + i]) {
        return false;
      }
    }
    return true;
  }
  return false;
}
