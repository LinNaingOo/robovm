{
  if (0 <= start && start <= end && end <= count) {
    end+=offset;
    try {
      for (int i=offset + start; i < end; i++) {
        data[index++]=(byte)value[i];
      }
    }
 catch (    ArrayIndexOutOfBoundsException e) {
      throw new StringIndexOutOfBoundsException();
    }
  }
 else {
    throw new StringIndexOutOfBoundsException();
  }
}
