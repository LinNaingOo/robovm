{
  if (this.getClass() != Field.class) {
    throw new InvalidObjectException(Messages.getString("text.0C"));
  }
  if (calendarField != -1) {
    try {
      Field result=ofCalendarField(calendarField);
      if (result != null && this.getName().equals(result.getName())) {
        return result;
      }
    }
 catch (    IllegalArgumentException e) {
      throw new InvalidObjectException(Messages.getString("text.02"));
    }
  }
 else {
    if (this.equals(TIME_ZONE)) {
      return TIME_ZONE;
    }
    if (this.equals(HOUR1)) {
      return HOUR1;
    }
    if (this.equals(HOUR_OF_DAY1)) {
      return HOUR_OF_DAY1;
    }
  }
  throw new InvalidObjectException(Messages.getString("text.02"));
}
