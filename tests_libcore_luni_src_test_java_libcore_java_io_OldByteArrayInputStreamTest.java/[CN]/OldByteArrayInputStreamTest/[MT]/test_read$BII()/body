{
  byte[] buf1=new byte[20];
  is.skip(50);
  is.mark(100);
  is.read(buf1,0,buf1.length);
  assertTrue("Test 1: Failed to read correct data.",new String(buf1,0,buf1.length).equals(fileString.substring(50,70)));
  try {
    is.read(null,1,0);
    fail("Test 2: NullPointerException expected.");
  }
 catch (  NullPointerException e) {
  }
  try {
    is.read(buf1,-1,1);
    fail("Test 3: IndexOutOfBoundsException expected.");
  }
 catch (  IndexOutOfBoundsException e) {
  }
  try {
    is.read(buf1,1,-1);
    fail("Test 4: IndexOutOfBoundsException expected.");
  }
 catch (  IndexOutOfBoundsException e) {
  }
  try {
    is.read(buf1,1,buf1.length);
    fail("Test 5: IndexOutOfBoundsException expected.");
  }
 catch (  IndexOutOfBoundsException e) {
  }
}
