{
  byte[] buf1=new byte[10];
  byte[] buf2=new byte[10];
  try {
    is.mark(200);
    is.read(buf1,0,10);
    is.reset();
    is.read(buf2,0,10);
    is.reset();
    assertTrue("Reset failed",new String(buf1,0,buf1.length).equals(new String(buf2,0,buf2.length)));
  }
 catch (  Exception e) {
    fail("Exception during reset test : " + e.getMessage());
  }
}
