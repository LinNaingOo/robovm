{
  Matcher mat;
  Pattern pat;
  pat=Pattern.compile("((aa|bb)??).*cc");
  mat=pat.matcher("aacc");
  assertTrue(pat.toString() + " vs: " + "aacc",mat.matches());
  assertEquals("",mat.group(1));
  pat=Pattern.compile("((aa|bb)??)cc");
  mat=pat.matcher("aacc");
  assertTrue(pat.toString() + " vs: " + "aacc",mat.matches());
  assertEquals("aa",mat.group(1));
}
