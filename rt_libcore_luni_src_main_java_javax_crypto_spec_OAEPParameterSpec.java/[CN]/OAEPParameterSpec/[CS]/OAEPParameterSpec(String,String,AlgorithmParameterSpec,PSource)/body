{
  if (mdName == null) {
    throw new NullPointerException("mdName == null");
  }
 else   if (mgfName == null) {
    throw new NullPointerException("mgfName == null");
  }
 else   if (pSrc == null) {
    throw new NullPointerException("pSrc == null");
  }
  this.mdName=mdName;
  this.mgfName=mgfName;
  this.mgfSpec=mgfSpec;
  this.pSrc=pSrc;
}
