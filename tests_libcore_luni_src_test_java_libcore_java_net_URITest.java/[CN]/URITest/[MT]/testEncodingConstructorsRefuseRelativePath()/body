{
  try {
    new URI("http","host","relative",null);
    fail();
  }
 catch (  URISyntaxException expected) {
  }
  try {
    new URI("http","host","relative",null,null);
    fail();
  }
 catch (  URISyntaxException expected) {
  }
  try {
    new URI("http",null,"host",-1,"relative",null,null);
    fail();
  }
 catch (  URISyntaxException expected) {
  }
}
