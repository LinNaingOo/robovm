{
  URI base=new URI("http://a/b/c/d;p?q");
  assertEquals("http://a/g",base.resolve("/./g").toString());
  assertEquals("http://a/g",base.resolve("/../g").toString());
  assertEquals("http://a/b/c/g.",base.resolve("g.").toString());
  assertEquals("http://a/b/c/.g",base.resolve(".g").toString());
  assertEquals("http://a/b/c/g..",base.resolve("g..").toString());
  assertEquals("http://a/b/c/..g",base.resolve("..g").toString());
}
