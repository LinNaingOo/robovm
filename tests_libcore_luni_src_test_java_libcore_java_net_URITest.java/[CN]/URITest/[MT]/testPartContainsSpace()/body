{
  try {
    new URI("ht tp://host/");
    fail();
  }
 catch (  URISyntaxException expected) {
  }
  try {
    new URI("http://user name@host/");
    fail();
  }
 catch (  URISyntaxException expected) {
  }
  try {
    new URI("http://ho st/");
    fail();
  }
 catch (  URISyntaxException expected) {
  }
  try {
    new URI("http://host:80 80/");
    fail();
  }
 catch (  URISyntaxException expected) {
  }
  try {
    new URI("http://host/fi le");
    fail();
  }
 catch (  URISyntaxException expected) {
  }
  try {
    new URI("http://host/file?que ry");
    fail();
  }
 catch (  URISyntaxException expected) {
  }
  try {
    new URI("http://host/file?query#re f");
    fail();
  }
 catch (  URISyntaxException expected) {
  }
}
