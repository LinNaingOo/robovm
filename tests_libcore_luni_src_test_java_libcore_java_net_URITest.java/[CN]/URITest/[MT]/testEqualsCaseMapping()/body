{
  assertEquals(new URI("HTTP://localhost/foo?bar=baz#quux"),new URI("HTTP://localhost/foo?bar=baz#quux"));
  assertEquals(new URI("http://localhost/foo?bar=baz#quux"),new URI("http://LOCALHOST/foo?bar=baz#quux"));
  assertFalse(new URI("http://localhost/foo?bar=baz#quux").equals(new URI("http://localhost/FOO?bar=baz#quux")));
  assertFalse(new URI("http://localhost/foo?bar=baz#quux").equals(new URI("http://localhost/foo?BAR=BAZ#quux")));
  assertFalse(new URI("http://localhost/foo?bar=baz#quux").equals(new URI("http://localhost/foo?bar=baz#QUUX")));
}
