{
  if (streamHandle == -1) {
    throw new IllegalStateException();
  }
  if (off <= buf.length && nbytes >= 0 && off >= 0 && buf.length - off >= nbytes) {
    inRead=0;
    inLength=nbytes;
    setInputImpl(buf,off,nbytes,streamHandle);
  }
 else {
    throw new ArrayIndexOutOfBoundsException();
  }
}
