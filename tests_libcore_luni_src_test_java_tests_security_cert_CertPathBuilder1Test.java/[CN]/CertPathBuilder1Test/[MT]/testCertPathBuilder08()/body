{
  if (!PKIXSupport) {
    fail(NotSupportMsg);
    return;
  }
  Provider prov=null;
  for (int t=0; t < validValues.length; t++) {
    try {
      CertPathBuilder.getInstance(validValues[t],prov);
      fail("IllegalArgumentException must be thrown");
    }
 catch (    IllegalArgumentException e1) {
    }
  }
}
