{
  if (!PKIXSupport) {
    fail(NotSupportMsg);
    return null;
  }
  try {
    CertPathBuilder[] certPBs=new CertPathBuilder[3];
    certPBs[0]=CertPathBuilder.getInstance(defaultType);
    certPBs[1]=CertPathBuilder.getInstance(defaultType,defaultProviderName);
    certPBs[2]=CertPathBuilder.getInstance(defaultType,defaultProvider);
    return certPBs;
  }
 catch (  Exception e) {
    return null;
  }
}
