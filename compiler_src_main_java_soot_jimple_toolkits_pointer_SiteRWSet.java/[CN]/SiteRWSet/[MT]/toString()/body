{
  boolean empty=true;
  final StringBuffer ret=new StringBuffer();
  ret.append("SiteRWSet: ");
  for (Iterator<RWSet> keyIt=sets.iterator(); keyIt.hasNext(); ) {
    final Object key=keyIt.next();
    ret.append(key.toString());
    empty=false;
  }
  if (empty)   ret.append("empty");
  return ret.toString();
}
