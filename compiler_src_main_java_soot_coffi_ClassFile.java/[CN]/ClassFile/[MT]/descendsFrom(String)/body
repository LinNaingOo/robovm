{
  cp_info cf;
  int i;
  cf=constant_pool[super_class];
  if (cf.toString(constant_pool).compareTo(cname) == 0)   return true;
  for (i=0; i < interfaces_count; i++) {
    cf=constant_pool[interfaces[i]];
    if (cf.toString(constant_pool).compareTo(cname) == 0)     return true;
  }
  return false;
}
