{
  initOis(FILENAME);
  Support_GetPutFields object=(Support_GetPutFields)ois.readObject();
  Support_GetPutFields newObject=new Support_GetPutFields();
  newObject.initTestValues();
  assertTrue("Test 1: The object read from the reference file does " + "not match a locally created instance of the same class.",object.equals(newObject));
  initOis(DEFAULTED_FILENAME);
  Support_GetPutFieldsDefaulted defaulted=(Support_GetPutFieldsDefaulted)ois.readObject();
  Support_GetPutFieldsDefaulted newDefaulted=new Support_GetPutFieldsDefaulted();
  newDefaulted.initTestValues();
  assertTrue("Test 2: The object read from the reference file does " + "not match a locally created instance of the same class.",defaulted.equals(newDefaulted));
}
