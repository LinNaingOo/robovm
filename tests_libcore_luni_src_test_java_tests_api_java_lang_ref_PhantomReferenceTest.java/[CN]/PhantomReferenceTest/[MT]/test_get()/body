{
  ReferenceQueue rq=new ReferenceQueue();
  bool=new Boolean(false);
  PhantomReference pr=new PhantomReference(bool,rq);
  assertNull("get() should return null.",pr.get());
  pr.enqueue();
  assertNull("get() should return null.",pr.get());
  pr.clear();
  assertNull("get() should return null.",pr.get());
}
