{
  if (sameType(otherFlow) && sameType(destFlow)) {
    ArrayPackedSet other=(ArrayPackedSet)otherFlow;
    ArrayPackedSet dest=(ArrayPackedSet)destFlow;
    if (!(other instanceof ArrayPackedSet) || bits.length != other.bits.length)     throw new RuntimeException("Incompatible other set for union");
    for (int i=0; i < bits.length; i++)     dest.bits[i]=this.bits[i] | other.bits[i];
  }
 else   super.union(otherFlow,destFlow);
}
