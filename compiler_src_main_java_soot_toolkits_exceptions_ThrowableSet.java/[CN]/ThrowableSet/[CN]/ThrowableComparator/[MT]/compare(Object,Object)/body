{
  RefType t1=baseType(o1);
  RefType t2=baseType(o2);
  if (t1.equals(t2)) {
    if (o1 instanceof AnySubType) {
      if (o2 instanceof AnySubType) {
        return 0;
      }
 else {
        return -1;
      }
    }
 else     if (o2 instanceof AnySubType) {
      return 1;
    }
 else {
      return 0;
    }
  }
 else {
    return t1.toString().compareTo(t2.toString());
  }
}
