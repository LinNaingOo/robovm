{
  HashMap inMap1=(HashMap)in1;
  HashMap inMap2=(HashMap)in2;
  HashMap outMap=(HashMap)o;
  for (  Local l : locals) {
    Set l1=(Set)inMap1.get(l), l2=(Set)inMap2.get(l);
    Set out=(Set)outMap.get(l);
    out.clear();
    if (l1.contains(UNKNOWN) || l2.contains(UNKNOWN)) {
      out.add(UNKNOWN);
    }
 else {
      out.addAll(l1);
      out.addAll(l2);
    }
  }
}
