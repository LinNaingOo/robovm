{
  this.entry=entry;
  LoginModuleControlFlag flg=entry.getControlFlag();
  if (flg == LoginModuleControlFlag.OPTIONAL) {
    flag=OPTIONAL;
  }
 else   if (flg == LoginModuleControlFlag.REQUISITE) {
    flag=REQUISITE;
  }
 else   if (flg == LoginModuleControlFlag.SUFFICIENT) {
    flag=SUFFICIENT;
  }
 else {
    flag=REQUIRED;
  }
}
