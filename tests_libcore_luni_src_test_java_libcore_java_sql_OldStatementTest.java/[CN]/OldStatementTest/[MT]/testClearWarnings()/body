{
  Statement st=null;
  try {
    st=conn.createStatement();
    st.execute("select animals from zoo");
  }
 catch (  SQLException e) {
  }
 finally {
    try {
      st.close();
    }
 catch (    SQLException ee) {
    }
  }
  try {
    st=conn.createStatement();
    st.clearWarnings();
    SQLWarning w=st.getWarnings();
    assertNull(w);
  }
  finally {
    try {
      st.close();
    }
 catch (    SQLException ee) {
    }
  }
}
