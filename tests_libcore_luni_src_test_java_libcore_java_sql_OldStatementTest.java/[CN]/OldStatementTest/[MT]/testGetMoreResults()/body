{
  Statement st=null;
  ResultSet res1=null;
  ResultSet res2=null;
  String[] queries={"insert into zoo values (3,'John','bird');","update zoo set name='Masha', family='cat' where id=3;","update zoo set name='Masha', family='bear' where id=3;"};
  try {
    st=conn.createStatement();
    st.execute(queries[0]);
    assertFalse(st.getMoreResults());
    try {
      st.getResultSet();
      fail("Exception expected");
    }
 catch (    SQLException e) {
    }
  }
  finally {
    try {
      st.close();
    }
 catch (    SQLException ee) {
    }
  }
  try {
    st.getMoreResults();
    fail("Exception expected");
  }
 catch (  SQLException e) {
  }
}
