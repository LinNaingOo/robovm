{
  try {
    testBuffer.deleteCharAt(testBuffer.length() + 1);
    fail("StringIndexOutOfBoundsException was not thrown.");
  }
 catch (  StringIndexOutOfBoundsException sioobe) {
  }
  try {
    testBuffer.deleteCharAt(-1);
    fail("StringIndexOutOfBoundsException was not thrown.");
  }
 catch (  StringIndexOutOfBoundsException sioobe) {
  }
}
