{
  try {
    testBuffer.insert(-1,"");
    fail("StringIndexOutOfBoundsException is not thrown.");
  }
 catch (  StringIndexOutOfBoundsException sioobe) {
  }
  try {
    testBuffer.insert(testBuffer.length() + 1,"");
    fail("StringIndexOutOfBoundsException is not thrown.");
  }
 catch (  StringIndexOutOfBoundsException sioobe) {
  }
}
