{
  byte b;
  b=bytes[curindex++];
  if ((b & ((byte)0x80)) == 0) {
    c=b;
  }
 else   if ((b & ((byte)0xe0)) == 0xc0) {
    c=((b & ((byte)0x1f))) << 6;
    b=bytes[curindex++];
    c|=(b & ((byte)0x3f));
  }
 else {
    c=((b & ((byte)0x0f))) << 12;
    b=bytes[curindex++];
    c|=((b & ((byte)0x3f))) << 6;
    b=bytes[curindex++];
    c|=(b & ((byte)0x3f));
  }
  return this;
}
