{
  byte[] p=new byte[]{1,2,3,4,5};
  PSource.PSpecified ps=new PSource.PSpecified(p);
  byte[] result=ps.getValue();
  if (!Arrays.equals(p,result)) {
    fail("The returned array does not equal to the specified " + "in the constructor.");
  }
  result[0]++;
  assertFalse("The change of returned by getValue() array " + "should not cause the change of internal array.",result[0] == ps.getValue()[0]);
}
