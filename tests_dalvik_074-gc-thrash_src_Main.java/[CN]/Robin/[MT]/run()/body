{
  Main.startupDelay();
  String strings[]=new String[ARRAY_SIZE];
  int idx=0;
  while (!Main.quit) {
    strings[idx]=makeString(idx);
    if (idx % (ARRAY_SIZE / 4) == 0) {
      Main.sleep(400);
      sleepCount++;
    }
    idx=(idx + 1) % ARRAY_SIZE;
  }
  if (Main.DEBUG)   System.out.println("Robin: sleepCount=" + sleepCount);
}
