{
  Robin robin=new Robin();
  Deep deep=new Deep();
  Large large=new Large();
  robin.start();
  deep.start();
  large.start();
  sleep(TEST_TIME * 1000);
  quit=true;
  try {
    robin.join();
    deep.join();
    large.join();
  }
 catch (  InterruptedException ie) {
    System.err.println("join was interrupted");
  }
}
