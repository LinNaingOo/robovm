{
  assertEquals("[-126, -96]",Arrays.toString("???".getBytes("Shift_JIS")));
  assertEquals("[-126, -87]",Arrays.toString("???".getBytes("Shift_JIS")));
  assertEquals("[-105, 67]",Arrays.toString("???".getBytes("Shift_JIS")));
  assertEquals("[36]",Arrays.toString("$".getBytes("Shift_JIS")));
  assertEquals("[-29, -127, -117]",Arrays.toString("???".getBytes("UTF-8")));
}
