{
  String[] isoLang=Locale.getISOLanguages();
  int length=isoLang.length;
  assertTrue("Random element in wrong format.",(isoLang[length / 2].length() == 2 || isoLang[length / 2].length() == 3) && isoLang[length / 2].toLowerCase().equals(isoLang[length / 2]));
  assertTrue("Wrong number of ISOLanguages.",length > 130);
}
