{
  buffer[offset++]=(byte)(val >> 56);
  buffer[offset++]=(byte)(val >> 48);
  buffer[offset++]=(byte)(val >> 40);
  buffer[offset++]=(byte)(val >> 32);
  buffer[offset++]=(byte)(val >> 24);
  buffer[offset++]=(byte)(val >> 16);
  buffer[offset++]=(byte)(val >> 8);
  buffer[offset++]=(byte)val;
  return offset;
}
