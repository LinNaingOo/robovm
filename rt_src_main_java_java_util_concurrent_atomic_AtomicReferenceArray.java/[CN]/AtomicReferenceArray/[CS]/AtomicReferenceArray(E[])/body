{
  if (array == null)   throw new NullPointerException();
  int length=array.length;
  this.array=new Object[length];
  if (length > 0) {
    int last=length - 1;
    for (int i=0; i < last; ++i)     this.array[i]=array[i];
    E e=array[last];
    unsafe.putObjectVolatile(this.array,rawIndex(last),e);
  }
}
