{
  int size=sources.size();
  String name=null;
  for (int i=0; i < size; i++) {
    DOMImplementationSource source=(DOMImplementationSource)sources.elementAt(i);
    DOMImplementation impl=source.getDOMImplementation(features);
    if (impl != null) {
      return impl;
    }
  }
  return null;
}
