{
  return isJRE11() ? null : (ClassLoader)AccessController.doPrivileged(new PrivilegedAction(){
    public Object run(){
      ClassLoader classLoader=null;
      try {
        classLoader=Thread.currentThread().getContextClassLoader();
      }
 catch (      SecurityException ex) {
      }
      return classLoader;
    }
  }
);
}
