{
  try {
    File srcRoot=path.getFile();
    if (path.getFile().isFile()) {
      srcRoot=srcRoot.getParentFile();
    }
    return new File(makeFileRelativeTo(llvmCacheDir,srcRoot.getCanonicalFile()),getArchiveName(path) + ".classes");
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
