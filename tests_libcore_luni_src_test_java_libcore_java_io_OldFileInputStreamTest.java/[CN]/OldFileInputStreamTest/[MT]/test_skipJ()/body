{
  byte[] buf1=new byte[10];
  is=new FileInputStream(fileName);
  is.skip(1000);
  is.read(buf1,0,buf1.length);
  assertTrue("Test 1: Failed to skip to correct position.",new String(buf1,0,buf1.length).equals(fileString.substring(1000,1010)));
  is.close();
  try {
    is.read();
    fail("Test 2: IOException expected.");
  }
 catch (  IOException e) {
