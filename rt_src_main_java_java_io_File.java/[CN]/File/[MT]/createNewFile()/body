{
  SecurityManager security=System.getSecurityManager();
  if (security != null) {
    security.checkWrite(path);
  }
  if (0 == path.length()) {
    throw new IOException(Messages.getString("luni.B3"));
  }
  int result=newFileImpl(properPath(true));
switch (result) {
case 0:
    return true;
case 1:
  return false;
default :
throw new IOException(Messages.getString("luni.B4",path));
}
}
