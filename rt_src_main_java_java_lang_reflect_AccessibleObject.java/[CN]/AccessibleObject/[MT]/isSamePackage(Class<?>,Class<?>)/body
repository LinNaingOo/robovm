{
  if (c1 == c2) {
    return true;
  }
  if (c1.getClassLoader() != c2.getClassLoader()) {
    return false;
  }
  String n1=c1.getName();
  String n2=c2.getName();
  int dot1=n1.lastIndexOf('.');
  int dot2=n2.lastIndexOf('.');
  if (dot1 == -1) {
    return dot2 == -1;
  }
  if (dot1 != dot2) {
    return false;
  }
  return n1.substring(0,dot1).equals(n2.substring(0,dot2));
}
