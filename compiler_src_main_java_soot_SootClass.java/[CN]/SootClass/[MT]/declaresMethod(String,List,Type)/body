{
  checkLevel(SIGNATURES);
  Iterator<SootMethod> methodIt=methodIterator();
  while (methodIt.hasNext()) {
    SootMethod method=methodIt.next();
    if (method.getName().equals(name) && method.getParameterTypes().equals(parameterTypes) && method.getReturnType().equals(returnType))     return true;
  }
  return false;
}
