{
  if (!checkUpperBound(start)) {
    throw new IllegalArgumentException();
  }
  if (fromStart && ((!loInclusive && (startKeyInclusive || (endKeyInclusive && start.equals(end)))) ? keyCompare(start,lo) <= 0 : keyCompare(start,lo) < 0) || (toEnd && ((!hiInclusive && (endKeyInclusive)) ? keyCompare(end,hi) >= 0 : keyCompare(end,hi) > 0))) {
    throw new IllegalArgumentException();
  }
  if (keyCompare(start,end) > 0) {
    throw new IllegalArgumentException();
  }
  return new DescendingSubMap<K,V>(start,startKeyInclusive,m,end,endKeyInclusive);
}
