{
  boolean isInRange=true;
  int result;
  if (map.toEnd) {
    result=(null != comparator()) ? comparator().compare(start.getKey(),map.hi) : toComparable(start.getKey()).compareTo(map.hi);
    isInRange=(map.hiInclusive || !startInclusive) ? result <= 0 : result < 0;
  }
  if (map.fromStart) {
    result=(null != comparator()) ? comparator().compare(start.getKey(),map.lo) : toComparable(start.getKey()).compareTo(map.lo);
    isInRange=isInRange && ((map.loInclusive || !startInclusive) ? result >= 0 : result > 0);
  }
  if (isInRange) {
    return new AscendingSubMapEntrySet<K,V>(map,start,startInclusive,null,false);
  }
  throw new IllegalArgumentException();
}
