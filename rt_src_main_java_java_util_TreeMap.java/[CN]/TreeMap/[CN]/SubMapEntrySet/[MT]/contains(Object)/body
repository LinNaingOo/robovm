{
  if (object instanceof Map.Entry) {
    Map.Entry<K,V> entry=(Map.Entry<K,V>)object;
    K key=entry.getKey();
    if (subMap.isInRange(key)) {
      V v1=subMap.get(key), v2=entry.getValue();
      return v1 == null ? (v2 == null && subMap.containsKey(key)) : v1.equals(v2);
    }
  }
  return false;
}
