{
  if (expectedModCount != backingMap.modCount) {
    throw new ConcurrentModificationException();
  }
 else   if (node == null) {
    throw new NoSuchElementException();
  }
  lastNode=node;
  lastOffset=offset;
  if (offset != node.right_idx) {
    offset++;
  }
 else {
    node=node.next;
    if (node != null) {
      offset=node.left_idx;
    }
  }
}
