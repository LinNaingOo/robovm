{
  if (value == null ? node.value == null : value.equals(node.value)) {
    return true;
  }
  if (node.left != null) {
    if (containsValue(node.left,value)) {
      return true;
    }
  }
  if (node.right != null) {
    if (containsValue(node.right,value)) {
      return true;
    }
  }
  return false;
}
