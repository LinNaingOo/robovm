{
  char[] ch="Android".toCharArray();
  try {
    parent.characters(ch,2,5);
  }
 catch (  SAXException e) {
    throw new RuntimeException("Unexpected exception",e);
  }
  assertEquals(logger.size(),1);
  assertEquals("characters",logger.getMethod());
