{
  System.out.println("FloatMath.convTest");
  float f;
  double d;
  int i;
  long l;
  f=1234.5678f;
  i=(int)f;
  assert(i == 1234);
  f=-1234.5678f;
  i=(int)f;
  assert(i == -1234);
  f=1238.5678f;
  l=(long)f;
  assert(l == 1238);
  f=-1238.5678f;
  l=(long)f;
  assert(l == -1238);
  f=1238.5678f;
  d=(double)f;
  assert(d > 1238.567 && d < 1238.568);
  d=1234.5678;
  i=(int)d;
  assert(i == 1234);
  d=-1234.5678;
  i=(int)d;
  assert(i == -1234);
  d=5678956789.0123;
  l=(long)d;
  assert(l == 5678956789L);
  d=-5678956789.0123;
  l=(long)d;
  assert(l == -5678956789L);
  d=1238.5678;
  f=(float)d;
  assert(f > 1238.567 && f < 1238.568);
  i=7654;
  l=(long)i;
  assert(l == 7654L);
  i=-7654;
  l=(long)i;
  assert(l == -7654L);
  i=1234;
  f=(float)i;
  assert(f > 1233.9f && f < 1234.1f);
  i=-1234;
  f=(float)i;
  assert(f < -1233.9f && f > -1234.1f);
  i=1238;
  d=(double)i;
  assert(d > 1237.9f && d < 1238.1f);
  i=-1238;
  d=(double)i;
  assert(d < -1237.9f && d > -1238.1f);
  l=5678956789L;
  i=(int)l;
  assert(i == 1383989493);
  l=-5678956789L;
  i=(int)l;
  assert(i == -1383989493);
  l=5678956789L;
  f=(float)l;
  assert(f > 5.6789564E9 && f < 5.6789566E9);
  l=-5678956789L;
  f=(float)l;
  assert(f < -5.6789564E9 && f > -5.6789566E9);
  l=6678956789L;
  d=(double)l;
  assert(d > 6.6789567E9 && d < 6.6789568E9);
  l=-6678956789L;
  d=(double)l;
  assert(d < -6.6789567E9 && d > -6.6789568E9);
}
