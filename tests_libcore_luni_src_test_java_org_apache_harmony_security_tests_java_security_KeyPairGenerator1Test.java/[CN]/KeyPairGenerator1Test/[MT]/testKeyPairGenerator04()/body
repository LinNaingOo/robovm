{
  if (!DSASupported) {
    fail(NotSupportMsg);
    return;
  }
  for (int i=0; i < algs.length; i++) {
    for (int j=1; j < invalidValues.length; j++) {
      try {
        KeyPairGenerator.getInstance(algs[i],invalidValues[j]);
        fail("NoSuchProviderException must be thrown (algorithm: ".concat(algs[i]).concat(" provider: ").concat(invalidValues[j]).concat(")"));
      }
 catch (      NoSuchProviderException e) {
      }
    }
  }
}
