{
  if (null == record) {
    throw new NullPointerException();
  }
  if (this.level.intValue() == Level.OFF.intValue()) {
    return false;
  }
 else   if (record.getLevel().intValue() >= this.level.intValue()) {
    return null == this.filter || this.filter.isLoggable(record);
  }
  return false;
}
