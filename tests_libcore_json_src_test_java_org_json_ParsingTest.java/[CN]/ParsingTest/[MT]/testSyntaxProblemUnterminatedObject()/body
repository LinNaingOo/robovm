{
  assertParseFail("{");
  assertParseFail("{\"foo\"");
  assertParseFail("{\"foo\":");
  assertParseFail("{\"foo\":bar");
  assertParseFail("{\"foo\":bar,");
  assertParseFail("{\"foo\":bar,\"baz\"");
  assertParseFail("{\"foo\":bar,\"baz\":");
  assertParseFail("{\"foo\":bar,\"baz\":true");
  assertParseFail("{\"foo\":bar,\"baz\":true,");
}
