{
  if (index >= bits.length)   throw new NoSuchElementException();
  long k=(save & (save - 1));
  long diff=save ^ k;
  save=k;
  int result=(diff < 0) ? 64 * index + 63 : 64 * index + lookup[(int)(diff % 67)];
  if (save == 0) {
    index++;
    while (index < bits.length && bits[index] == 0L)     index++;
    if (index < bits.length)     save=bits[index];
  }
  return result;
}
