{
  int sport=startServer("SServer close");
  s=new Socket(InetAddress.getLocalHost(),sport,null,0);
  try {
    s.setSoLinger(false,100);
  }
 catch (  IOException e) {
    handleException(e,SO_LINGER);
  }
  s.close();
  try {
    s.getOutputStream();
    fail("IOException was not thrown.");
  }
 catch (  java.io.IOException e) {
  }
}
