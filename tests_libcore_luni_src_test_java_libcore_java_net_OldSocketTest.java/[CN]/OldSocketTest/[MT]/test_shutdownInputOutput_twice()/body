{
  if (System.getProperty("os.name").contains("Darwin") || System.getProperty("os.name").contains("Mac")) {
    return;
  }
  Socket s=new Socket("0.0.0.0",0,false);
  s.shutdownInput();
  try {
    s.shutdownInput();
    fail("should throw SocketException");
  }
 catch (  SocketException se) {
  }
  s.shutdownOutput();
  try {
    s.shutdownOutput();
    fail("should throw SocketException");
  }
 catch (  SocketException se) {
  }
}
