{
switch (option) {
case SO_MULTICAST:
    return "Multicast";
case SO_LINGER:
  return "Linger";
case SO_RCVBUF:
return "Receive buffer size";
case SO_TIMEOUT:
return "Socket timeout";
case SO_SNDBUF:
return "Send buffer size";
case TCP_NODELAY:
return "TCP no delay";
case SO_KEEPALIVE:
return "Keepalive";
case SO_REUSEADDR:
return "Reuse address";
case SO_OOBINLINE:
return "out of band data inline";
case IP_TOS:
return "Traffic class";
case SO_BROADCAST:
return "broadcast";
case SO_USELOOPBACK:
return "loopback";
}
return "Unknown socket option";
}
