{
  RandomAccessFile raf=new java.io.RandomAccessFile(fileName,"rw");
  raf.write(testString.getBytes());
  assertEquals("Test 1: Incorrect length returned. ",testLength,raf.length());
  raf.close();
  try {
    raf.length();
    fail("Test 2: IOException expected.");
  }
 catch (  IOException e) {
