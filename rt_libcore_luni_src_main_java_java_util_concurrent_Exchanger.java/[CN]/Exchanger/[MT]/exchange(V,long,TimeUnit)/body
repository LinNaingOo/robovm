{
  Object v;
  Object item=(x == null) ? NULL_ITEM : x;
  long ns=unit.toNanos(timeout);
  if ((arena != null || (v=slotExchange(item,true,ns)) == null) && ((Thread.interrupted() || (v=arenaExchange(item,true,ns)) == null)))   throw new InterruptedException();
  if (v == TIMED_OUT)   throw new TimeoutException();
  return (v == NULL_ITEM) ? null : (V)v;
}
