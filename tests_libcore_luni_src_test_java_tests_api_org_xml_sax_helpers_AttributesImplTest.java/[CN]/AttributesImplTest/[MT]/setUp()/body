{
  multi.addAttribute("http://some.uri","foo","ns1:foo","string","abc");
  multi.addAttribute("http://some.uri","bar","ns1:bar","string","xyz");
  multi.addAttribute("http://some.other.uri","answer","ns2:answer","int","42");
  multi.addAttribute("","gabbaHey","","string","1-2-3-4");
  multi.addAttribute("","","gabba:hey","string","1-2-3-4");
}
