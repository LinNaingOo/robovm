{
  if (rep != this) {
    return ecr().toString();
  }
  StringBuffer s=new StringBuffer();
  s.append(",[parents:");
{
    boolean comma=false;
    for (BitSetIterator i=parents.iterator(); i.hasNext(); ) {
      if (comma) {
        s.append(",");
      }
 else {
        comma=true;
      }
      s.append(i.next());
    }
  }
  s.append("],[children:");
{
    boolean comma=false;
    for (BitSetIterator i=children.iterator(); i.hasNext(); ) {
      if (comma) {
        s.append(",");
      }
 else {
        comma=true;
      }
      s.append(i.next());
    }
  }
  s.append("]");
  return "[id:" + id + ",depth:"+ depth+ ((type != null) ? (",type:" + type) : "")+ ",approx:"+ approx+ s+ (element == null ? "" : ",arrayof:" + element.id())+ "]";
}
