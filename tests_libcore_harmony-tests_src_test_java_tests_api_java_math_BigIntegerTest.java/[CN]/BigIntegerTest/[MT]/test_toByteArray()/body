{
  byte[] myByteArray, anotherByteArray;
  myByteArray=new byte[]{97,33,120,124,50,2,0,0,0,12,124,42};
  anotherByteArray=new BigInteger(myByteArray).toByteArray();
  assertTrue("Incorrect byte array returned",myByteArray.length == anotherByteArray.length);
  for (int counter=myByteArray.length - 1; counter >= 0; counter--) {
    assertTrue("Incorrect values in returned byte array",myByteArray[counter] == anotherByteArray[counter]);
  }
}
