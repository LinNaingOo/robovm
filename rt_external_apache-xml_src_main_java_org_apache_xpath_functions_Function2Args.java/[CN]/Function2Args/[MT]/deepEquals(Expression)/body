{
  if (!super.deepEquals(expr))   return false;
  if (null != m_arg1) {
    if (null == ((Function2Args)expr).m_arg1)     return false;
    if (!m_arg1.deepEquals(((Function2Args)expr).m_arg1))     return false;
  }
 else   if (null != ((Function2Args)expr).m_arg1)   return false;
  return true;
}
