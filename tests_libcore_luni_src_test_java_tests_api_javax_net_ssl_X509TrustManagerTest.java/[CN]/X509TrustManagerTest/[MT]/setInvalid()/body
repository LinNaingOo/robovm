{
  try {
    CertificateFactory certFact=CertificateFactory.getInstance("X.509");
    X509Certificate pemCert=(X509Certificate)certFact.generateCertificate(new ByteArrayInputStream(TestUtils.getX509Certificate_v1()));
    X509Certificate[] xcert={pemCert};
    return xcert;
  }
 catch (  Exception ex) {
    fail("Unexpected exception " + ex);
  }
  return null;
}
