{
  String value=null;
  OutputProperties props=m_outputFormat;
  value=props.getProperty(name);
  if (null == value) {
    if (!OutputProperties.isLegalPropertyKey(name))     throw new IllegalArgumentException(XSLMessages.createMessage(XSLTErrorResources.ER_OUTPUT_PROPERTY_NOT_RECOGNIZED,new Object[]{name}));
  }
  return value;
}
