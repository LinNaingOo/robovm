{
  String protocol="TLS";
  SSLContext clientContext=createSSLContext(protocol,clientProvider,client.keyManagers,client.trustManagers);
  SSLContext serverContext=createSSLContext(protocol,serverProvider,server.keyManagers,server.trustManagers);
  return create(client.keyStore,client.storePassword,server.keyStore,server.storePassword,client.keyManagers[0],server.keyManagers[0],client.trustManagers[0],server.trustManagers[0],clientContext,serverContext);
}
