{
  if (count < 0 || levelStart < 0 || objectStart < 0 || count > levels.length - levelStart || count > objects.length - objectStart) {
    throw new IllegalArgumentException(Messages.getString("text.13",new Object[]{levels.length,levelStart,objects.length,objectStart,count}));
  }
  com.ibm.icu.text.Bidi.reorderVisually(levels,levelStart,objects,objectStart,count);
}
