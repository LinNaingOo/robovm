{
  int length=icuBidi.getLength();
  if (lineStart < 0 || lineLimit < 0 || lineLimit > length || lineStart > lineLimit) {
    throw new IllegalArgumentException(Messages.getString("text.12",new Object[]{lineStart,lineLimit,length}));
  }
  Bidi bidi=new Bidi();
  bidi.icuBidi=icuBidi.createLineBidi(lineStart,lineLimit);
  return bidi;
}
