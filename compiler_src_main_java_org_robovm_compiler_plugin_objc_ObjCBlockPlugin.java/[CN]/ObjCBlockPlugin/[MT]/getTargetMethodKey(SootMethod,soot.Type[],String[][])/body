{
  StringBuilder key=new StringBuilder(Mangler.mangleMethod(Types.getInternalName(targetMethod.getDeclaringClass()),targetMethod.getName(),Arrays.asList(actualTypes).subList(1,actualTypes.length),actualTypes[0]));
  for (  String[] a : targetMethodAnnotations) {
    key.append(',');
    for (    String s : a) {
      key.append(s);
    }
  }
  return key.toString();
}
