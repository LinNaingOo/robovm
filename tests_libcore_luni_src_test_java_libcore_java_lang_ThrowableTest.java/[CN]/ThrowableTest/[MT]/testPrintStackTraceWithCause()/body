{
  Throwable throwable=newThrowable("Throwable","A","B");
  throwable.initCause(newThrowable("Cause","A","B","C","D"));
  assertEquals("java.lang.Throwable: Throwable\n" + "\tat ClassB.doB(ClassB.java:1)\n" + "\tat ClassA.doA(ClassA.java:0)\n"+ "Caused by: java.lang.Throwable: Cause\n"+ "\tat ClassD.doD(ClassD.java:3)\n"+ "\tat ClassC.doC(ClassC.java:2)\n"+ "\t... 2 more\n",printStackTraceToString(throwable));
}
