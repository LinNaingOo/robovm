{
  Vector<FieldContainer> v=new Vector<FieldContainer>();
  v.add(new FieldContainer(0,2,NumberFormat.Field.INTEGER));
  v.add(new FieldContainer(2,3,NumberFormat.Field.DECIMAL_SEPARATOR));
