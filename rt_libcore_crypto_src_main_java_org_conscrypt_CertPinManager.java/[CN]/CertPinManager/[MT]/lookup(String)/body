{
  if (!initialized) {
    return null;
  }
  if (cacheIsNotValid()) {
    rebuild();
  }
  String cn=hostnameCache.get(hostname);
  if (cn != null) {
    return entries.get(cn);
  }
  cn=getMatchingCN(hostname);
  if (cn != null) {
    hostnameCache.put(hostname,cn);
    return entries.get(cn);
  }
  return null;
}
