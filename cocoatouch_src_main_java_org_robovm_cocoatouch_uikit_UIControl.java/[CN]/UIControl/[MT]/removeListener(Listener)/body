{
  List<ListenerWrapper> listeners=getListeners(false);
  if (listeners == null) {
    return;
  }
synchronized (listeners) {
    for (Iterator<ListenerWrapper> it=listeners.iterator(); it.hasNext(); ) {
      ListenerWrapper wrapper=it.next();
      if (wrapper.listener == listener) {
        it.remove();
        break;
      }
    }
  }
}
