{
  if (permission instanceof FilePermission) {
    FilePermission fp=(FilePermission)permission;
    int matchedMask=0;
    int i=0;
    while (i < permissions.size() && ((matchedMask & fp.mask) != fp.mask)) {
      matchedMask|=((FilePermission)permissions.elementAt(i)).impliesMask(permission);
      i++;
    }
    return ((matchedMask & fp.mask) == fp.mask);
  }
  return false;
}
