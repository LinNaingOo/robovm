{
  if (nc.getMethodDesc().startsWith("()")) {
    return true;
  }
  Clazz target=config.getClazzes().load(nc.getTarget());
  int nativeCount=0;
  for (  SootMethod m : target.getSootClass().getMethods()) {
    if (m.isNative() && m.getName().equals(nc.getMethodName())) {
      nativeCount++;
    }
  }
  return nativeCount > 1;
}
