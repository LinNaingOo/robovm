{
  int index;
  char[] stringCharArray=stringConstant.toCharArray();
  if ((index=stringCache.get(stringCharArray)) < 0) {
    int stringIndex=literalIndex(stringCharArray);
    index=stringCache.put(stringCharArray,currentIndex++);
    writeU1(StringTag);
    writeU2(stringIndex);
  }
  return index;
}
