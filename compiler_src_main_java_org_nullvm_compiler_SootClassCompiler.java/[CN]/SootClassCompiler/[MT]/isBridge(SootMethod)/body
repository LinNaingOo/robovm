{
  VisibilityAnnotationTag vatag=(VisibilityAnnotationTag)sm.getTag("VisibilityAnnotationTag");
  if (vatag != null) {
    for (    AnnotationTag tag : vatag.getAnnotations()) {
      if ("Lorg/nullvm/rt/bro/annotation/Bridge;".equals(tag.getType())) {
        return true;
      }
    }
  }
  return false;
}
