{
  StringBuilder sb=new StringBuilder();
  sb.append("Java_");
  sb.append(mangleNativeString(owner));
  sb.append("_");
  sb.append(mangleNativeString(name));
  if (desc != null && !desc.startsWith("()")) {
    sb.append("__");
    sb.append(mangleNativeString(desc.substring(1,desc.lastIndexOf(')'))));
  }
  return sb.toString();
}
