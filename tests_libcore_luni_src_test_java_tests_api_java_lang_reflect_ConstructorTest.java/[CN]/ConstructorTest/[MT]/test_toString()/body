{
  Class[] parms=null;
  Constructor<? extends ConstructorTestHelper> ctor=null;
  try {
    parms=new Class[1];
    parms[0]=new Object().getClass();
    ctor=new ConstructorTestHelper().getClass().getConstructor(parms);
  }
 catch (  Exception e) {
    fail("Exception during getParameterTypes test:" + e.toString());
  }
  assertTrue("Returned incorrect string representation: " + ctor.toString(),ctor.toString().equals("public tests.api.java.lang.reflect.ConstructorTest$ConstructorTestHelper(java.lang.Object)"));
}
