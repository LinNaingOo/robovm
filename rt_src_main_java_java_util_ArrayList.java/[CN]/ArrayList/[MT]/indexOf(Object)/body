{
  int lastIndex=firstIndex + size;
  if (object != null) {
    for (int i=firstIndex; i < lastIndex; i++) {
      if (object.equals(array[i])) {
        return i - firstIndex;
      }
    }
  }
 else {
    for (int i=firstIndex; i < lastIndex; i++) {
      if (array[i] == null) {
        return i - firstIndex;
      }
    }
  }
  return -1;
}
