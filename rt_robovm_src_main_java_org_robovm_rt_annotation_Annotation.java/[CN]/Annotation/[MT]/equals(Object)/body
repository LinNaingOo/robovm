{
  if (this == o) {
    return true;
  }
  if (o == null) {
    return false;
  }
  if (this.getClass() == o.getClass()) {
    return fastEquals(o);
  }
  if (annotationType.isInstance(o)) {
    try {
      return slowEquals(o);
    }
 catch (    Throwable t) {
      return false;
    }
  }
  return false;
}
