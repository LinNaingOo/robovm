{
  int pos=findPosition(key);
  if (array[pos] == key) {
    if (values[pos] == value)     return false;
    values[pos]=value;
    return true;
  }
  size++;
  if (size * 3 > array.length * 2) {
    doubleSize();
    pos=findPosition(key);
  }
  array[pos]=key;
  values[pos]=value;
  return true;
}
