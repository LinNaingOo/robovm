{
  try {
    crypt();
  }
 catch (  Exception e) {
    if (errorSB == null) {
      errorSB=new StringBuffer();
    }
    errorSB.append(e.getMessage());
    errorSB.append("\n");
    errorSB.append(getCipherParameters());
    errorSB.append("\n");
    StackTraceElement[] st=e.getStackTrace();
    for (int i=0; i < st.length; i++) {
      errorSB.append(st[i].toString());
      errorSB.append("\n");
    }
    fails++;
    return;
  }
  flagTestResult=true;
}
