{
  byte[] localBuf=buf;
  InputStream localIn=in;
  if (localBuf == null || localIn == null) {
    throw new IOException(Messages.getString("luni.24"));
  }
  if (pos >= count && fillbuf(localIn,localBuf) == -1) {
    return -1;
  }
  if (localBuf != buf) {
    localBuf=buf;
    if (localBuf == null) {
      throw new IOException(Messages.getString("luni.24"));
    }
  }
  if (count - pos > 0) {
    return localBuf[pos++] & 0xFF;
  }
  return -1;
}
