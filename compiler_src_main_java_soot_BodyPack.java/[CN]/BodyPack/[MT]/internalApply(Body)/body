{
  for (Iterator tIt=this.iterator(); tIt.hasNext(); ) {
    final Transform t=(Transform)tIt.next();
    if (Options.v().interactive_mode()) {
      InteractionHandler.v().handleNewAnalysis(t,b);
    }
    t.apply(b);
    if (Options.v().interactive_mode()) {
      InteractionHandler.v().handleTransformDone(t,b);
    }
  }
}
