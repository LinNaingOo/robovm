{
  Iterator it=edges.get(n).iterator();
  while (it.hasNext()) {
    PurityEdge e=(PurityEdge)it.next();
    backEdges.remove(e.getTarget(),e);
  }
  it=backEdges.get(n).iterator();
  while (it.hasNext()) {
    PurityEdge e=(PurityEdge)it.next();
    edges.remove(e.getSource(),e);
  }
  it=backLocals.get(n).iterator();
  while (it.hasNext()) {
    Local l=(Local)it.next();
    locals.remove(l,n);
  }
  ret.remove(n);
  edges.remove(n);
  backEdges.remove(n);
  backLocals.remove(n);
  nodes.remove(n);
  paramNodes.remove(n);
  globEscape.remove(n);
  mutated.remove(n);
}
