{
  Iterator it=toColor.iterator();
  while (it.hasNext()) {
    PurityEdge edge=(PurityEdge)it.next();
    if (consider_inside || !edge.isInside()) {
      PurityNode node=edge.getTarget();
      if (!dest.contains(node)) {
        dest.add(node);
        internalPassEdges(edges.get(node),dest,consider_inside);
      }
    }
  }
}
