{
  if (thisName == thatName) {
    return true;
  }
  int end=thisName.length();
  if (end > thatName.length()) {
    return false;
  }
  if (thisName.charAt(--end) == '*' && (end == 0 || thisName.charAt(end - 1) == '.')) {
    end--;
  }
 else   if (end != (thatName.length() - 1)) {
    return false;
  }
  for (int i=end; i >= 0; i--) {
    if (thisName.charAt(i) != thatName.charAt(i)) {
      return false;
    }
  }
  return true;
}
