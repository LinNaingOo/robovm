{
  HashMap hm2=new HashMap();
  hm2.putAll(hm);
  for (int i=0; i < 1000; i++)   assertTrue("Failed to clear all elements",hm2.get(new Integer(i).toString()).equals((new Integer(i))));
  Map mockMap=new MockMap();
  hm2=new HashMap();
  hm2.putAll(mockMap);
  assertEquals("Size should be 0",0,hm2.size());
}
