{
  HashMap hashMap=new HashMap();
  try {
    hashMap.putAll(new MockMapNull());
    fail("Should throw NullPointerException");
  }
 catch (  NullPointerException e) {
  }
  try {
    hashMap=new HashMap(new MockMapNull());
    fail("Should throw NullPointerException");
  }
 catch (  NullPointerException e) {
  }
}
