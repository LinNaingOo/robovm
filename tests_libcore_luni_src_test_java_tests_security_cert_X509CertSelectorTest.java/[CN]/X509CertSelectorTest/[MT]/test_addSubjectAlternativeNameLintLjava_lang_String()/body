{
  int[] types={GeneralName.OTHER_NAME,GeneralName.DNS_NAME,GeneralName.X400_ADDR,GeneralName.DIR_NAME,GeneralName.EDIP_NAME,GeneralName.UR_ID,GeneralName.IP_ADDR,GeneralName.REG_ID};
  for (int i=0; i < types.length; i++) {
    try {
      new X509CertSelector().addSubjectAlternativeName(types[i],"-0xDFRF");
      fail("IOException expected for type: " + types[i]);
    }
 catch (    IOException e) {
    }
  }
}
