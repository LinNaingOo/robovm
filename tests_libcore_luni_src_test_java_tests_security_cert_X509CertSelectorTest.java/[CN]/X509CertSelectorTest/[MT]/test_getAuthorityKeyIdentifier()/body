{
  byte[] akid1=new byte[]{4,5,1,2,3,4,5};
  byte[] akid2=new byte[]{4,5,5,4,3,2,1};
  X509CertSelector selector=new X509CertSelector();
  assertNull("Selector should return null",selector.getAuthorityKeyIdentifier());
  selector.setAuthorityKeyIdentifier(akid1);
  assertTrue("The returned keyID should be equal to specified",Arrays.equals(akid1,selector.getAuthorityKeyIdentifier()));
  assertTrue("The returned keyID should be equal to specified",Arrays.equals(akid1,selector.getAuthorityKeyIdentifier()));
  assertFalse("The returned keyID should differ",Arrays.equals(akid2,selector.getAuthorityKeyIdentifier()));
}
