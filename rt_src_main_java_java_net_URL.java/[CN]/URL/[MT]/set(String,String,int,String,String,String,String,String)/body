{
  String filePart=path;
  if (query != null && !query.equals("")) {
    if (filePart != null) {
      filePart=filePart + "?" + query;
    }
 else {
      filePart="?" + query;
    }
  }
  set(protocol,host,port,filePart,ref);
  this.authority=authority;
  this.userInfo=userInfo;
  this.path=path;
  this.query=query;
}
