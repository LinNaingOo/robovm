{
  if (name == null) {
    throw new NullPointerException(Messages.getString("logging.1C"));
  }
  this.name=name;
  this.value=level;
  this.resourceBundleName=resourceBundleName;
  if (resourceBundleName != null) {
    try {
      rb=ResourceBundle.getBundle(resourceBundleName,Locale.getDefault(),VM.callerClassLoader());
    }
 catch (    MissingResourceException e) {
      rb=null;
    }
  }
synchronized (levels) {
    levels.add(this);
  }
}
