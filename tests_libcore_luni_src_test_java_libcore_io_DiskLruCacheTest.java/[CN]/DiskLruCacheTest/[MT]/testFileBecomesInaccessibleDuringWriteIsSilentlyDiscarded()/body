{
  set("A","a","a");
  DiskLruCache.Editor editor=cache.edit("A");
  OutputStream out0=editor.newOutputStream(0);
  out0.write('b');
  out0.close();
  OutputStream out1=editor.newOutputStream(1);
  out1.write('c');
  mockOs.enqueueFault("write");
  out1.write('c');
  out1.close();
  editor.commit();
  assertAbsent("A");
}
