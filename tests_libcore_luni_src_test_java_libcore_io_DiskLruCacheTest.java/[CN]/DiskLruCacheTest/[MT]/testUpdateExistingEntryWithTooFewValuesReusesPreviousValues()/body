{
  DiskLruCache.Editor creator=cache.edit("k1");
  creator.set(0,"A");
  creator.set(1,"B");
  creator.commit();
  DiskLruCache.Editor updater=cache.edit("k1");
  updater.set(0,"C");
  updater.commit();
  DiskLruCache.Snapshot snapshot=cache.get("k1");
  assertEquals("C",snapshot.getString(0));
  assertEquals("B",snapshot.getString(1));
  snapshot.close();
}
