{
  Writer writer=new FileWriter(journalFile);
  writer.write(magic + "\n");
  writer.write(version + "\n");
  writer.write(appVersion + "\n");
  writer.write(valueCount + "\n");
  writer.write(blank + "\n");
  for (  String line : bodyLines) {
    writer.write(line);
    writer.write('\n');
  }
  writer.close();
}
