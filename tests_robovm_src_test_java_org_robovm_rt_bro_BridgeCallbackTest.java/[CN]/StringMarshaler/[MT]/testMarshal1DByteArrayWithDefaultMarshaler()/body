{
  assertEquals("a = foo, b = bar",marshal1DByteArrayWithDefaultMarshaler(new byte[]{'f','o','o',0},new byte[]{'b','a','r',0}).toStringAsciiZ());
  assertEquals("a = null, b = bar",marshal1DByteArrayWithDefaultMarshaler(null,new byte[]{'b','a','r',0}).toStringAsciiZ());
  assertEquals("a = foo, b = null",marshal1DByteArrayWithDefaultMarshaler(new byte[]{'f','o','o',0},null).toStringAsciiZ());
  assertNull(marshal1DByteArrayWithDefaultMarshaler(null,null));
}
