{
  Iterator it=b.getUnits().iterator();
  while (it.hasNext()) {
    Stmt s=(Stmt)it.next();
    if (!(s instanceof InvokeStmt))     continue;
    InvokeExpr invokeExpr=((InvokeStmt)s).getInvokeExpr();
    if (!(invokeExpr instanceof SpecialInvokeExpr))     continue;
    return (InvokeStmt)s;
  }
  return null;
}
