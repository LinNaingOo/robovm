{
  ParsePosition pos=new ParsePosition(2);
  MessageFormat mf=new MessageFormat("{0}; {0}; {0}");
  String parse="a; b; c";
  try {
    mf.parse(parse,null);
    fail("NullPointerException was not thrown.");
  }
 catch (  NullPointerException npe) {
  }
  try {
    mf.parse(null,pos);
  }
 catch (  NullPointerException npe) {
    fail("NullPointerException was thrown.");
  }
}
