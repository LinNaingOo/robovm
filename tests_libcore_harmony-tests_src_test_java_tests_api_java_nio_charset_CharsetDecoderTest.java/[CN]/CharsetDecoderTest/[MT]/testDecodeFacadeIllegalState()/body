{
  ByteBuffer in=getByteBuffer();
  decoder.decode(in);
  in.rewind();
  decoder.decode(in);
  in.rewind();
  decoder.reset();
  decoder.decode(getByteBuffer(),CharBuffer.allocate(30),true);
  decoder.decode(in);
  in.rewind();
  decoder.reset();
  decoder.decode(getByteBuffer(),CharBuffer.allocate(30),false);
  decoder.decode(in);
  in.rewind();
  decoder.reset();
  decoder.decode(getByteBuffer(),CharBuffer.allocate(30),true);
  decoder.flush(CharBuffer.allocate(10));
  decoder.decode(in);
  in.rewind();
}
