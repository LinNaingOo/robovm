{
  int r=0;
  Iterator<Local> ib=b.getLocals().iterator();
  for (  Local v : a.getLocals()) {
    Type ta=v.getType(), tb=ib.next().getType();
    if (soot.jimple.toolkits.typing.fast.TypeResolver.typesEqual(ta,tb))     continue;
 else     if (true && ((ta instanceof CharType && (tb instanceof ByteType || tb instanceof ShortType)) || (tb instanceof CharType && (ta instanceof ByteType || ta instanceof ShortType))))     continue;
 else     if (soot.jimple.toolkits.typing.fast.AugHierarchy.ancestor_(ta,tb)) {
      if (r == -1)       return 3;
 else       r=1;
    }
 else     if (soot.jimple.toolkits.typing.fast.AugHierarchy.ancestor_(tb,ta)) {
      if (r == 1)       return 3;
 else       r=-1;
    }
 else     return 3;
  }
  return r;
}
