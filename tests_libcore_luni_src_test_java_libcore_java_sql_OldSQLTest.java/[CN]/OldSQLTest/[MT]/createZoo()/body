{
  String[] queries={"create table zoo(id smallint,  name varchar(10), family varchar(10))","insert into zoo values (1, 'Kesha', 'parrot')","insert into zoo values (2, 'Yasha', 'sparrow')"};
  Statement st=null;
  try {
    st=conn.createStatement();
    for (int i=0; i < queries.length; i++) {
      st.execute(queries[i]);
    }
  }
  finally {
    try {
      if (st != null) {
        st.close();
      }
    }
 catch (    SQLException ee) {
    }
  }
}
